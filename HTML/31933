  <script>
   function tapar(enla) {
  obj = document.getElementById('fondoup');
  obj.style.zIndex = (obj.style.zIndex == '-1') ? '1000' : '-1';
  enla.innerHTML = (enla.innerHTML == 'MOSTRAR CONTENIDO +') ? 'OCULTAR CONTENIDO X' : 'MOSTRAR CONTENIDO +';
}
  </script>
