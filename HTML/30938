                <script type="text/javascript">
                 try{google &amp;&amp; google.maps}catch(err){document.write('&lt;script type="text/javascript" src="//maps.googleapis.com/maps/api/js?v=3.6&amp;sensor=false"&gt;&lt;\/script&gt;');}
