            <div class="item-page">
             <h2>
              Rreth Nesh
             </h2>
             <ul class="actions">
              <li class="print-icon">
               <a rel="nofollow" onclick="window.open(this.href,'win2','status=no,toolbar=no,scrollbars=yes,titlebar=no,menubar=no,resizable=yes,width=640,height=480,directories=no,location=no'); return false;" title="Print" href="/index.php/rrethnesh?tmpl=component&print=1&page=">
