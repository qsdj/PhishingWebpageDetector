   var ss = jQuery.noConflict();
ss(document).ready(function(){
ss("li.menu-item-has-children a").click(function(){
if(ss(this).hasClass("opened")) {  return true; } 
else {
  ss(this).parent("li.menu-item-has-children").find("ul.sub-menu").show();
  ss(this).addClass("opened");
  return false;
}
});
});
