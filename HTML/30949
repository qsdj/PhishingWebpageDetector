          jQuery.noConflict();
		jQuery(document).ready(function($)
			{
				var overlayH= $(this).find('span.overlay .overlay-title').height();
				//alert(overlayH);
				var imgH = '172';
				$(this).find('span.overlay').css('top',''+(imgH-overlayH)+'px');
				
				$('#ahgallery ul.hover_block0 li.ahitem').hover(function(){
					$(this).find('span.overlay').animate({top:'107px'},{queue:false,duration:300});
				}, function(){
					$(this).find('span.overlay').animate({top:''+(imgH-overlayH)+'px'},{queue:false,duration:300});
				});
		});
         </script>
