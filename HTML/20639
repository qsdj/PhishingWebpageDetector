  <script>
   function showcourse(cat)
{
	$.ajax
	({
		type: "POST",
		url: "get_college.php",
		data: 'cat='+cat,
		
		success: function(data)
		{
			$("#college_place_id").html(data);
		} 
	});
}
function showbatch()
{
var univ=$('#univ').val();
var college=$('#college').val();
var dataString = 'univ=' + univ + '&amp;college=' + college;
$.ajax({
type: "POST",
url: 'get_course.php',
data: dataString,
success:function(data) 
{	
$("#showbatch").html(data);}
});
}
  </script>
