	function shrinkText() {
		var somediv = document.getElementById("big_text");
		var someimg = document.getElementById("textarrow");
		
		someimg.setAttribute("onClick", "expandText()");
		somediv.style.height = "25px";
		someimg.src="/templates/bee-turbo-dark/images/down_arrow.png";
		
		return false;
	}
	
	function expandText() {
		
		var somediv = document.getElementById("big_text");
		var someimg = document.getElementById("textarrow");
		
		someimg.setAttribute("onClick", "shrinkText()");
		somediv.style.height = "280px";
		someimg.src="/templates/bee-turbo-dark/images/top_arrow.png";
		
		return false;
	}
