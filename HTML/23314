					onhovertouchunbindevents();
					menumousetouch.isopenspan = true;
					jQuery('#header').stop().animate({ marginLeft: "0px" }, 300);
					jQuery('#footer').stop().animate({ marginLeft: "0px" }, 300);
					jQuery('#footer_copyright').css({ padding: '20px 30px' });
					jQuery(".header-arrow").css({'display':'none'});
					jQuery('#content').css({'left':'210px'});
					jQuery('.page_description').css({'left':'210px'});
					jQuery('.cooming-soon-content').css({'left':'210px'});
					jQuery('#scrollbar2 .viewport').css({ left: "220px", 'width': (((jQuery(window).width()&lt;210)?210:jQuery(window).width())-210)+'px'});
					jQuery('.imgscontainer').stop().animate({ top: "0px", left: "220px", 'width': ((jQuery(window).width()&lt;220)?220:(jQuery(window).width()-200))+'px'});
				}
			}
		});
