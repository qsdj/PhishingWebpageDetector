function refresh_top(){

    $.get(
        rootUrl + 'wsdl/main.ajax.php',
      {
         action: 'getTopPopup'
      },  
      function(data){
//        if($('#notifications_counter').text() == data.Response.count) return false;
        $('#notifications_counter').text(data.Response.count);
        $('#notifications_popup .notification_row').remove();




        $(data.Response.results).insertAfter('#notifications_empty');
//        $('#notifications_popup').append();
        check_notification();
      }, 'json');
}


function mark_all_as_read(){
        if ($('#notifications_popup').is(':visible') &amp;&amp; $('#notifications_counter').text() != "0")
        $.get(
                rootUrl + 'wsdl/main.ajax.php',
                {
                    action: 'markAllRead'
                },
                function(data){
                    if($('#notifications_counter').text() == data.Response.count) return false;
                    $('#notifications_counter').text(data.Response.count);
                    $('#notifications_popup .notification_row').remove();
                    $(data.Response.results).insertAfter('#notifications_empty');
//        $('#notifications_popup').append();
                    check_notification();
//                    console.log('all read');
                }, 'json');
}


    function accept(type, cid){
//        console.log('accept click' + cid + " " + type);
        var linkgoto;
        if (type == 1) {
            linkgoto = $('.invite_' + cid + ':first').find('.chelink').attr('href');
        }

        $('.invite_' + cid).fadeOut('slow', function(){
            
                $.get(
                    rootUrl + 'wsdl/main.ajax.php',
                  {
                    cid: cid,
                    type: type,
                    action: 'acceptdecline'
                  },  
                  function(data){
//                        console.log('accept click resp' + cid + " " + type);
//                        console.log($('.invite_' + cid + ':first').find('.chelink'));

                        if (type == 1) {
                          window.location.href= linkgoto;
                        }
                          
                        $('.invite_' + cid).remove();

                        if ($('.invite_list li').length  == 0) {
                            $('.invite_list').parent().remove();
                        }
                        check_notification();
                        
//                        if ($('.notification_row').length  == 0) {
//                            $('#notifictions_count').hide();
//                        }
                  }, 'json'
                  


            );
            

        });        
    }
function add_message(text, loader, isred){
    clearTimeout(messager_timer);
        if (!loader) {
            $('#messagebar').removeClass('blue');           
        } else {
            $('#messagebar').addClass('blue');
        }

    $('#messagebar').removeClass('red');
    if (isred) $('#messagebar').addClass('red');
        
    if (!$('#messagebar').is(':visible')){
        $('#messagebar div').remove();
        $('#messagebar').append('&lt;div&gt;' + text + '&lt;/div&gt;');
        show_messager();   
        messager_timer = setTimeout('hide_messager()', 2000);
    } else {
        $('#messagebar').append('&lt;div&gt;' + text + '&lt;/div&gt;');
        $('#messagebar div:first').animate({
            'margin-top' : "-=30px"
        }, function(){
           $('#messagebar div:first').remove();
         messager_timer = setTimeout('hide_messager()', 2000);
        });
    }
    
}
    
    
    function refresh_points(){
        $.post(rootUrl + '/wsdl/main.ajax.php', {
                __ajaxEventUID: ajaxEventUID,
                action: 'getPoints'
            }, function(data){

                 if (data.Response.Exit == "1") {
                     return false;
                 }
                
                 if (data.Response.Medals) {
                    //console.log(data.Response.Medals);
                    for(var medal in data.Response.Medals) {
                        add_message(data.Response.Medals[medal]);
                        return false;                    
                        //console.log(data.Response.Medals[medal]);
                    }
                    
                 }                 
                 
                 if (data.Response.Points != "") {
                    if ($('input[name=bonuspoints]').val() != data.Response.Points) {
                        add_message('&lt;img src="/images/point_star.png" style="position: relative;top: 2px;left: 2px;"/&gt; +' + (data.Response.Points - parseInt($('input[name=bonuspoints]').val())) + ' ' + window.translations.POINTS_ADDED + ' &lt;img src="/images/point_star.png" style="position: relative;top: 2px;left: 2px;"/&gt;');
                        $('input[name=bonuspoints]').val(data.Response.Points);
                    }
                    loadticker(data.Response.Points);
                 }
                 

            }, 'json');
    }
    
    function check_sidebar(){
        if ($(window).width() &lt; 1100) return false;
        var right_height = $('#right').height() + 100;
        if ($('#liste_ape').is(':visible')){
            right_height += 140;
        }
        $('#container').css('min-height', right_height + 'px');
    }
    
