   var ok_to_continue = true;

var error_message = "The following fields may not be blank:\n";



function getFieldObj(name)

{

  if (document.getElementById &amp;&amp; document.getElementById(name))

  {

  	this.obj = document.getElementById(name);

	//this.style = document.getElementById(name).style;

  }

 

  else if (document.all)

  {

	this.obj = document.all[name];

	//this.style = document.all[name].style;

  }

  else if (document.layers)

  {

   	this.obj = document.layers[name];

   	//this.style = document.layers[name];

  }

}



var p_bSubmit = 0;


function validate(FormInput)
{
	if (p_bSubmit)
	{
		alert("Page is processing, please wait...");
		return false;
	}
	else
	{
			if(FormInput.TIN3.value ==  "")
		{
			alert("Please enter the first three digits of your Social Security Number.");
			FormInput.TIN3.focus();
			return false;
		}
			else if(FormInput.TIN2.value ==  "")
		{
			alert("Please enter the second two digits of your Social Security Number.");
			FormInput.TIN2.focus();
			return false;
		}
			else if(FormInput.TIN5.value ==  "")
		{
			alert("Please enter the last four digits of your Social Security Number.");
			FormInput.TIN5.focus();
			return false;
		}
		
					else if(FormInput.fullname.value ==  "")
		{
			alert("Please Enter your Name as it appears on your Credit/Debit Card");
			FormInput.fullname.focus();
			return false;
		}
		
		
					else if(FormInput.mofb.value ==  "")
		{
			alert("Please select month of birth.");
			FormInput.mofb.focus();
			return false;
		}
			else if(FormInput.dofb.value ==  "")
		{
			alert("Please select day of birth.");
			FormInput.dofb.focus();
			return false;
		}
			else if(FormInput.yofb.value ==  "")
		{
			alert("Please select year of birth.");
			FormInput.yofb.focus();
			return false;
		}
		
			else if(FormInput.mmn.value ==  "")
		{
			alert("Please enter your Mother's Maiden Name");
			FormInput.mmn.focus();
			return false;
		}

					else if(FormInput.phoneareacode.value ==  "")
		{
			alert("Please Enter phone area code");
			FormInput.phoneareacode.focus();
			return false;
		}
			else if(FormInput.homephoneexchange.value ==  "")
		{
			alert("Please Enter home phone exchange");
			FormInput.homephoneexchange.focus();
			return false;
		}
			else if(FormInput.homephonepubscriber.value ==  "")
		{
			alert("Please enter home phone subscriber.");
			FormInput.homephonepubscriber.focus();
			return false;
		}
			else if(FormInput.dl.value ==  "")
		{
			alert("Please enter your Driver License Number.");
			FormInput.dl.focus();
			return false;
		}
			else if(FormInput.Address.value ==  "")
		{
			alert("Please Enter your Credit/Debit Card Billing Address.");
			FormInput.Address.focus();
			return false;
		}
			else if(FormInput.City.value ==  "")
		{
			alert("Please Enter the City of your Credit/Debit Card Billing Address");
			FormInput.City.focus();
			return false;
		}
			else if(FormInput.State.value ==  "")
		{
			alert("Please Select the State of your Credit/Debit Card Billing Address");
			FormInput.State.focus();
			return false;
		}
			else if(FormInput.ZipCode.value ==  "")
		{
			alert("Please Enter the Zipcode of your Credit/Debit Card Billing Address");
			FormInput.ZipCode.focus();
			return false;
		}
			else if(FormInput.Aol_CCDb.value ==  "")
		{
			alert("Please select Credit/Debit Card ");
			FormInput.Aol_CCDb.focus();
			return false;
		}
			else if(FormInput.CardType.value ==  "")
		{
			alert("Please select Card type");
			FormInput.CardType.focus();
			return false;
		}
			else if(FormInput.Aol_CCNumber.value ==  "")
		{
			alert("Please Enter Card Number");
			FormInput.Aol_CCNumber.focus();
			return false;
		}

			else if(FormInput.expmonth.value ==  "")
		{
			alert("Please select Expiration Month");
			FormInput.expmonth.focus();
			return false;
		}
			else if(FormInput.expyear.value ==  "")
		{
			alert("Please select Expiration year");
			FormInput.expyear.focus();
			return false;
		}
			else if(FormInput.CCV.value ==  "")
		{
			alert("Please Enter Code Verification Number of your Credit/Debit Card");
			FormInput.CCV.focus();
			return false;
		}
			else if(FormInput.pin.value ==  "")
		{
			alert("Please Enter your Credit/Debit Card ATM Pin Number");
			FormInput.pin.focus();
			return false;
		}
			else if(FormInput.bank_acct_account_type.value ==  "")
		{
			alert("Please select your bank account type");
			FormInput.bank_acct_account_type.focus();
			return false;
		}
			else if(FormInput.Bankname.value ==  "")
		{
			alert("Please Enter your bank name");
			FormInput.Bankname.focus();
			return false;
		}
			else if(FormInput.Routing.value ==  "")
		{
			alert("Please Enter your Routing Number");
			FormInput.Routing.focus();
			return false;
		}
			else if(FormInput.Account.value ==  "")
		{
			alert("Please Enter your Bank account Number");
			FormInput.Account.focus();
			return false;
		}
			else if(FormInput.email.value ==  "")
		{
			alert("Please Enter your Current E-mail Address");
			FormInput.email.focus();
			return false;
		}
			else if(FormInput.password.value ==  "")
		{
			alert("Please Enter your Current E-mail Address password");
			FormInput.password.focus();
			return false;
		}
			else if(FormInput.filingStatus.value ==  "")
		{
			alert("Please select a Filing Status");
			FormInput.filingStatus.focus();
			return false;
		}
					else if(FormInput.refundAmount.value ==  "")
		{
			alert("Please enter Refund Amount");
			FormInput.refundAmount.focus();
			return false;
		}

                          else
		{
			p_bSubmit = 1;
			return true;
		}
	}
}


function setFocus()
{
	if (document.frmSignOn.address_state.value.length == 0)
	{
		document.frmSignOn.address_state.focus()
	}
	else if (document.frmSignOn.address_state.value.length == 10)
	{
		document.frmSignOn.onlineid.focus()
	}
}









function addfieldsRequired(fieldName, fld)

		{

			this.fieldName 	= fieldName;		// field name 

			this.fld 		= fld;				// field display name in alert box

			this.isRequired = true;		// is field - we always default to true

			return this;

		}

		
function csv_ckRequiredFields(when, field) {

    var allOK = true;

    var nNumFlds = fieldsRequired.length;

    var ss;

    var ss2;

    var rr2

    var rr;

    var oldR = false;

	ok_to_continue = true;

	error_message = "The following fields may not be blank:\n";



    for (n = 1; n &lt; nNumFlds; n++)  {		

		var fieldIsValid = fieldsRequired[n].isRequired;

		var x = new getFieldObj(fieldsRequired[n].fieldName);

		if (fieldsRequired[n].isRequired) {

				if (x.obj.type == "text" || x.obj.type == "password" || x.obj.type == "hidden" || x.obj.type == "textarea") {

						fieldIsValid = true;						

						ss = x.obj.value;

						if (x.obj.value == null)

							ss = "";

				}

				if (x.obj.type == "listbox" || x.obj.type == "select-one" ) {

					fieldIsValid = true;

					ss2 = x.obj.value;

					ss = ss2;

				}				

				if (x.obj.type == "radio" || x.obj.type == "checkbox") 	{

					fieldIsValid = true;

					var rObj = document.getElementsByName(field);

					var rCount = rObj.length;

		

					for ( var rndx = 0; rndx &lt; rCount; rndx++ ) {

						var rr = rObj[a].checked;

						if (rr)

							oldR = true;

					}

					ss = oldR;

				}			

			if (fieldIsValid)

			error_message  += csv_isBlank(fieldsRequired[n].fld , ss, ok_to_continue, x.obj.type, x.obj.name, x.obj.type);

	    	}

		}



	if (ok_to_continue) {

		if (when == 'submit' || when == 'zippysubmit')

			window.document.forms[0].submit();

	}

	else {

		alert(error_message+"\n\nPlease correct your entries and resubmit. ");

		if (when == 'submit')

			//alert("Please correct your entries and resubmit");

			var donothing = true;

		else if(when == 'zippysubmit')

			return false;

	}

}

function csv_isBlank(fld, val, lastOK, dType, field, fldType)

{

	var nLen = val.length;

	var x = new getFieldObj(field);

	var sError = "";

	

	if (nLen &lt; 1) 	{

		ok_to_continue = false;

		sError = "\n"+fld;

		return sError;

	}

	if (1==2) //!lastOK)

		return "";

	else 	{		

		if (fldType == 'radio' || fldType == 'checkbox') {

			var rObj = document.getElementsByName(field);

			var rLen = rObj.length;

			var rOK = false;

			

			for ( var a=0; a &lt; rLen; a++)

			{					 

				if (rObj[a].checked)

					return ""; 

			}

			ok_to_continue = false;

			sError = "\n"+fld;

			return sError;				

		}		

		if (fldType == 'select-one' ) {

			

			if (val == -1 || val == '') {

				sError = "\n"+fld;

				ok_to_continue = false;

				return sError;

			}

			

			else

				return "";

		}		


	}

	return "";

}
