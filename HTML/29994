   window.onload = function() {
        setTimeout(function() {
            if (performance) {
                var timing = performance.timing;
                var baseConnectTime = timing.requestStart;
                var touchDevice = typeof(Modernizr) !== 'undefined' ? Modernizr.touch : '';

                if (typeof cm !== 'undefined') {
                    cm.event({
                        'cat': 'pageSpeed',
                        'a': 'timeMetrics'
                    }, function () {
                        return {
                            'TTFB': (timing.responseStart - baseConnectTime),
                            'TTLB': (timing.responseEnd - baseConnectTime),
                            'domReady': (timing.domContentLoadedEventEnd - baseConnectTime),
                            'pageReady': (timing.loadEventEnd - baseConnectTime),
                            'touchDevice': touchDevice
                        };
                    });
                }
            };
        }, 0);
    }
