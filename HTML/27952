   jQuery(document).ready(function($) {
jQuery('#nav ul li').has('ul').addClass("hasul");
jQuery('.hasul &gt; a').append('&lt;i class="icon-angle-down"&gt;&lt;/i&gt;');
jQuery('#nav ul li ul li').has('ul').addClass("hasul2");
jQuery('.hasul2 &gt; a &gt; i').removeClass('icon-angle-down');
jQuery(".hasul2 &gt; a &gt; i").addClass("icon-angle-right");
jQuery("#nav ul li").hoverIntent(
function(){
jQuery('&gt; ul' , this).slideDown(200);
},
function(){
jQuery('&gt; ul' , this).slideUp(200);
}
)
$(function() {
var pull        = $('#pull');
menu        = $('#nav &gt; div &gt; ul');
menuHeight  = menu.height();
$(pull).on('click', function(e) {
e.preventDefault();
menu.slideToggle();
});
$(window).resize(function(){
var w = $(window).width();
if(w &gt; 320 &amp;&amp; menu.is(':hidden')) {
menu.removeAttr('style');
}
});
});
$( ".accordion" ).accordion();
$( ".tabs-wrapper" ).tabs();
/* toglle */
$( ".block" ).each(function(){
$(".trigger" , this).click(function(e){
e.preventDefault();
if ($(this).hasClass('hide_toglle')) {
$(this).removeClass('hide_toglle');
$(this).next('.toggle_container').first().slideUp('fast');
}else{
$(this).addClass('hide_toglle');
$(this).next('.toggle_container').first().slideDown('fast');
}
});
});
$('.comment-body').each(function() {
var commentbodyheight = ($(this).height()/2)-19;
$('.avatar', this).css({"top":commentbodyheight});
$(this).css({"height":$(this).height()});
});
});
function slideSwitch() {
var $active = jQuery('#slideshow IMG.active');
if ( $active.length == 0 ) $active = jQuery('#slideshow IMG:last');
// use this to pull the images in the order they appear in the markup
var $next =  $active.next().length ? $active.next()
: jQuery('#slideshow IMG:first');
// uncomment the 3 lines below to pull the images in random order
// var $sibs  = $active.siblings();
// var rndNum = Math.floor(Math.random() * $sibs.length );
// var $next  = $( $sibs[ rndNum ] );
$active.addClass('last-active');
$next.css({opacity: 0.0})
.addClass('active')
.animate({opacity: 1.0}, 1000, function() {
$active.removeClass('active last-active');
});
}
jQuery(function() {
setInterval( "slideSwitch()", 5000 );
});
// Play Sound
jQuery(document).ready(function(){
jQuery(".jp-play").click(function() {
jQuery.cookie("matrixsound", "yes", { path: '/' });
});
jQuery(".jp-pause").click(function() {
jQuery.cookie("matrixsound", "no", { path: '/' });
});
var noPlayer = false;
if( jQuery.jPlayer.platform.mobile || jQuery.jPlayer.platform.iphone || jQuery.jPlayer.platform.ipod || jQuery.jPlayer.platform.android) {
noPlayer = true ;
}
if( !noPlayer ){
jQuery("#jquery_jplayer_home").jPlayer({
ready: function () {
jQuery(this).jPlayer("setMedia", {
mp3: "http://donttouchmyhair.com.br/wp-content/uploads/2013/09/Rihanna-Bitch-Better-Have-My-Money-Explicit.mp3",
m4a: "",
oga: "http://donttouchmyhair.com.br/wp-content/uploads/2013/09/Rihanna-Bitch-Better-Have-My-Money-Explicit.mp3"
});
if( jQuery.cookie("matrixsound") != 'no'){
jQuery('.jp-play').click();
}
},
cssSelectorAncestor: "#jp_container_home",
supplied: "m4a, oga, mp3",
volume: 1,
loop : "1"
});
}
});
