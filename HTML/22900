  <script type="text/javascript">
   function activeSuburb(idChk,idDivContent)
		{
			var chkReg = document.getElementById(idChk);
			if(chkReg.checked)
			{document.getElementById(idDivContent).style.display="inline";}
			else
			{document.getElementById(idDivContent).style.display="none";}
		}
		
		function validateForm()
		{
			var err = "";
			err += isRequired(document.getElementById("title_id").value,"Title");
			err += isText(document.getElementById("first_name").value,"First Name");
			err += isText(document.getElementById("last_name").value,"Last Name");
			
			err += isEmail(document.getElementById("email").value,"Email");
			
			err += isRequired(document.getElementById("password").value,"Password");
			
			if(document.getElementById("password").value != document.getElementById("confirm_password").value)
			{err += "Password and Confirm Password should be equal\n";}
			
			err += isRequired(document.getElementById("address").value,"Address");
			
			if(document.getElementById("suburb_state_postcode_id").value=="0" &amp;&amp; document.getElementById("country_id").value==6 ||
			document.getElementById("suburb_state_postcode_id").value=="" &amp;&amp; document.getElementById("country_id").value==6)
			{
				err += "Please select a location from the list of suburb, state, and potal code\n";
			}

			var conditionsVar = document.getElementById("client_terms_and_conditions");
			if(!conditionsVar.checked)
			{err += "You must agree to the terms and conditions\n";}
			
			if(err=="")
			return true;
			else
			{
				alert("Please check the following errors.\n" + err);
				return false;
			}
		}
		
		function calculate_age()
		{
			var day = document.getElementById("date_of_birth_day").options[document.getElementById("date_of_birth_day").selectedIndex].value;
			var month = document.getElementById("date_of_birth_month").options[document.getElementById("date_of_birth_month").selectedIndex].value;
			var year = document.getElementById("date_of_birth_year").options[document.getElementById("date_of_birth_year").selectedIndex].value;
			
			var age = 0;
			if(day!=0 &amp;&amp; month!=0 &amp;&amp; year!=0)
			{
				age = return_age(day,month,year);
				
				if(age &gt;= 18)
				{document.getElementById("i_am_adult").disabled = false;}
				else
				{
				document.getElementById("i_am_adult").checked = false;
				document.getElementById("i_am_adult").disabled = true;}
			}
			return true;
		}
		
		function return_age(day_birth,month_birth,year_birth)
		{
			date_now = new Date();
			now_year = date_now.getYear();
			now_month = date_now.getMonth();
			now_day = date_now.getDate();
			age = (now_year + 1900) - year_birth;
			if ( now_month &lt; (month_birth - 1))
			{
			  age--;
			}
			if (((month_birth - 1) == now_month) &amp;&amp; (now_day &lt; day_birth))
			{ 
			  age--;
			}
			if (age &gt; 1900)
			{
			age -= 1900;
			}
		  return age;
		}
  </script>
