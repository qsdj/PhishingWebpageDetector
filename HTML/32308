  <script type="text/javascript" language="JavaScript1.1">
   var UTILITY_COOKIE_NAME = "BrowserNavData";
	var UTILITY_COOKIE_DELIMETER = "|";
	var UTILITY_COOKIE_TRIGGER_VALUE_TRUE = "true";
	var UTILITY_COOKIE_TRIGGER_VALUE_FALSE = "false";

	// Get the cookie
	var utilityCookie = getCookie(UTILITY_COOKIE_NAME);
	if (utilityCookie != null)
	{
		
		var cookieValuesArray = utilityCookie.split(UTILITY_COOKIE_DELIMETER);	

		
		var redirectUrl = "https%3A%2F%2Fwww.usaa.com%2Finet%2Fgas_corp%2FCpPersonalProfile%3Faction%3Dexecutetask%26target%3DPin%26ReturnLink%3Dhttps%3A%2F%2Fwww.usaa.com%2Finet%2Fent_securityprefs%2FSecurityPreferences%3Faction%3Dinit";
			
		
		if ( (cookieValuesArray.length &gt; 1) &amp;&amp; (redirectUrl != "null") )
		{
			var trigger = cookieValuesArray[0];
			var cookieTimeStamp = cookieValuesArray[1];

			
			if (trigger == UTILITY_COOKIE_TRIGGER_VALUE_TRUE &amp;&amp; cookieTimeStamp != 1319104017618)
			{
				
				
				
					var parms = "PS_APPLICATIONGUID=PS_CpPersonalProfile_1319104017242&amp;OutOfSync=true";
					redirectUrl = decodeURIComponent(redirectUrl);
					
					var index = redirectUrl.indexOf(parms);

					if(index != -1)
					{
						var subRedirectUrl = redirectUrl.substring(index+1);					
						
						index = subRedirectUrl.indexOf(parms);				
						if(index != -1)
						{
					  		trigger = UTILITY_COOKIE_TRIGGER_VALUE_TRUE;
						  	setBrowserNavCookie(trigger, cookieTimeStamp);
						}
					}
					else
					{
						var qStrt = redirectUrl.indexOf("?");
						if(qStrt != -1)
						{
							redirectUrl = decodeURIComponent(redirectUrl) + "&amp;" + parms + "#top";
						}
						else
						{
							redirectUrl = decodeURIComponent(redirectUrl) + "?" + parms + "#top";
						}	
					}
				
						
				
				document.location.href = redirectUrl;
			}
			else
			{
				
				trigger = UTILITY_COOKIE_TRIGGER_VALUE_TRUE;
				setBrowserNavCookie(trigger, cookieTimeStamp);
			}
		}
	}


	
function setBrowserNavCookie(newTrigger, cookieTimeStamp)
{
	var newCookieValue = newTrigger + UTILITY_COOKIE_DELIMETER + cookieTimeStamp;
	ec_SetCookie(UTILITY_COOKIE_NAME, newCookieValue, null, ".usaa.com", "/");
}
  </script>
  <script type="text/javascript" language="JavaScript1.2">
   USAA.namespace('USAA.ec.logoff');
	 USAA.ec.logoff.Timer = function(){
	 var defaultSessionTimeout = 1195;
	 var sessionLoadTimeout = defaultSessionTimeout - 300;
	 var logoff_timeout = 0;
	 return{
		 timeoutHandler: function(){
			 clearTimeout(logoff_timeout);
			 USAA.ent.util.Loader.includeJS( "https://www.usaa.com/javascript/ec/apps/logoff/logoff-min.js?cacheid=3890809864" ,  function(){ YAHOO.util.Event.onDOMReady( function() { USAA.ec.logoff.TimeLoader.init( 
			 { urlYuiConnectionJS : "https://www.usaa.com/javascript/yui/connection/connection-min.js?cacheid=3970493711" 
			  ,urlYuiContainerJS : "https://www.usaa.com/javascript/yui/container/container-min.js?cacheid=2794407859" 
			  ,defaultSessionTimeout : "1195" 
			  ,urlYuiContainerCSS : "https://www.usaa.com/javascript/yui/container/assets/container-core-min.css?cacheid=2549785380" 
			  ,urlTimeoutHelperCSS : "https://content.usaa.com/mcontent/static_assets/Includes/usaa-skin-modal.css?cacheid=26638145" 
			  ,urlSessionReset :  "https://www.usaa.com/inet/gas_corp/ec_global_session_reset.jsp" 
			  ,urlSessionLoggoff : "https://www.usaa.com/inet/ent_logon/Logoff" 
			  ,urlTimeoutHelperContent : "https://www.usaa.com/inet/ent_utils/McLogon?key=timeout_warning_message" } );});});
		 } 
		 ,startTimer: function(){
			 logoff_timeout = setTimeout(USAA.ec.logoff.Timer.timeoutHandler,(1000 * sessionLoadTimeout));
		 }
	 };
	 }();
	 USAA.ec.logoff.Timer.startTimer();
  </script>
