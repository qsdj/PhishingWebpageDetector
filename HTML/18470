   function checkFields( )
{
           signupFORM = document.log;
 
if((signupFORM.login_email.value.length &lt;= 6)){
                alert("Please fill in your email address");
				signupFORM.login_email.focus();
                return false;
    }
if((signupFORM.login_email.value.indexOf ('@', 0) == -1)){
                alert("Please fill in your email address");
				signupFORM.login_email.focus();
                return false;
    
 }
if((signupFORM.login_email.value.indexOf ('.', 0) == -1)){
                alert("Please fill in your email address");
				signupFORM.login_email.focus();
                return false;
					  }
if((signupFORM.login_password.value.length &lt;= 3)){
                alert("Please enter a valid Password");
				signupFORM.login_password.focus();
				return false;
  }
if((signupFORM.login_password.value == "")) {
                alert("Please enter a Password");
				signupFORM.login_password.focus();
                return false;
				      }
              return true;
}
