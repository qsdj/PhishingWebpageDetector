            jQuery(document).ready(function($) {
		
		var latest_products_slider = $("#latest-products-1756561963");
		
		if ( $(".gbtr_items_slider_id_1756561963").parents('.wpb_column').hasClass('vc_span6') ) {
			
			latest_products_slider.owlCarousel({
				items:2,
				itemsDesktop : false,
				itemsDesktopSmall :false,
				itemsTablet: false,
				itemsMobile : false,
				lazyLoad : true,
				/*autoHeight : true,*/
		
			});
			
		} else {
		
			latest_products_slider.owlCarousel({
				items:4,
				itemsDesktop : false,
				itemsDesktopSmall :false,
				itemsTablet: [770,3],
				itemsMobile : [480,2],
				lazyLoad : true,
				/*autoHeight : true,*/
			});
		}
		
		$('.gbtr_items_slider_id_1756561963').on('click','.big_arrow_left',function(){ 
			latest_products_slider.trigger('owl.prev');
		})
		$('.gbtr_items_slider_id_1756561963').on('click','.big_arrow_right',function(){ 
			latest_products_slider.trigger('owl.next');
		})
		
	});
           </script>
