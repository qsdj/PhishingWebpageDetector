     jQuery(function($){
				$(".search_span").on("click", function(){
					var str = $("#s").val().trim(); 
					if ( str != "")
						$("#searchform2").submit();
				});

				var w = 0;

				$(".page-numbers li").each(function(index){
					w = w + Number($(".page-numbers li").eq(index).width()) + 10;					
				});				
				
				$(".page-numbers").css("width", w + 'px');
			});
    </script>
