    <script type="text/javascript">
     var an = 0;
		var animelogo = "";		
		var settimeee = setInterval(function(){
		animelogo = setInterval(function(){
				$('#logo &gt; img').css('z-index','10');
				$('#logo &gt; img:eq('+an+')').css('z-index','20');
				$('#logo &gt; img:eq('+an+')').fadeIn('slow');
				if(an &lt; 7){
					an++
				}else{
					an = 0;
					clearInterval(animelogo);
				}
			},50);
		},3000);
    </script>
