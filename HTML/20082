	});
	
	
$(document).ready(function(){
	$("#scroll1").click(function(e){
		  $(".form_apply").show();
		$('html, body').animate({
		scrollTop: $("#form-career-id-new").offset().top
		  }, 2000);
		return false;
	});	
		
	$("#scroll2").click(function(e){
 
	  $(".form_apply").show();
	$('html, body').animate({
	scrollTop: $("#form-career-id-new").offset().top
      }, 2000);
	return false;
	});
	$("#scroll3").click(function(e){
	  $(".form_apply").show();
	$('html, body').animate({
	scrollTop: $("#form-career-id-new").offset().top
      }, 2000);
	return false;
	});
