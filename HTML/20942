        <script>
         &lt;!--//--&gt;&lt;![CDATA[// &gt;&lt;!--

jQuery(document).ready(function(){
var currentMousePos = { x: -1, y: -1 };
    jQuery(this).mousemove(function(event) {
        currentMousePos.x = event.pageX;
        currentMousePos.y = event.pageY;
    });
function handlerIn(){
jQuery(this).after('&lt;iframe&gt;');
jQuery(this).parent().find('iframe').addClass('webpage-preview').attr('src',jQuery(this).attr("href"));
if (currentMousePos.x &gt;700) {
        jQuery('.webpage-preview').css('margin-left','-500px');
    }else{
 jQuery('.webpage-preview').css('margin-left','0');
}
}
function handlerOut(){
jQuery(this).parent().find('iframe').remove();
}
jQuery('.view-instock-featured-brands .views-field-field-image a').each(function(){
jQuery(this).hover(handlerIn,handlerOut)
});
});

//--&gt;&lt;!]]&gt;
        </script>
