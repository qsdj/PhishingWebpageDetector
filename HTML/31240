  <script type="text/JavaScript">
   /* Block_wsx 5: JS to change the search module. */
function validate_search(search_form) {
   var strTemp = document.search_form.s.value;
   strTemp = trimAll(strTemp);
   if(strTemp == 'Search')
	{
	   alert ("Please enter a valid search keyword.");
	   return false;
	}
	else
	{
   if(strTemp.length &gt; 0){
   /* Check for valid keyword */
   var objRegExp= /^[a-z][-a-z0-9_ ]{0,49}$/i;
   if(objRegExp.test(strTemp)==false)
   {
    alert("Keyword can contain alphabets, digits, hyphen, underscore and space.");
    return false;
   }
   /* End of Check */
   }
   else
   {
   alert("Please enter a valid search keyword.");
   document.search_form.s.focus();
   return false;
   }
	}
}
function trimAll( strValue ) {
 var objRegExp = /^(\s*)$/;

    //check for all spaces
    if(objRegExp.test(strValue)) {
       strValue = strValue.replace(objRegExp, '');
       if( strValue.length == 0)
          return strValue;
    }

   
   objRegExp = /^(\s*)([\W\w]*)(\b\s*$)/;
   if(objRegExp.test(strValue)) {
       //remove leading and trailing whitespace characters
       strValue = strValue.replace(objRegExp, '$2');
    }
  return strValue;
}
       /* WSX Block 5 end */
