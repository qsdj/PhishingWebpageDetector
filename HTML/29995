   $(function(){
           $("a[rel^='prettyPhoto']").prettyPhoto({autoplay: true, overlay_gallery: false, theme:'light_square', show_title: false, social_tools: false}); 
           $('#goTop').click(function(){
                $('body,html').animate({scrollTop: 0}, 500); 
                return false;
           }); 
           $("#goTop").hover(function(){
                $(this).stop(true, false).animate({ bottom: "0px" },'fast');
            }, function() {
                $(this).stop(true, false).animate({ bottom: "-8px" },'fast');
            });
            if( $('.item-page &gt; h2').length &gt; 0 ){
                $('#pageTitle .text').text( $('.item-page &gt; h2').text() );
                $('#pageTitle').show();
                $('.item-page &gt; h2').remove();
            }
        });
  </script>
