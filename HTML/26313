   &lt;!--
// This function gives the user a second chance to submit the form --
function askUserAgain(f) 
{
  var msg = "\nClick OK if you really want to decline. " +
               "If you decline, the data on the form will be cleared.\n\n" +
               "Click Cancel if you want to work on the form again or submit the registration.\n\n\n" ;
  var declineOk = confirm(msg);
  if ( declineOk == true )
  {
		document.location.href='../index.htm';
    return true;
  }
  else
  {
    return false;
  }
}
//--&gt;
  </script>
