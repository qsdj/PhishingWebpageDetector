                 <script>
                  var bwg_trans_in_progress_110 = false;
      var bwg_transition_duration_110 = 750;
      var bwg_playInterval_110;
      /* Stop autoplay.*/
      window.clearInterval(bwg_playInterval_110);
      /* Set watermark container size.*/
      function bwg_change_watermark_container_110() {
        jQuery(".bwg_slider_110").children().each(function() {
          if (jQuery(this).css("zIndex") == 2) {
            var bwg_current_image_span = jQuery(this).find("img");
            if (!bwg_current_image_span.length) {
              bwg_current_image_span = jQuery(this).find("iframe");
            }
            var width = bwg_current_image_span.width();
            var height = bwg_current_image_span.height();
            jQuery(".bwg_slideshow_watermark_spun_110").width(width);
            jQuery(".bwg_slideshow_watermark_spun_110").height(height);
            jQuery(".bwg_slideshow_title_spun_110").width(width);
            jQuery(".bwg_slideshow_title_spun_110").height(height);
            jQuery(".bwg_slideshow_description_spun_110").width(width);
            jQuery(".bwg_slideshow_description_spun_110").height(height);
            jQuery(".bwg_slideshow_watermark_110").css({display: ''});
            if (jQuery.trim(jQuery(".bwg_slideshow_title_text_110").text())) {
              jQuery(".bwg_slideshow_title_text_110").css({display: ''});
            }
            if (jQuery.trim(jQuery(".bwg_slideshow_description_text_110").text())) {
              jQuery(".bwg_slideshow_description_text_110").css({display: ''});
            }
          }
        });
      }
      var bwg_current_key_110 = '0';
      var bwg_current_filmstrip_pos_110 = 0;
      /* Set filmstrip initial position.*/
      function bwg_set_filmstrip_pos_110(filmStripWidth) {
        var selectedImagePos = -bwg_current_filmstrip_pos_110 - (jQuery(".bwg_slideshow_filmstrip_thumbnail_110").width() + 2) / 2;
        var imagesContainerLeft = Math.min(0, Math.max(filmStripWidth - jQuery(".bwg_slideshow_filmstrip_thumbnails_110").width(), selectedImagePos + filmStripWidth / 2));
        jQuery(".bwg_slideshow_filmstrip_thumbnails_110").animate({
            left: imagesContainerLeft
          }, {
            duration: 500,
            complete: function () { bwg_filmstrip_arrows_110(); }
          });
      }
      function bwg_move_filmstrip_110() {
        var image_left = jQuery(".bwg_slideshow_thumb_active_110").position().left;
        var image_right = jQuery(".bwg_slideshow_thumb_active_110").position().left + jQuery(".bwg_slideshow_thumb_active_110").outerWidth(true);
        var bwg_filmstrip_width = jQuery(".bwg_slideshow_filmstrip_110").outerWidth(true);
        var bwg_filmstrip_thumbnails_width = jQuery(".bwg_slideshow_filmstrip_thumbnails_110").outerWidth(true);
        var long_filmstrip_cont_left = jQuery(".bwg_slideshow_filmstrip_thumbnails_110").position().left;
        var long_filmstrip_cont_right = Math.abs(jQuery(".bwg_slideshow_filmstrip_thumbnails_110").position().left) + bwg_filmstrip_width;
        if (bwg_filmstrip_width &gt; bwg_filmstrip_thumbnails_width) {
          return;
        }
        if (image_left &lt; Math.abs(long_filmstrip_cont_left)) {
          jQuery(".bwg_slideshow_filmstrip_thumbnails_110").animate({
            left: -image_left
          }, {
            duration: 500,
            complete: function () { bwg_filmstrip_arrows_110(); }
          });
        }
        else if (image_right &gt; long_filmstrip_cont_right) {
          jQuery(".bwg_slideshow_filmstrip_thumbnails_110").animate({
            left: -(image_right - bwg_filmstrip_width)
          }, {
            duration: 500,
            complete: function () { bwg_filmstrip_arrows_110(); }
          });
        }
      }
      function bwg_move_dots_110() {
        var image_left = jQuery(".bwg_slideshow_dots_active_110").position().left;
        var image_right = jQuery(".bwg_slideshow_dots_active_110").position().left + jQuery(".bwg_slideshow_dots_active_110").outerWidth(true);
        var bwg_dots_width = jQuery(".bwg_slideshow_dots_container_110").outerWidth(true);
        var bwg_dots_thumbnails_width = jQuery(".bwg_slideshow_dots_thumbnails_110").outerWidth(true);
        var long_filmstrip_cont_left = jQuery(".bwg_slideshow_dots_thumbnails_110").position().left;
        var long_filmstrip_cont_right = Math.abs(jQuery(".bwg_slideshow_dots_thumbnails_110").position().left) + bwg_dots_width;
        if (bwg_dots_width &gt; bwg_dots_thumbnails_width) {
          return;
        }
        if (image_left &lt; Math.abs(long_filmstrip_cont_left)) {
          jQuery(".bwg_slideshow_dots_thumbnails_110").animate({
            left: -image_left
          }, {
            duration: 500,
            complete: function () {  }
          });
        }
        else if (image_right &gt; long_filmstrip_cont_right) {
          jQuery(".bwg_slideshow_dots_thumbnails_110").animate({
            left: -(image_right - bwg_dots_width)
          }, {
            duration: 500,
            complete: function () {  }
          });
        }
      }
      /* Show/hide filmstrip arrows.*/
      function bwg_filmstrip_arrows_110() {
        if (jQuery(".bwg_slideshow_filmstrip_thumbnails_110").width() &lt; jQuery(".bwg_slideshow_filmstrip_110").width()) {
          jQuery(".bwg_slideshow_filmstrip_left_110").hide();
          jQuery(".bwg_slideshow_filmstrip_right_110").hide();
        }
        else {
          jQuery(".bwg_slideshow_filmstrip_left_110").show();
          jQuery(".bwg_slideshow_filmstrip_right_110").show();
        }
      }
      function bwg_testBrowser_cssTransitions_110() {
        return bwg_testDom_110('Transition');
      }
      function bwg_testBrowser_cssTransforms3d_110() {
        return bwg_testDom_110('Perspective');
      }
      function bwg_testDom_110(prop) {
        /* Browser vendor CSS prefixes.*/
        var browserVendors = ['', '-webkit-', '-moz-', '-ms-', '-o-', '-khtml-'];
        /* Browser vendor DOM prefixes.*/
        var domPrefixes = ['', 'Webkit', 'Moz', 'ms', 'O', 'Khtml'];
        var i = domPrefixes.length;
        while (i--) {
          if (typeof document.body.style[domPrefixes[i] + prop] !== 'undefined') {
            return true;
          }
        }
        return false;
      }
      function bwg_cube_110(tz, ntx, nty, nrx, nry, wrx, wry, current_image_class, next_image_class, direction) {
        /* If browser does not support 3d transforms/CSS transitions.*/
        if (!bwg_testBrowser_cssTransitions_110()) {
          return bwg_fallback_110(current_image_class, next_image_class, direction);
        }
        if (!bwg_testBrowser_cssTransforms3d_110()) {
          return bwg_fallback3d_110(current_image_class, next_image_class, direction);
        }
        bwg_trans_in_progress_110 = true;
        /* Set active thumbnail.*/
        jQuery(".bwg_slideshow_filmstrip_thumbnail_110").removeClass("bwg_slideshow_thumb_active_110").addClass("bwg_slideshow_thumb_deactive_110");
        jQuery("#bwg_filmstrip_thumbnail_" + bwg_current_key_110 + "_110").removeClass("bwg_slideshow_thumb_deactive_110").addClass("bwg_slideshow_thumb_active_110");
        jQuery(".bwg_slideshow_dots_110").removeClass("bwg_slideshow_dots_active_110").addClass("bwg_slideshow_dots_deactive_110");
        jQuery("#bwg_dots_" + bwg_current_key_110 + "_110").removeClass("bwg_slideshow_dots_deactive_110").addClass("bwg_slideshow_dots_active_110");
        jQuery(".bwg_slide_bg_110").css('perspective', 1000);
        jQuery(current_image_class).css({
          transform : 'translateZ(' + tz + 'px)',
          backfaceVisibility : 'hidden'
        });
        jQuery(next_image_class).css({
          opacity : 1,
          filter: 'Alpha(opacity=100)',
          backfaceVisibility : 'hidden',
          transform : 'translateY(' + nty + 'px) translateX(' + ntx + 'px) rotateY('+ nry +'deg) rotateX('+ nrx +'deg)'
        });
        jQuery(".bwg_slider_110").css({
          transform: 'translateZ(-' + tz + 'px)',
          transformStyle: 'preserve-3d'
        });
        /* Execution steps.*/
        setTimeout(function () {
          jQuery(".bwg_slider_110").css({
            transition: 'all ' + bwg_transition_duration_110 + 'ms ease-in-out',
            transform: 'translateZ(-' + tz + 'px) rotateX('+ wrx +'deg) rotateY('+ wry +'deg)'
          });
        }, 20);
        /* After transition.*/
        jQuery(".bwg_slider_110").one('webkitTransitionEnd transitionend otransitionend oTransitionEnd mstransitionend', jQuery.proxy(bwg_after_trans));
        function bwg_after_trans() {
          /*if (bwg_from_focus_110) {
            bwg_from_focus_110 = false;
            return;
          }*/
          jQuery(current_image_class).removeAttr('style');
          jQuery(next_image_class).removeAttr('style');
          jQuery(".bwg_slider_110").removeAttr('style');
          jQuery(current_image_class).css({'opacity' : 0, filter: 'Alpha(opacity=0)', 'z-index': 1});
          jQuery(next_image_class).css({'opacity' : 1, filter: 'Alpha(opacity=100)', 'z-index' : 2});
          bwg_change_watermark_container_110();
          bwg_trans_in_progress_110 = false;
          if (typeof event_stack_110 !== 'undefined' &amp;&amp; event_stack_110.length &gt; 0) {
            key = event_stack_110[0].split("-");
            event_stack_110.shift();
            bwg_change_image_110(key[0], key[1], data_110, true);
          }
        }
      }
      function bwg_cubeH_110(current_image_class, next_image_class, direction) {
        /* Set to half of image width.*/
        var dimension = jQuery(current_image_class).width() / 2;
        if (direction == 'right') {
          bwg_cube_110(dimension, dimension, 0, 0, 90, 0, -90, current_image_class, next_image_class, direction);
        }
        else if (direction == 'left') {
          bwg_cube_110(dimension, -dimension, 0, 0, -90, 0, 90, current_image_class, next_image_class, direction);
        }
      }
      function bwg_cubeV_110(current_image_class, next_image_class, direction) {
        /* Set to half of image height.*/
        var dimension = jQuery(current_image_class).height() / 2;
        /* If next slide.*/
        if (direction == 'right') {
          bwg_cube_110(dimension, 0, -dimension, 90, 0, -90, 0, current_image_class, next_image_class, direction);
        }
        else if (direction == 'left') {
          bwg_cube_110(dimension, 0, dimension, -90, 0, 90, 0, current_image_class, next_image_class, direction);
        }
      }
      /* For browsers that does not support transitions.*/
      function bwg_fallback_110(current_image_class, next_image_class, direction) {
        bwg_fade_110(current_image_class, next_image_class, direction);
      }
      /* For browsers that support transitions, but not 3d transforms (only used if primary transition makes use of 3d-transforms).*/
      function bwg_fallback3d_110(current_image_class, next_image_class, direction) {
        bwg_sliceV_110(current_image_class, next_image_class, direction);
      }
      function bwg_none_110(current_image_class, next_image_class, direction) {
        jQuery(current_image_class).css({'opacity' : 0, 'z-index': 1});
        jQuery(next_image_class).css({'opacity' : 1, 'z-index' : 2});
        bwg_change_watermark_container_110();
        /* Set active thumbnail.*/
        jQuery(".bwg_slideshow_filmstrip_thumbnail_110").removeClass("bwg_slideshow_thumb_active_110").addClass("bwg_slideshow_thumb_deactive_110");
        jQuery("#bwg_filmstrip_thumbnail_" + bwg_current_key_110 + "_110").removeClass("bwg_slideshow_thumb_deactive_110").addClass("bwg_slideshow_thumb_active_110");
        jQuery(".bwg_slideshow_dots_110").removeClass("bwg_slideshow_dots_active_110").addClass("bwg_slideshow_dots_deactive_110");
        jQuery("#bwg_dots_" + bwg_current_key_110 + "_110").removeClass("bwg_slideshow_dots_deactive_110").addClass("bwg_slideshow_dots_active_110");
      }
      function bwg_fade_110(current_image_class, next_image_class, direction) {
        /* Set active thumbnail.*/
        jQuery(".bwg_slideshow_filmstrip_thumbnail_110").removeClass("bwg_slideshow_thumb_active_110").addClass("bwg_slideshow_thumb_deactive_110");
        jQuery("#bwg_filmstrip_thumbnail_" + bwg_current_key_110 + "_110").removeClass("bwg_slideshow_thumb_deactive_110").addClass("bwg_slideshow_thumb_active_110");
        jQuery(".bwg_slideshow_dots_110").removeClass("bwg_slideshow_dots_active_110").addClass("bwg_slideshow_dots_deactive_110");
        jQuery("#bwg_dots_" + bwg_current_key_110 + "_110").removeClass("bwg_slideshow_dots_deactive_110").addClass("bwg_slideshow_dots_active_110");
        if (bwg_testBrowser_cssTransitions_110()) {
          jQuery(next_image_class).css('transition', 'opacity ' + bwg_transition_duration_110 + 'ms linear');
          jQuery(current_image_class).css({'opacity' : 0, 'z-index': 1});
          jQuery(next_image_class).css({'opacity' : 1, 'z-index' : 2});
          bwg_change_watermark_container_110();
        }
        else {
          jQuery(current_image_class).animate({'opacity' : 0, 'z-index' : 1}, bwg_transition_duration_110);
          jQuery(next_image_class).animate({
              'opacity' : 1,
              'z-index': 2
            }, {
              duration: bwg_transition_duration_110,
              complete: function () { bwg_change_watermark_container_110(); }
            });
          /* For IE.*/
          jQuery(current_image_class).fadeTo(bwg_transition_duration_110, 0);
          jQuery(next_image_class).fadeTo(bwg_transition_duration_110, 1);
        }
      }
      function bwg_grid_110(cols, rows, ro, tx, ty, sc, op, current_image_class, next_image_class, direction) {
        /* If browser does not support CSS transitions.*/
        if (!bwg_testBrowser_cssTransitions_110()) {
          return bwg_fallback_110(current_image_class, next_image_class, direction);
        }
        bwg_trans_in_progress_110 = true;
        /* Set active thumbnail.*/
        jQuery(".bwg_slideshow_filmstrip_thumbnail_110").removeClass("bwg_slideshow_thumb_active_110").addClass("bwg_slideshow_thumb_deactive_110");
        jQuery("#bwg_filmstrip_thumbnail_" + bwg_current_key_110 + "_110").removeClass("bwg_slideshow_thumb_deactive_110").addClass("bwg_slideshow_thumb_active_110");
        jQuery(".bwg_slideshow_dots_110").removeClass("bwg_slideshow_dots_active_110").addClass("bwg_slideshow_dots_deactive_110");
        jQuery("#bwg_dots_" + bwg_current_key_110 + "_110").removeClass("bwg_slideshow_dots_deactive_110").addClass("bwg_slideshow_dots_active_110");
        /* The time (in ms) added to/subtracted from the delay total for each new gridlet.*/
        var count = (bwg_transition_duration_110) / (cols + rows);
        /* Gridlet creator (divisions of the image grid, positioned with background-images to replicate the look of an entire slide image when assembled)*/
        function bwg_gridlet(width, height, top, img_top, left, img_left, src, imgWidth, imgHeight, c, r) {
          var delay = (c + r) * count;
          /* Return a gridlet elem with styles for specific transition.*/
          return jQuery('&lt;div class="bwg_gridlet_110" /&gt;').css({
            width : width,
            height : height,
            top : top,
            left : left,
            backgroundImage : 'url("' + src + '")',
            backgroundColor: jQuery(".bwg_slideshow_image_wrap_110").css("background-color"),
            /*backgroundColor: rgba(0, 0, 0, 0),*/
            backgroundRepeat: 'no-repeat',
            backgroundPosition : img_left + 'px ' + img_top + 'px',
            backgroundSize : imgWidth + 'px ' + imgHeight + 'px',
            transition : 'all ' + bwg_transition_duration_110 + 'ms ease-in-out ' + delay + 'ms',
            transform : 'none'
          });
        }
        /* Get the current slide's image.*/
        var cur_img = jQuery(current_image_class).find('img');
        /* Create a grid to hold the gridlets.*/
        var grid = jQuery('&lt;div /&gt;').addClass('bwg_grid_110');
        /* Prepend the grid to the next slide (i.e. so it's above the slide image).*/
        jQuery(current_image_class).prepend(grid);
        /* vars to calculate positioning/size of gridlets*/
        var cont = jQuery(".bwg_slide_bg_110");
        var imgWidth = cur_img.width();
        var imgHeight = cur_img.height();
        var contWidth = cont.width(),
            contHeight = cont.height(),
            imgSrc = cur_img.attr('src'),/*.replace('/thumb', ''),*/
            colWidth = Math.floor(contWidth / cols),
            rowHeight = Math.floor(contHeight / rows),
            colRemainder = contWidth - (cols * colWidth),
            colAdd = Math.ceil(colRemainder / cols),
            rowRemainder = contHeight - (rows * rowHeight),
            rowAdd = Math.ceil(rowRemainder / rows),
            leftDist = 0,
            img_leftDist = (jQuery(".bwg_slide_bg_110").width() - cur_img.width()) / 2;
        /* tx/ty args can be passed as 'auto'/'min-auto' (meaning use slide width/height or negative slide width/height).*/
        tx = tx === 'auto' ? contWidth : tx;
        tx = tx === 'min-auto' ? - contWidth : tx;
        ty = ty === 'auto' ? contHeight : ty;
        ty = ty === 'min-auto' ? - contHeight : ty;
        /* Loop through cols*/
        for (var i = 0; i &lt; cols; i++) {
          var topDist = 0,
              img_topDst = (jQuery(".bwg_slide_bg_110").height() - cur_img.height()) / 2,
              newColWidth = colWidth;
          /* If imgWidth (px) does not divide cleanly into the specified number of cols, adjust individual col widths to create correct total.*/
          if (colRemainder &gt; 0) {
            var add = colRemainder &gt;= colAdd ? colAdd : colRemainder;
            newColWidth += add;
            colRemainder -= add;
          }
          /* Nested loop to create row gridlets for each col.*/
          for (var j = 0; j &lt; rows; j++)  {
            var newRowHeight = rowHeight,
                newRowRemainder = rowRemainder;
            /* If contHeight (px) does not divide cleanly into the specified number of rows, adjust individual row heights to create correct total.*/
            if (newRowRemainder &gt; 0) {
              add = newRowRemainder &gt;= rowAdd ? rowAdd : rowRemainder;
              newRowHeight += add;
              newRowRemainder -= add;
            }
            /* Create &amp; append gridlet to grid.*/
            grid.append(bwg_gridlet(newColWidth, newRowHeight, topDist, img_topDst, leftDist, img_leftDist, imgSrc, imgWidth, imgHeight, i, j));
            topDist += newRowHeight;
            img_topDst -= newRowHeight;
          }
          img_leftDist -= newColWidth;
          leftDist += newColWidth;
        }
        /* Set event listener on last gridlet to finish transitioning.*/
        var last_gridlet = grid.children().last();
        /* Show grid &amp; hide the image it replaces.*/
        grid.show();
        cur_img.css('opacity', 0);
        /* Add identifying classes to corner gridlets (useful if applying border radius).*/
        grid.children().first().addClass('rs-top-left');
        grid.children().last().addClass('rs-bottom-right');
        grid.children().eq(rows - 1).addClass('rs-bottom-left');
        grid.children().eq(- rows).addClass('rs-top-right');
        /* Execution steps.*/
        setTimeout(function () {
          grid.children().css({
            opacity: op,
            transform: 'rotate('+ ro +'deg) translateX('+ tx +'px) translateY('+ ty +'px) scale('+ sc +')'
          });
        }, 1);
        jQuery(next_image_class).css('opacity', 1);
        /* After transition.*/
        jQuery(last_gridlet).one('webkitTransitionEnd transitionend otransitionend oTransitionEnd mstransitionend', jQuery.proxy(bwg_after_trans));
        function bwg_after_trans() {
          /*if (bwg_from_focus_110) {
            bwg_from_focus_110 = false;
            return;
          }*/
          jQuery(current_image_class).css({'opacity' : 0, 'z-index': 1});
          jQuery(next_image_class).css({'opacity' : 1, 'z-index' : 2});
          cur_img.css('opacity', 1);
          bwg_change_watermark_container_110();
          grid.remove();
          bwg_trans_in_progress_110 = false;
          if (typeof event_stack_110 !== 'undefined' &amp;&amp; event_stack_110.length &gt; 0) {
            key = event_stack_110[0].split("-");
            event_stack_110.shift();
            bwg_change_image_110(key[0], key[1], data_110, true);
          }
        }
      }
      function bwg_sliceH_110(current_image_class, next_image_class, direction) {
        if (direction == 'right') {
          var translateX = 'min-auto';
        }
        else if (direction == 'left') {
          var translateX = 'auto';
        }
        bwg_grid_110(1, 8, 0, translateX, 0, 1, 0, current_image_class, next_image_class, direction);
      }
      function bwg_sliceV_110(current_image_class, next_image_class, direction) {
        if (direction == 'right') {
          var translateY = 'min-auto';
        }
        else if (direction == 'left') {
          var translateY = 'auto';
        }
        bwg_grid_110(10, 1, 0, 0, translateY, 1, 0, current_image_class, next_image_class, direction);
      }
      function bwg_slideV_110(current_image_class, next_image_class, direction) {
        if (direction == 'right') {
          var translateY = 'auto';
        }
        else if (direction == 'left') {
          var translateY = 'min-auto';
        }
        bwg_grid_110(1, 1, 0, 0, translateY, 1, 1, current_image_class, next_image_class, direction);
      }
      function bwg_slideH_110(current_image_class, next_image_class, direction) {
        if (direction == 'right') {
          var translateX = 'min-auto';
        }
        else if (direction == 'left') {
          var translateX = 'auto';
        }
        bwg_grid_110(1, 1, 0, translateX, 0, 1, 1, current_image_class, next_image_class, direction);
      }
      function bwg_scaleOut_110(current_image_class, next_image_class, direction) {
        bwg_grid_110(1, 1, 0, 0, 0, 1.5, 0, current_image_class, next_image_class, direction);
      }
      function bwg_scaleIn_110(current_image_class, next_image_class, direction) {
        bwg_grid_110(1, 1, 0, 0, 0, 0.5, 0, current_image_class, next_image_class, direction);
      }
      function bwg_blockScale_110(current_image_class, next_image_class, direction) {
        bwg_grid_110(8, 6, 0, 0, 0, .6, 0, current_image_class, next_image_class, direction);
      }
      function bwg_kaleidoscope_110(current_image_class, next_image_class, direction) {
        bwg_grid_110(10, 8, 0, 0, 0, 1, 0, current_image_class, next_image_class, direction);
      }
      function bwg_fan_110(current_image_class, next_image_class, direction) {
        if (direction == 'right') {
          var rotate = 45;
          var translateX = 100;
        }
        else if (direction == 'left') {
          var rotate = -45;
          var translateX = -100;
        }
        bwg_grid_110(1, 10, rotate, translateX, 0, 1, 0, current_image_class, next_image_class, direction);
      }
      function bwg_blindV_110(current_image_class, next_image_class, direction) {
        bwg_grid_110(1, 8, 0, 0, 0, .7, 0, current_image_class, next_image_class);
      }
      function bwg_blindH_110(current_image_class, next_image_class, direction) {
        bwg_grid_110(10, 1, 0, 0, 0, .7, 0, current_image_class, next_image_class);
      }
      function bwg_random_110(current_image_class, next_image_class, direction) {
        var anims = ['sliceH', 'sliceV', 'slideH', 'slideV', 'scaleOut', 'scaleIn', 'blockScale', 'kaleidoscope', 'fan', 'blindH', 'blindV'];
        /* Pick a random transition from the anims array.*/
        this["bwg_" + anims[Math.floor(Math.random() * anims.length)] + "_110"](current_image_class, next_image_class, direction);
      }
      function iterator_110() {
        var iterator = 1;
        if (0) {
          iterator = Math.floor((data_110.length - 1) * Math.random() + 1);
        }
        return iterator;
      }
      function bwg_change_image_110(current_key, key, data_110, from_effect) {
        /* Pause videos.*/
        jQuery("#bwg_slideshow_image_container_110").find("iframe").each(function () {
          jQuery(this)[0].contentWindow.postMessage('{"event":"command","func":"pauseVideo","args":""}', '*');
          jQuery(this)[0].contentWindow.postMessage('{ "method": "pause" }', "*");
        });
        if (data_110[key]) {
          if (jQuery('.bwg_ctrl_btn_110').hasClass('fa-pause')) {
            play_110();
          }
          if (!from_effect) {
            /* Change image key.*/
            jQuery("#bwg_current_image_key_110").val(key);
            if (current_key == '-1') { /* Filmstrip.*/
              current_key = jQuery(".bwg_slideshow_thumb_active_110").children("img").attr("image_key");
            }
            else if (current_key == '-2') { /* Dots.*/
              current_key = jQuery(".bwg_slideshow_dots_active_110").attr("image_key");
            }
          }
          if (bwg_trans_in_progress_110) {
            event_stack_110.push(current_key + '-' + key);
            return;
          }
          var direction = 'right';
          if (bwg_current_key_110 &gt; key) {
            var direction = 'left';
          }
          else if (bwg_current_key_110 == key) {
            return;
          }
          jQuery(".bwg_slideshow_watermark_110").css({display: 'none'});
          jQuery(".bwg_slideshow_title_text_110").css({display: 'none'});
          jQuery(".bwg_slideshow_description_text_110").css({display: 'none'});
          /* Set active thumbnail position.*/
          bwg_current_filmstrip_pos_110 = key * (jQuery(".bwg_slideshow_filmstrip_thumbnail_110").width() + 2 + 2 * 1);
          bwg_current_key_110 = key;
          /* Change image id, title, description.*/
          jQuery("#bwg_slideshow_image_110").attr('image_id', data_110[key]["id"]);
          jQuery(".bwg_slideshow_title_text_110").html(jQuery('&lt;div /&gt;').html(data_110[key]["alt"]).text());
          jQuery(".bwg_slideshow_description_text_110").html(jQuery('&lt;div /&gt;').html(data_110[key]["description"]).text());
          var current_image_class = "#image_id_110_" + data_110[current_key]["id"];
          var next_image_class = "#image_id_110_" + data_110[key]["id"];
          bwg_sliceH_110(current_image_class, next_image_class, direction);
                      bwg_move_filmstrip_110();
                      if (data_110[key]["is_video"]) {
            jQuery("#bwg_slideshow_play_pause_110").css({display: 'none'});
          }
          else {
            jQuery("#bwg_slideshow_play_pause_110").css({display: ''});            
          }
        }
      }
      function bwg_popup_resize_110() {
        var parent_width = jQuery(".bwg_slideshow_image_wrap_110").parent().width();
        if (parent_width &gt;= 1000) {
          jQuery(".bwg_slideshow_image_wrap_110").css({width: 1000});
          jQuery(".bwg_slideshow_image_wrap_110").css({height: 550});
          jQuery(".bwg_slideshow_image_container_110").css({width: 1000});
          jQuery(".bwg_slideshow_image_container_110").css({height: (500)});
          jQuery(".bwg_slideshow_image_110").css({
            cssText: "max-width: 1000px !important; max-height: 500px !important;"
          });
          jQuery(".bwg_slideshow_video_110").css({
            cssText: "width: 1000px !important; height: 500px !important;"
          });
          /* Set watermark container size.*/
          bwg_change_watermark_container_110();
          jQuery(".bwg_slideshow_filmstrip_container_110").css({width: 1000});
          jQuery(".bwg_slideshow_filmstrip_110").css({width: 960});
          jQuery(".bwg_slideshow_dots_container_110").css({width: 1000});
          jQuery("#bwg_slideshow_play_pause-ico_110").css({fontSize: (60)});
          jQuery(".bwg_slideshow_watermark_image_110").css({maxWidth: 90, maxHeight: 90});
          jQuery(".bwg_slideshow_watermark_text_110, .bwg_slideshow_watermark_text_110:hover").css({fontSize: (20)});
          jQuery(".bwg_slideshow_title_text_110").css({fontSize: (32)});
          jQuery(".bwg_slideshow_description_text_110").css({fontSize: (28)});
        }
        else {
          jQuery(".bwg_slideshow_image_wrap_110").css({width: (parent_width)});
          jQuery(".bwg_slideshow_image_wrap_110").css({height: ((parent_width) * 0.55)});
          jQuery(".bwg_slideshow_image_container_110").css({width: (parent_width - 0)});
          jQuery(".bwg_slideshow_image_container_110").css({height: ((parent_width) * 0.55 - 50)});
          jQuery(".bwg_slideshow_image_110").css({
            cssText: "max-width: " + (parent_width - 0) + "px !important; max-height: " + (parent_width * (0.55) - 50 - 1) + "px !important;"
          });
          jQuery(".bwg_slideshow_video_110").css({
            cssText: "width: " + (parent_width - 0) + "px !important; height: " + (parent_width * (0.55) - 50 - 1) + "px !important;"
          });
          /* Set watermark container size.*/
          bwg_change_watermark_container_110();
                    jQuery(".bwg_slideshow_filmstrip_container_110").css({width: (parent_width)});
          jQuery(".bwg_slideshow_filmstrip_110").css({width: (parent_width - 40)});
                    jQuery(".bwg_slideshow_dots_container_110").css({width: (parent_width)});
          jQuery("#bwg_slideshow_play_pause-ico_110").css({fontSize: ((parent_width) * 0.06)});
          jQuery(".bwg_slideshow_watermark_image_110").css({maxWidth: ((parent_width) * 0.09), maxHeight: ((parent_width) * 0.09)});
          jQuery(".bwg_slideshow_watermark_text_110, .bwg_slideshow_watermark_text_110:hover").css({fontSize: ((parent_width) * 0.02)});
          jQuery(".bwg_slideshow_title_text_110").css({fontSize: ((parent_width) * 0.032)});
          jQuery(".bwg_slideshow_description_text_110").css({fontSize: ((parent_width) * 0.028)});
        }
      }
      jQuery(window).resize(function() {
        bwg_popup_resize_110();
      });
      jQuery(window).load(function () {
      	        if (typeof jQuery().swiperight !== 'undefined' &amp;&amp; jQuery.isFunction(jQuery().swiperight)) {
          jQuery('#bwg_container1_110').swiperight(function () {
            bwg_change_image_110(parseInt(jQuery('#bwg_current_image_key_110').val()), (parseInt(jQuery('#bwg_current_image_key_110').val()) - iterator_110()) &gt;= 0 ? (parseInt(jQuery('#bwg_current_image_key_110').val()) - iterator_110()) % data_110.length : data_110.length - 1, data_110);
            return false;
          });
        }
        if (typeof jQuery().swipeleft !== 'undefined' &amp;&amp; jQuery.isFunction(jQuery().swipeleft)) {
          jQuery('#bwg_container1_110').swipeleft(function () {
            bwg_change_image_110(parseInt(jQuery('#bwg_current_image_key_110').val()), (parseInt(jQuery('#bwg_current_image_key_110').val()) + iterator_110()) % data_110.length, data_110);
            return false;
          });
        }

        var isMobile = (/android|webos|iphone|ipad|ipod|blackberry|iemobile|opera mini/i.test(navigator.userAgent.toLowerCase()));
        var bwg_click = isMobile ? 'touchend' : 'click';
        bwg_popup_resize_110();
        jQuery("#bwg_container1_110").css({visibility: 'visible'});
        jQuery(".bwg_slideshow_watermark_110").css({display: 'none'});
        jQuery(".bwg_slideshow_title_text_110").css({display: 'none'});
        jQuery(".bwg_slideshow_description_text_110").css({display: 'none'});
        setTimeout(function () {
          bwg_change_watermark_container_110();
        }, 500);
        /* Set image container height.*/
                  jQuery(".bwg_slideshow_image_container_110").height(jQuery(".bwg_slideshow_image_wrap_110").height() - 50);
                  var mousewheelevt = (/Firefox/i.test(navigator.userAgent)) ? "DOMMouseScroll" : "mousewheel"; /* FF doesn't recognize mousewheel as of FF3.x */
        jQuery('.bwg_slideshow_filmstrip_110').bind(mousewheelevt, function(e) {
          var evt = window.event || e; /* Equalize event object.*/
          evt = evt.originalEvent ? evt.originalEvent : evt; /* Convert to originalEvent if possible.*/
          var delta = evt.detail ? evt.detail*(-40) : evt.wheelDelta; /* Check for detail first, because it is used by Opera and FF.*/
          if (delta &gt; 0) {
            /* Scroll up.*/
            jQuery(".bwg_slideshow_filmstrip_left_110").trigger("click");
          }
          else {
            /* Scroll down.*/
            jQuery(".bwg_slideshow_filmstrip_right_110").trigger("click");
          }
          return false;
        });
        jQuery(".bwg_slideshow_filmstrip_right_110").on(bwg_click, function () {
          jQuery( ".bwg_slideshow_filmstrip_thumbnails_110" ).stop(true, false);
          if (jQuery(".bwg_slideshow_filmstrip_thumbnails_110").position().left &gt;= -(jQuery(".bwg_slideshow_filmstrip_thumbnails_110").width() - jQuery(".bwg_slideshow_filmstrip_110").width())) {
            jQuery(".bwg_slideshow_filmstrip_left_110").css({opacity: 1, filter: "Alpha(opacity=100)"});
            if (jQuery(".bwg_slideshow_filmstrip_thumbnails_110").position().left &lt; -(jQuery(".bwg_slideshow_filmstrip_thumbnails_110").width() - jQuery(".bwg_slideshow_filmstrip_110").width() - 102)) {
              jQuery(".bwg_slideshow_filmstrip_thumbnails_110").animate({left: -(jQuery(".bwg_slideshow_filmstrip_thumbnails_110").width() - jQuery(".bwg_slideshow_filmstrip_110").width())}, 500, 'linear');
            }
            else {
              jQuery(".bwg_slideshow_filmstrip_thumbnails_110").animate({left: (jQuery(".bwg_slideshow_filmstrip_thumbnails_110").position().left - 102)}, 500, 'linear');
            }
          }
          /* Disable right arrow.*/
          window.setTimeout(function(){
            if (jQuery(".bwg_slideshow_filmstrip_thumbnails_110").position().left == -(jQuery(".bwg_slideshow_filmstrip_thumbnails_110").width() - jQuery(".bwg_slideshow_filmstrip_110").width())) {
              jQuery(".bwg_slideshow_filmstrip_right_110").css({opacity: 0.3, filter: "Alpha(opacity=30)"});
            }
          }, 500);
        });
        jQuery(".bwg_slideshow_filmstrip_left_110").on(bwg_click, function () {
          jQuery( ".bwg_slideshow_filmstrip_thumbnails_110" ).stop(true, false);
          if (jQuery(".bwg_slideshow_filmstrip_thumbnails_110").position().left &lt; 0) {
            jQuery(".bwg_slideshow_filmstrip_right_110").css({opacity: 1, filter: "Alpha(opacity=100)"});
            if (jQuery(".bwg_slideshow_filmstrip_thumbnails_110").position().left &gt; - 102) {
              jQuery(".bwg_slideshow_filmstrip_thumbnails_110").animate({left: 0}, 500, 'linear');
            }
            else {
              jQuery(".bwg_slideshow_filmstrip_thumbnails_110").animate({left: (jQuery(".bwg_slideshow_filmstrip_thumbnails_110").position().left + 102)}, 500, 'linear');
            }
          }
          /* Disable left arrow.*/
          window.setTimeout(function(){
            if (jQuery(".bwg_slideshow_filmstrip_thumbnails_110").position().left == 0) {
              jQuery(".bwg_slideshow_filmstrip_left_110").css({opacity: 0.3, filter: "Alpha(opacity=30)"});
            }
          }, 500);
        });
        /* Set filmstrip initial position.*/
        bwg_set_filmstrip_pos_110(jQuery(".bwg_slideshow_filmstrip_110").width());
        /* Play/pause.*/
        jQuery("#bwg_slideshow_play_pause_110").on(bwg_click, function () {
          if (jQuery(".bwg_ctrl_btn_110").hasClass("fa-play")) {
            play_110();
            jQuery(".bwg_slideshow_play_pause_110").attr("title", "Pause");
            jQuery(".bwg_slideshow_play_pause_110").attr("class", "bwg_ctrl_btn_110 bwg_slideshow_play_pause_110 fa fa-pause");
            if (1) {
              document.getElementById("bwg_audio_110").play();
            }
          }
          else {
            /* Pause.*/
            window.clearInterval(bwg_playInterval_110);
            jQuery(".bwg_slideshow_play_pause_110").attr("title", "Play");
            jQuery(".bwg_slideshow_play_pause_110").attr("class", "bwg_ctrl_btn_110 bwg_slideshow_play_pause_110 fa fa-play");
            if (1) {
              document.getElementById("bwg_audio_110").pause();
            }
          }
        });
        if (1) {
          play_110();
          jQuery(".bwg_slideshow_play_pause_110").attr("title", "Pause");
          jQuery(".bwg_slideshow_play_pause_110").attr("class", "bwg_ctrl_btn_110 bwg_slideshow_play_pause_110 fa fa-pause");
          if (1) {
            document.getElementById("bwg_audio_110").play();
          }
        }
      });
      function play_110() {
        window.clearInterval(bwg_playInterval_110);
        /* Play.*/
        bwg_playInterval_110 = setInterval(function () {
          var iterator = 1;
          if (0) {
            iterator = Math.floor((data_110.length - 1) * Math.random() + 1);
          }
          bwg_change_image_110(parseInt(jQuery('#bwg_current_image_key_110').val()), (parseInt(jQuery('#bwg_current_image_key_110').val()) + iterator) % data_110.length, data_110)
        }, '3000');
      }
      jQuery(window).focus(function() {
        /* event_stack_110 = [];*/
        if (!jQuery(".bwg_ctrl_btn_110").hasClass("fa-play")) {
          play_110();
        }
        var i_110 = 0;
        jQuery(".bwg_slider_110").children("span").each(function () {
          if (jQuery(this).css('opacity') == 1) {
            jQuery("#bwg_current_image_key_110").val(i_110);
          }
          i_110++;
        });
      });
      jQuery(window).blur(function() {
        event_stack_110 = [];
        window.clearInterval(bwg_playInterval_110);
      });
                 </script>
