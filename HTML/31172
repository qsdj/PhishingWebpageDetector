   <script type="text/javascript">
    $('.bt_log').click(function(){
		if($('[name=usuario]').val()==''){
			alert('Você deve fornecer um usuário!');
			return false;
		}else if($('[name=senha]').val()==''){
			alert('Você deve fornecer uma senha!');
			return false;
		}else{
			$('.formlogin').submit();
		}
	})
	
	$('.formlogin').submit(function(){
		if($('[name=usuario]').val()==''){
			alert('Você deve fornecer um usuário!');
			return false;
		}else if($('[name=senha]').val()==''){
			alert('Você deve fornecer uma senha!');
			return false;
		}
	})
   </script>
