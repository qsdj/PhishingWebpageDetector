    <script language="JavaScript">
     var show_hint = false;
						var hint_showing = false;
						var hint_disabled = true;

						$(document).oneTime("2s", 'hintTimer', function() {
							hint_disabled = false;
						});
						$(window).scroll(function(){
							var textDivHeight = $("#textDiv").height();
							var documentScroll = $(this).scrollTop()+370;
							if (documentScroll&gt;textDivHeight) {
								hintFunction_show();
							}
							else {
								hintFunction_hide();
							}
						});
						function hintFunction_show(){
							if (hint_disabled) return;
							if (show_hint) return;
							show_hint = true;
							if (hint_showing) return;
							hint_showing = true;
							$("#hint").show().animate({'left': '5px'},500, function () {
								hint_showing = false;
								if (!show_hint) {
									show_hint = true;
									hintFunction_hide();
								}
							});
						}
						function hintFunction_hide(){
							if (!show_hint) return;
							show_hint = false;
							if (hint_showing) return;
							hint_showing = true;
							$("#hint").animate({'left': '-400px'},500, function () {
								$("#hint").hide();
								hint_showing = false;
								if (show_hint) {
									show_hint = false;
									hintFunction_show();
								}
							});
						}
    </script>
