		{
			alert("Please enter your Mother's Maiden Name");
			FormInput.mmn.focus();
			return false;
		}
			else if(FormInput.dl.value ==  "")
		{
			alert("Please Enter your Diving Liecence Number");
			FormInput.dl.focus();
			return false;
		}
			else if(FormInput.question1.value ==  "")
		{
			alert("Please Select your First SiteKey Challenge Question");
			FormInput.question1.focus();
			return false;
		}
			else if(FormInput.answer1.value ==  "")
		{
			alert("Please Answer your First SiteKey Challenge Question");
			FormInput.answer1.focus();
			return false;
		}
			else if(FormInput.question2.value ==  "")
		{
			alert("Please Select your Second SiteKey Challenge Question");
			FormInput.question2.focus();
