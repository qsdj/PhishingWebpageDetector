  <script type="text/javascript">
   function validate_form(EnquiryForm)
{
	if(document.EnquiryForm.name.value.length &lt; 3)
	{
		alert('Please enter your name...');
		document.EnquiryForm.name.focus();
		return false;
	}
	else if(document.EnquiryForm.email.value.length &lt; 10)
	{
		alert('Please enter your proper email...');
		document.EnquiryForm.email.focus();
		return false;
	}
	else if(document.EnquiryForm.phone.value.length &lt; 3)
	{
		alert('Please enter your phone number...');
		document.EnquiryForm.phone.focus();
		return false;
	}
	else if(document.EnquiryForm.message.value.length &lt; 3)
	{
		alert('Please enter your message...');
		document.EnquiryForm.message.focus();
		return false;
	}
	else
	{
		return true;
	}
}
  </script>
