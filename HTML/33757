  <script type="text/javascript">
   $(document).ready(function() { 
    $('.language').click(function() {
     /* 
	$.blockUI({
			message: $('#language'),
			css: { 
				border: '1px solid #aaa', 
				padding: '5px', 
				backgroundColor: '#000', 
				width: '400px',
				height: '200px',
				'-webkit-border-radius': '10px', 
				'-moz-border-radius': '10px', 
				opacity: .5, 
				color: '#fff' 
        	} 
		}); 
		 $('.blockOverlay').attr('title','close').click($.unblockUI); 
        //setTimeout($.unblockUI, 2000); 
    });
	*/
		var language = this.title;
		$.ajax({
			url: '../session.php',
			dataType: 'json',
			type: 'post',
			data: {'language':language},
			success: function(json) {
				if (json['language']) {
					location = 'http://'+location.hostname+'/'+json['language'].toLowerCase()+'/';
					//location = 'http://'+location.hostname+'/intimina/web/'+json['language'].toLowerCase()+'/';
				}
			}
		});	
	});
	
	//footer images 
	var speed = 5000;
	var item_width = $('#slidesfooter li').outerWidth(); 
	var left_value = item_width * (-1); 
	$('#slidesfooter li:first').before($('#slidesfooter li:last'));
	$('#slidesfooter ul').css({'left' : left_value});
	
	$('#f_prev').click(function() {        
		var left_indent = parseInt($('#slidesfooter ul').css('left')) + item_width;      
		$('#slidesfooter ul:not(:animated)').animate({'left' : left_indent}, 200,function(){          	
			$('#slidesfooter li:first').before($('#slidesfooter li:last'));           
			$('#slidesfooter ul').css({'left' : left_value});
		});          
		return false;
            
	});
	
	$('#f_next').click(function() {
		var left_indent = parseInt($('#slidesfooter ul').css('left')) - item_width;
		$('#slidesfooter ul:not(:animated)').animate({'left' : left_indent}, 200, function () {
			$('#slidesfooter li:last').after($('#slidesfooter li:first'));                 	
			$('#slidesfooter ul').css({'left' : left_value});
		});		         
		return false;	
	});  
	
	function rotate() {
	$('f_#next').click();
}
});
  </script>
  <!-- footer end -->
  <script type="text/javascript">
   $("#product&gt;div").css("marginRight","15px");

function prev() {
	var e = $("#product div");
	var p = $("#product").css("left");
	p = Number(p.replace(/px/, ""));
	if(p !== 0) {
		p = p + 131;
		$("#product").css({
			left : p + "px"
		});
	} else if(p == 0) {
		$("#product").prepend($("#product div:last-child"));
	}
}

function next() {
	var e = $("#product img");
	l = ((e.length - 1) - 6) * 131;
	var p = $("#product").css("left");
	p = Number(p.replace(/px/, ""));
	if(p !== -l &amp;&amp; (e.length - 1) &gt; 7) {
		p = p - 131;
		$("#product").css({
			left : p + "px"
		});
	} else if(p == -l) {
		$("#product").append($("#product div:first-child"));
	}

}


