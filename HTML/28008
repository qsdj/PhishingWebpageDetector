      <script type="text/javascript">
       jQuery('document').ready(function(){
        jQuery('#past-content').html('');
        jQuery('#upcoming-content').html('');
        jQuery('#active-content').html('');
        var activeDealsSize = parseInt("0");
        var upcomingDealsSize = parseInt("0");
        var pastDealsSize = parseInt("0");
        if(activeDealsSize &gt; 0){
            if((jQuery(window).width() &lt; 768)){
                jQuery('.active').addClass('section-title padding-right section-title-strip');
                openActive();
            }else{
                openActive();
            }
        }else if(upcomingDealsSize &gt; 0){
            if((jQuery(window).width() &gt; 768)){
                openUpcoming();
            }else{
                console.log(activeDealsSize);
                if(activeDealsSize == 0){
                    jQuery('.tabs').hide();
                }
                jQuery('#upcoming-content').hide();
                jQuery('#upcoming').hide();
            }
        }else if((pastDealsSize &gt; 0)){
            if((jQuery(window).width() &gt; 768)){
                openPast();
            }else{
                if(activeDealsSize == 0){
                    jQuery('.tabs').hide();
                }
                jQuery('#past-content').hide();
                jQuery('#past').hide();
                jQuery('#active').addClass('section-title padding-right section-title-strip');
            }
        }else{
            jQuery('.tabs').hide();
        }

    });
    function openActive(){
        active();
        var base_url = 'http://www.goalsquad.com/';
        getActiveSlider(base_url);
    }
    function openUpcoming(){
        upcoming();
        var base_url = 'http://www.goalsquad.com/';
        getUpcomingSlider(base_url);
    }
    function openPast(){
        past();
        var base_url = 'http://www.goalsquad.com/';
        getPastSlider(base_url);
    }
      </script>
