function show_prevaddress()
{

	var obj = 'prev_addr_section';
	if(csv_getField('currentaddress_less_year'))
	 {
		if(csv_getField('currentaddress_less_year').checked)
		{
			show(obj);
		}
		else
		{
			hide(obj);
		}
	 }

}

function show_accdetails()
{

	var obj = 'acc_detail_section';
	if(csv_getField('currentaddress_more_year'))
	 {
		if(csv_getField('currentaddress_more_year').checked)
		{
			show(obj);
		}
		else
		{
			hide(obj);
			csv_getField('account_number').value="";
			csv_getField('routing_number').value="";
			account_number
		}
	 }

}


function resetPrevAddrFields()
{
	csv_getField('previous_address_line1').value = "";
	csv_getField('previous_address_line2').value = "";
	csv_getField('previous_city').value = "";
	csv_getField('previous_zipcode5').value = "";
	csv_getField('previous_state').value = "";
}


