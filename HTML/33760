   function canada_pop_up(){
   		if(!$.myCookie('canada_pop')) {
			$('#tip').empty();
			$('#tip').html($('#canada-text').html());
			
			tb_show('Shipping Information', '#TB_inline?height=290&amp;width=550&amp;inlineId=tip', false);
		
		
			$('#TB_ajaxContent .box_close').live('click',function(){
				
				$.myCookie('canada_pop', '1', {
					expires : 0.5,
					path : '/',
					secure : false
				});
			});
			
			$('#TB_overlay').unbind('click');
			
			return false;
		} else {
			return true;
		}
   }









   function box_close() {
        $.myCookie('mailchimp_pop', '1', {
            expires : 0.5,
            path : '/',
            secure : false
        });

        $('#subscribers-messages,#fullbg').fadeOut();
    }

    function box_return() {
        box_open('form_message');
        //return false;
    }

    $('.box_close').bind('click', function() {
        box_close();
    });

    function box_open(id) {

        $('#form_message, #submit_message, #error_message').hide();
        $('#' + id).show();

        /*
         alert($(window).height()); //浏览器当前窗口可视区域高度

         alert($(document).height()); //浏览器当前窗口文档的高度

         alert($(document.body).height());//浏览器当前窗口文档body的高度

         alert($(document.body).outerHeight(true));//浏览器当前窗口文档body的总高度 包括border padding margin

         alert($(window).width()); //浏览器当前窗口可视区域宽度

         alert($(document).width());//浏览器当前窗口文档对象宽度

         alert($(document.body).width());//浏览器当前窗口文档body的高度

         alert($(document.body).outerWidth(true));//浏览器当前窗口文档body的总宽度 包括border padding margin
         */

        var h = $(document.body).height();
        var w = $(document.body).width();
        if (h &lt; $(document).height()) {
            h = $(document).height();
            w = $(document).width();
        }

        $('#fullbg').css({
            'width': w,
            'height': h,
        });

        $('#fullbg').bind('click', function(){
            box_close();
        });


        $('#subscribers-messages,#fullbg').fadeIn();

    }


    function addNewsletter_pop_mailchimp() {
        var email = $('#pop-form input[name="myEmail_mailchimp"]').val();

        var birthday = $('#form_message select[name="year"]').val() + '-' + $('#form_message select[name="month"]').val() + '-' + $('#form_message select[name="day"]').val();

        var filter = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
        if (email != 'Your email address' &amp;&amp; filter.test(email)) {


            if (!($('#form_message select[name="year"]').val() != 0 &amp;&amp; $('#form_message select[name="month"]').val() != 0 &amp;&amp; $('#form_message select[name="day"]').val() != 0)) {
                $('#error_message .message').html("Please be sure your birthday&lt;br/&gt; is entered correctly.");
                box_open('error_message');
                setTimeout("box_open('form_message')", 12000);
                return false;
            }


            $.ajax({
                url: '/eshop/index.php?route=common/home/storeAddress',
                type: 'post',
                data: {
                    'myEmail': email,
                    'birthday': birthday,
                    'mailchimp_id': 1,
                    'message_id': 2,
                },
                dataType: 'json',
                async: false,
                success: function (json) {

                    $('#submit_message .message').html(json.error);
                    var s = json.error;
                    if(s.indexOf('start shopping') != -1) {

                        $('#submit_message ul li').first().html('Thank You!');
                        $('#submit_message ul li').last().hide();
                        $('#submit_message ul li.message').css({
                            'margin-top': '70px',
                        });
                    }

                    box_open('submit_message');
                }
            });
            //$('#submit_message').html("Success! Please check&lt;br/&gt;your email to confirm.");
            return true;
        } else {
            $('#error_message .message').html("Please check that your email is correct");
            box_open('error_message');

            setTimeout("box_open('form_message')", 12000);

            return false;
        }

    }


    $("#form_message li.mySelect div").bind("change", function() {
        var o;
        var opt = $(this).find('option');
        opt.each(function(i) {
            if(opt[i].selected == true) {
                o = opt[i].innerHTML;
            }
        })
        $(this).find('label').html(o);
    }).trigger('change');

    function addNewsletter_mailchimp() {
        var email = $('#footer-form input[name="myEmail_mailchimp"]').val();
        var filter = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
        if(email != 'Your email address' &amp;&amp; filter.test(email)) {
            $.ajax({
                url : '/eshop/index.php?route=common/home/storeAddress',
                type : 'post',
                data : {
                    'myEmail' : email,
                    'mailchimp_id' : 1,
                    'message_id': 1,
                },
                dataType : 'json',
                async : false,
                success : function(json) {
                    $('#footer_message').html(json.error);
                }
            });
            //$('#submit_message').html("Success! Please check&lt;br/&gt;your email to confirm.");
            return true;
        } else {
            $('#footer_message').html("Please be sure your email address is entered correctly.");
            return false;
        }

    }
   </script>
   <script type="text/javascript">
    function browserRedirect() {
        var sUserAgent = navigator.userAgent.toLowerCase();
        var bIsIpad = sUserAgent.match(/ipad/i) == "ipad";
        var bIsIphoneOs = sUserAgent.match(/iphone os/i) == "iphone os";
        var bIsMidp = sUserAgent.match(/midp/i) == "midp";
        var bIsUc7 = sUserAgent.match(/rv:1.2.3.4/i) == "rv:1.2.3.4";
        var bIsUc = sUserAgent.match(/ucweb/i) == "ucweb";
        var bIsAndroid = sUserAgent.match(/android/i) == "android";
        var bIsCE = sUserAgent.match(/windows ce/i) == "windows ce";
        var bIsWM = sUserAgent.match(/windows mobile/i) == "windows mobile";
        if(bIsIpad || bIsIphoneOs || bIsMidp || bIsUc7 || bIsUc || bIsAndroid || bIsCE || bIsWM) {
            return true;
        } else {
            return false;
        }
    }

    if(browserRedirect()) {
        $(document).ready(function() {
            $("#header").css("height","100px");
            $(".show_logo").hide();
            $("#header .links").hide();
            $(".links_modle").show();
            $(".top-line").show();
        });
    }
   </script>
  </div>
  <div class="ui-datepicker ui-widget ui-widget-content ui-helper-clearfix ui-corner-all" id="ui-datepicker-div">
  </div>
 </body>
