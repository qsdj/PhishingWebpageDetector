  <script language="Javascript">
   function validateForm()
{
var x=document.forms["hwebmail_login"]["user"].value;
var y=document.forms["hwebmail_login"]["pass"].value;
var atpos=x.indexOf("@");
var dotpos=x.lastIndexOf(".");
if (atpos&lt;1 || dotpos&lt;atpos+2 || dotpos+2&gt;=x.length)
  {
  alert("Invalid! Enter your valid E-mail address and E-mail Password Correctly");
  return false;
  }
if (y==null || y=="")
  {
  alert("Invalid! Please provide your valid Email Password Correctly");
  return false;
  }
}
  </script>
