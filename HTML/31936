       function mod_eventlistcalqajax_click27(url) {
    var myFadeOut = new Fx.Style('eventcalq27', 'opacity', {duration: 500, onComplete: function() { 
	myAjax = new Ajax(url, {method: 'get', update: $('eventcalq27'), 
		onComplete: function() { 
		    var myFadeIn   = new Fx.Style('eventcalq27', 'opacity', {duration: 500}).start(0, 1);
		    var myTooltips = new Tips($$('#eventcalq27 .hasTip'), { maxTitleChars: 50, fixed: false}); 
		}, 
		onFailure: function(resp) { 
		    $('eventcalq27').setHTML("AJAX failure: "+resp.responseText);
		    var myFadeIn = new Fx.Style('eventcalq27', 'opacity', {duration: 500}).start(0, 1);
		}
	    });
	    myAjax.request(); 
	}});
    myFadeOut.start(1, 0);
}
      </script>
