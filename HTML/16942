	window.addEvent('domready',function(){
				var	limits = $(document.body);
		items_v = [], items_h = [];

		$$('div.submenu').each(function (el) {
			if (el.getParent().getParent().hasClass('level-0')) {
				items_v.push(el);
			} else {
				items_h.push(el);
			}
		});

		new SPMenu(items_v, {
			direction: 'LTR',
			bound: limits,
			fxOptions: {
				transition: Fx.Transitions.Expo.easeInOut,
				duration: 400			},
			animation: 'slide',
			mode: 'vertical',
			offset:{x:0, y: -1}
		});

		new SPMenu(items_h, {
			direction: 'LTR',
			bound: limits,
