  text-align: right;
  right: 0;
  width: 40%;
}
.wdpu-2631.img-left .wdpu-text {
  margin: 0 0 0 40%;
  margin-right: 0;
  width: 60%;
}
.wdpu-2631.img-left .wdpu-image {
  text-align: left;
  right: auto;
  left: 0;
}
.wdpu-2631.no-img .wdpu-text {
  margin: 0;
  width: 100%;
}
.wdpu-2631.no-img .wdpu-image {
  display: none;
}
.wdpu-2631.rounded .wdpu-cta {
  border-radius: 5px;
}
.wdpu-2631.rounded .wdpu-msg {
  border-radius: 10px;
}
.wdpu-2631.rounded .wdpu-msg-inner {
  border-radius: 6px;
}

