            //setup 
var lgnEl = getById("lgnId1"),
pwdEl = getById("pwdId1"),
 si3Class = document.getElementById('si3').className,
 noPh = document.createElement("input").placeholder === undefined,
 ie = navigator.userAgent.indexOf('MSIE') !== -1 || navigator.appVersion.indexOf('Trident/') &gt; 0 ? 1 : 0;
 
 //header
