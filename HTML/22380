  </script>
  <script src="http://derricvanrensburg.com/artist/wp-content/themes/Theme/DeepFocus/js/jquery.easing.1.3.js" type="text/javascript">
  </script>
  <script src="http://derricvanrensburg.com/artist/wp-content/themes/Theme/DeepFocus/js/superfish.js" type="text/javascript">
  </script>
  <script src="http://derricvanrensburg.com/artist/wp-content/themes/Theme/DeepFocus/js/cufon-yui.js" type="text/javascript">
  </script>
