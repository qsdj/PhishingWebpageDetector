          <!-- The post content box -->
          <div class="the-content-list the-content  ">
           <div class="blog-small-image-content">
            <!-- post title -->
            <h2 class="post-title">
             <a href="https://www.rugnroll.co.uk/asiatic-jute-loop-charcoal-red-stripe-rug/">
              Asiatic Jute Loop Charcoal Red Stripe Rug
             </a>
