  <script type="text/javascript">
   function form_validation()

{

	var i = 0;

	if($('#name').val() == "" || $('#name').val() == "Your name...")

	{

		$("#name_required").show();

		//alert('Enter Name');

		i = 1;

	}

	else

	{

		$("#name_required").hide();

	}

	if($('#email').val() == "" || $('#email').val() == "Your email...")

	{

		$("#email_required").show();

		i = 1;

	}

	else

	{

	

	$email = $('#email').val();	

  	var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;

 	 if( !emailReg.test( $email ) )

	     {

		 $("#email_required").show();  

   		 i = 1;

 		 } 

	else

	 {

   	 return true;

  		}

	

	}

	if($('#subject').val() == "" || $('#subject').val() == "Please type subject...")

	{

		$("#subject_required").show();

		//alert('Enter Name');

		i = 1;

	}

	

	

	if(i == 1)

	{

		return false;

	}



}
  </script>
