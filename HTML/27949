jQuery(window).load(function() {
sidebarheaight = jQuery('#content-sidebar-right').height();
sidebarheaight = sidebarheaight+25;
jQuery('#sidebar-right').css({"min-height":sidebarheaight});
sidebarheaight2 = jQuery('#content-sidebar-left').height();
sidebarheaight2 = sidebarheaight2+25;
jQuery('#sidebar-left').css({"min-height":sidebarheaight2});
containerheight = jQuery('.container').height()+50;
datesheight = jQuery('#dates').height()+150;
if (datesheight &gt; containerheight) {
containerheight = datesheight;
};
containerheighton = containerheight/10;
containerheighton = Math.ceil(containerheighton);
jQuery('.timeline').css({"height":containerheight});
var timeline = jQuery('.timeline');
for (var i = 1; i &lt;= containerheighton; i++) {
timeline.append('&lt;span /&gt;');
};
var numberofli = 3+4+2;
var numberoflitop = 66;
for (var i = 1; i &lt;= numberofli; i++) {
numberoflitop = numberoflitop+99;
timeline.append('&lt;span class="evspan" style="top:'+numberoflitop+'px"&gt;&lt;/span&gt;');
};
});
