				// do field validation
				if (frm.email.invalid) {
					alert( "Por favor, informe um endereço de e-mail válido:" );
				} else if (frm.text.invalid) {
					alert( "Por favor, certifique-se que o formulário esteja totalmente e corretamente preenchido." );
				}
				return false;
			} else {
				frm.submit();
			}
