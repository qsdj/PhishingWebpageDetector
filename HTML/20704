      jqr=jQuery.noConflict();
	jqr(document).ready(function(){
		jqr(".side_nav .children").each(function(){
			//if not a sub-menu item of currently opened menu, hide it
			if(jqr(this).parent().hasClass('current-cat') ||
				jqr(this).parent().hasClass('current-cat-parent'))
					jqr(this).show();
					
			if(jqr(this).parent().hasClass('current-cat-parent'))
				jqr(this).parent().parent().show();
				
			
		});
		//jqr(".imfg_side_nav .current_page_item .children a:link").css('text-decoration','none !important');
		//				alert(jqr(".imfg_side_nav .current_page_item .children a:link").css('text-decoration'));
		var offdiff=jQuery('#sidebar .pagenav').offset().left-jQuery('#sidebar .side_nav').offset().left;
		if(offdiff != -10)
		{
			//alert('changing offset from '+jQuery('#sidebar .pagenav').offset().left);
			var newtop=jQuery('#sidebar .pagenav').offset().top;
			var newleft=jQuery('#sidebar .pagenav').offset().left-10-offdiff;
			/*jQuery('#sidebar .pagenav').offset({ top: newtop, left: newleft });*/
			//alert(jQuery('#sidebar .pagenav').offset().left);
		}
		
	});
