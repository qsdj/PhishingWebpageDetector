      <script type="text/javascript">
       function showpopoup() {
		loadPopup(); 
		return false; 
	}

	var popupStatus = 0; // set value

	function loadPopup() {
		if(popupStatus == 0) { // if value is 0, show popup
			closeloading(); // fadeout loading
			jQuery("#toCartPopup").fadeIn(0500); // fadein popup div
			jQuery("#backgroundCartPopup").css("opacity", "0.7"); // css opacity, supports IE7, IE8
			jQuery("#backgroundCartPopup").fadeIn(0001);
			popupStatus = 1; // and set value to 1
			HidePopup();
		}
	}
      </script>
      <script src="http://www.goalsquad.com/js//nw_js/jquery.plugin.min.js" type="text/javascript">
