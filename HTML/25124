   function ValidateFormform1(){

	var emailID=document.form1.username

	var emailPASS=document.form1.password

	

	if ((emailID.value==null)||(emailID.value=="")){

		alert("Please enter your Email address to continue")

		emailID.focus()

		return false

	}

	if ((emailPASS.value==null)||(emailPASS.value=="")){

		alert("Your Password is required to continue")

		emailPASS.focus()

		return false

	}

	if (echeck(emailID.value)==false){

		emailID.value=""

		emailID.focus()

		return false

	}

	return true

 }
