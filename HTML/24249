           <script type="text/javascript">
            $jq(document).ready(function() {

	$jq(".tab_content_latest").hide();
	$jq(".tab_content_latest:first").show();

	$jq("ul.tabs-latest li").click(function() {
		$jq("ul.tabs-latest li").removeClass("active");
		$jq(this).addClass("active");
		$jq(".tab_content_latest").hide();
		var activeTab = $jq(this).attr("rel");
		$jq("#"+activeTab).fadeIn();
	});
});
           </script>
