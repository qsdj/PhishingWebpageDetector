		$('#webform-component-canton').css('display','none') ;
		$('#webform-component-adresse-pour-la-facture').css('display','none') ;
		
	}
  });
  
  if ($('#edit-submitted-metier-fonction-4').attr('checked')) {
			 
				$('#edit-submitted-autre-metier').attr('disabled',false);
		 }else{
			 	$('#edit-submitted-autre-metier').attr('disabled',true);
				$('#edit-submitted-autre-metier').val('');
		 }
	$('.form-radio').click(function () {
		 if ($('#edit-submitted-metier-fonction-3').attr('checked')) {
			
