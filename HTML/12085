   function wyrownaj()
{
  var el=document.getElementById('tresc');
  var h=el.offsetHeight-13;
 if(h&lt;600)
 {
 h=600;
 }



document.getElementById('lewa_strona').style.height=h+'px';
document.getElementById('wygas').style.top=(h+190)+'px';
document.getElementById('wygas').style.display='block';

}
