  <script>
   $('#slide-header').click( function () {   
				  $('.slide').animate({"left":"+180px" }, "slow" );
				  $('#slide-header').removeClass("in")	
				  $('#slide-header').addClass("out")
				   $('#slide2-header').removeClass("out")	
				  $('#slide2-header').addClass("in")
				  
				 })
				 
				  $('#slide2-header').click( function () {   
				  $('.slide').animate({"left":"0px" }, "slow" );
				  $('#slide-header').removeClass("out")	
				  $('#slide-header').addClass("in")
				   $('#slide2-header').removeClass("in")	
				  $('#slide2-header').addClass("out")
				 
				 })
var opera = Boolean(window["opera"]);
var ie = (navigator.appName.indexOf("Microsoft") != -1) &amp;&amp; !opera;

function mju_play_track(num) {
	if (num &lt;= 0) return false;
	var mc = ie ? window.mju : window.document.mju;
	mc.SetVariable("play_track",num);
}

function mju_play_file(chars) {
	if (!chars.length) return false;
	var mc = ie ? window.mju : window.document.mju;
	mc.SetVariable("play_file",chars);
}

function mju_do(cmd) {
	if (!cmd.length) return false;
	var mc = ie ? window.mju : window.document.mju;
	mc.SetVariable("do_"+cmd," ");
}
  </script>
