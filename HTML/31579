function processSubmitDate(){
    if (csv_getField('time_current_year') &amp;&amp; typeof txField != 'undefined'){
        txField('eventForm', 'applicant[1].totalYearsAtCurrentAddress');
    }
}

function processSubmitState(){
    if (csv_getField('address_state') &amp;&amp; typeof txField != 'undefined'){
        txField('eventForm', 'applicant[1].currentAddressState');
    }
}

function processSubmit(id){
  if (csv_getField(id) &amp;&amp; typeof txField != 'undefined'){
      // set the field value
      csv_findFieldVal('hd_'+id) = csv_findFieldVal(id);
      // submit via txField
      txField('eventForm', id);
  }
}

