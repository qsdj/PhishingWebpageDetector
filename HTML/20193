    function check_header() {



        if ($window.scrollTop() &gt; $header.outerHeight()) {


            $header2.addClass('scroled');

            $header2.focus();

        } else {

            $header2.removeClass('scroled');


        }


    }

    function check_header_ios(){

           if (document.body.scrollTop  &gt; $header.outerHeight()) {
                $header2.addClass('scroled');
                if(!$("#header2 .addItem3 input").is(":focus")) {
                    $header2.css('position', 'fixed');
                    $header2.css('top', 0);
                } else {
                    $header2.css('position', 'absolute');
                    $header2.css('top', document.body.scrollTop);
                }

            } else {
                $header2.removeClass('scroled');

            }
    }






