      <script>
       function addDealProductSidebars(id,qty){
	jQuery("#toCartPopup2").fadeIn('normal');
	jQuery("div.loaderCart").fadeIn('normal');
	jQuery("div.loadecart_text").fadeIn('normal'); 
	jQuery("#backgroundCartPopup").css("opacity", "0.7"); // css opacity, supports IE7, IE8
	jQuery("#backgroundCartPopup").fadeIn('normal');
	var url = 'http://www.goalsquad.com/'+'ajaxcart/index/index';
	jQuery.ajax({
		url: url,
		type: 'post',
		dataType: 'json',
		data:{'id':id,qty:1},
		success:function(data){
			if(data['result'] == 1){
				if(jQuery('#mini-cart')){
					jQuery('#mini-cart').replaceWith(data['sidebar']); 
				}
				jQuery('#popup_content_cart').html(data['productInfo']);
				showpopoup(); 
				disableLoad();
				jQuery('#mini-cart .mini-cart-content').show();
				
			}else if(data['result'] == 'cofig'){
				window.location.href = data['url'];
			}else{
				jQuery('#popup_content_cart').html(data['message']);
				showpopoup(); 
				disableLoad();
			}
		}
	});
}
      </script>
      <script type="text/javascript">
       //&lt;![CDATA[
    function validatePollAnswerIsSelected()
    {
        var options = $$('input.poll_vote');
        for( i in options ) {
            if( options[i].checked == true ) {
                return true;
            }
        }
        return false;
    }
//]]&gt;
      </script>
