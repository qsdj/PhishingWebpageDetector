  <script type="text/javascript">
   $.validator.setDefaults({
                    submitHandler: function() { window.document.frm_contactus.submit() }
                });
                $.metadata.setType("attr", "validate");
                $().ready(function() {
                // validate signup form on keyup and submit
                $("#frm_contactus").validate({
                    rules: {
                        
                        campus: "required",
                        mainInterest: "required",
						workPreference: "required",
						firstName: "required",
                        lastName:"required",
                        email: {
                            required: true,
                            email: true
                        },
                        phone: "required",
						                        address:"required",
						city:"required",
						state:"required",
						zip:"required",
												DateOfBirth:"required",
						aboutus:"required"
                    },
                    messages: {
                        campus: "",
                        mainInterest: "",
						workPreference: "",
						firstName: "Required",
                        lastName: "Required",
                        email: "Required",
                        phone: "Required",
						                        address: "Required",
						city: "Required",
						state: "Required",
						zip: "Required",
												DateOfBirth: "Required",
						aboutus: "Required"
                    }
                });
                
            });
				
			function fun_about(id)
                    {
						var id = window.document.frm_contactus.campus.value;
						if(id != "")
					{
						document.getElementById('HowHeard1div').style.display="block";	
						$.ajax({
						type: "POST",
						url: "http://www.gocsb.com/ajaxHearAbout",
						data: "location=" + id,
						success: function(responce){
							document.getElementById('HowHeard1div').innerHTML=responce;
						}
						});
					}
					else
					{
						document.getElementById('HowHeard1div').style.display="none";	
					}
						
                        
                    }
					function howHeard1Change(selectedValue)
					{
						if(selectedValue != "")
						{
							document.getElementById('WhichWhoDiv').style.display="block";	
							var location = window.document.frm_contactus.campus.value;
						$.ajax({
								type: "POST",
								url: "http://www.gocsb.com/ajaxWhichWho",
								data: "hearabout=" + selectedValue + "&amp;location="+location,
								success: function(responce){
									document.getElementById('WhichWhoDiv').innerHTML=responce;
								}
								});
						}else{
							document.getElementById('WhichWhoDiv').style.display="none";	
						}
						
					}
			
			function fun_radioStation(id)
			{
				//alert(id);
				if(id == 3 || id == 5 || id == 6 || id == 9)
				{
					document.getElementById('radioStation').style.display="block";	
				}
				else
				{
					document.getElementById('radioStation').style.display="none";	
				}
			}
			$(function() {
				$( "#datepicker" ).datepicker({ changeMonth: true, changeYear: true, minDate: '1/1/1900', maxDate: '1/1/2010', defaultDate: '-25Y', yearRange: '-60:-13' });
				});
			/*$(function() {
			$("#txt_DOB").datepicker({ changeMonth: true, changeYear: true, minDate: '1/1/1900', maxDate: '1/1/2010', defaultDate: '-25Y', yearRange: '-60:-13' });
			});*/
