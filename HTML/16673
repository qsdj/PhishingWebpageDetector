            <script type="text/javascript">
             function mycarousel_most_popular_initCallback(carousel) {
                    jQuery('#slideshow_most_popular .navigation a').bind('click', function () {
                        jQuery('#slideshow_most_popular .navigation a').removeClass('active');
                        jQuery(this).addClass('active');
                        carousel.scroll(jQuery.jcarousel.intval(jQuery(this).text()));
                        return false;
                    });
                }

                jQuery(document).ready(function () {
                    if (jQuery('#slideshow_most_popular .slideshow-box').length &gt; 0) {
                        jQuery("#slideshow_most_popular .slideshow-box").jcarousel({
                            scroll: 8,
                            initCallback: mycarousel_most_popular_initCallback,
                            // This tells jCarousel NOT to autobuild prev/next buttons
                            buttonNextHTML: null,
                            buttonPrevHTML: null,
                            animation: 1000,
                            scroll: 1,
                            wrap: 'circular'
                        });
                    }
                });
            </script>
