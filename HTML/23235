$('#cpwd').keyup(function(){
var cpwd = $('#cpwd').val();
var pwd = $('#pwd').val();
if (pwd == cpwd){
	$('#match').html("&lt;span class='hbnp-span-success'&gt;Password matches!&lt;/span&gt;");
} else {
	$('#match').html("&lt;span class='hbnp-span-error'&gt;Password doesn't match!&lt;/span&gt;");
}

});


function sethbnpcookie(){
$.cookie('hbnp_one_time', 'b7a782741f667201b54880c925faec4b');
}

function autoclose(){
setTimeout(function(){
	$.magnificPopup.close();
}, 7000);
}


$(document).ready(function () {
setTimeout(function(){
if (($('#hb-nl-popup').length) &amp;&amp; ($.cookie('hbnp_one_time') != 'b7a782741f667201b54880c925faec4b') &amp;&amp; (1== 1 )){
	$.magnificPopup.open({
		items: {src: '#hb-nl-popup'},
		type: 'inline',
		
				
		removalDelay: 800,
		callbacks: {
			beforeOpen: function(){
				this.st.mainClass = 'mfp-zoom-in';
			}
		}
	});
}
}, 1000);  
}); 

$("#hbnp_subscribe").click(function(){var e=$("#hbnp_email").val(),s=/^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;return s.test(e)&amp;&amp;""!=$.trim(e)?($("#loadgif_hbnp").show(),void $.ajax({type:"post",url:"index.php?route=module/hbnp_core",data:{email:$("#hbnp_email").val(),childname:$("#hbnp_childname").val(),childage:$("#hbnp_childage").val()},dataType:"json",success:function(e){e.success&amp;&amp;($("#nl_form").html(""),$("#hbnp_msg").html(e.success),$("#loadgif_hbnp").hide(),sethbnpcookie(),autoclose(),setTimeout(function(){"1"==e.hbnp_redirect&amp;&amp;(window.location.href=window.location.href)},7e3))},error:function(e,s,n){alert(n+"\r\n"+e.statusText+"\r\n"+e.responseText)}})):(alert("Invalid email"),!1)});
    </script>
