             <script type="text/javascript">
              function clearText(theField)
					{
    					if (theField.defaultValue == theField.value)
    					theField.value = '';
					}
					
					function addText(theField)
					{
    					if (theField.value == '')
    					theField.value = theField .defaultValue;
					}
             </script>
             <input type="text" class="input-text required-entry validate-email" title="Assine nossa newsletter" value="Your email address" onfocus="clearText(this)" onblur="addText(this);" id="newsletter" name="email" />
            </div>
