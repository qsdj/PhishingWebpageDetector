   var map;
function initialize() {
  var mapOptions = {
    zoom: ,
    center: new google.maps.LatLng()
  };
  map = new google.maps.Map(document.getElementById('map-canvas'),
      mapOptions);
      map.setMapTypeId(google.maps.MapTypeId.);    
}

google.maps.event.addDomListener(window, 'load', initialize);
