       <form onsubmit="javascript:return valid_search();" enctype="multipart/form-data" id="search_frm" name="search_frm" method="post" action="http://www.yourcube.com/search">
        <div id="search_keyword1" class="searchbox">
         <input type="text" class="input1" value="" id="search_keyword" name="search_keyword" />
         <input type="submit" onclick="javascript:return valid_search();" class="search" value="SEARCH" id="search_submit" name="search_submit" />
        </div>
       </form>
       <script type="text/javascript">
        function valid_search()
            {
                var val = true;
            
                if(document.getElementById("search_keyword").value == "" )
                {
                    //alert("Please enter eny keyword");
                    document.getElementById("search_keyword1").style.border = "1px solid red";
                    //document.getElementById("search_keyword").style.border = "1px solid red";
                    document.getElementById("search_keyword").focus();
                    val = false;
                }
                else
                {
                    
                    document.getElementById("search_keyword1").style.border = "1px solid #b9b2ae";
                    
                }
                
                return val;
                
            }
       </script>
