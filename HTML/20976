		// gallery carousel
		$(".gallery2").jCarouselLite({
				  btnNext: ".next",
				  btnPrev: ".prev",
				  circular: true,
				  visible: 3
		 });
		$('.prev img, .next img').animate({opacity:'1.0'},{queue:false,duration:0});
		$('.prev, .next').hover(function(){
			$(this).find('img').animate({opacity:'0.7'},{queue:false,duration:500});
		}, function(){
			$(this).find('img').animate({opacity:'1.0'},{queue:false,duration:500});
		});	

