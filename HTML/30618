      jQuery('#tab-container').imagesLoaded( function() {
                    
                    // images have loaded
                    jQuery('#serviceTab a').click(function (e) {
                        e.preventDefault()
                        jQuery(this).tab('show')
                    });

                    //fire up the sticky sidebar that bootstrap affix was messing up
                    //jQuery('#side-nav').theiaStickySidebar({
                    //});

                    //fire up gumshoe, our non buggy scrollspy alternative
                    gumshoe.init({
                        offset: 20, // Distance in pixels to offset calculations
                    });

                    //destroy gumshoe and restart it when either of the tabs are clicked
                    jQuery( "#managed-button, #diybutton" ).click(function() {
                        gumshoe.destroy();
                        gumshoe.init({
                            offset: 20, // Distance in pixels to offset calculations
                        });
                    });
                });
