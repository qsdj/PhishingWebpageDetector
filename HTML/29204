      // &lt;![CDATA[
                var $popover = $('#saveUsernameWarning').wfPopover();
                $('#saveUsername').attr('value', $('#saveUsernameCheckbox').prop('checked'));
                $('#saveUsernameCheckbox').on('click', function() {
                	if ($(this).prop('checked') == true) {
                		$popover.wfShowPopover();
                	} else {
                		$popover.wfHidePopover();
                	}
                	$('#saveUsername').attr('value', $('#saveUsernameCheckbox').prop('checked'));
                });

                document.Signon.j_username.focus();
            // ]]&gt;
