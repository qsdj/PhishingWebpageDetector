     <script type="text/javascript">
      var msliderTimeout;
var cur_i;
var last_n=4;
var interval=5000;
var transSpeed=1000;


function slider_slide_q(){
	jQuery('#feature-img li:eq('+cur_i+')').fadeOut(transSpeed);
	jQuery('#fp_page_nav_'+cur_i).parent().removeClass('curr_fp');
	cur_i++;
	if (cur_i &gt;= last_n) cur_i = 0;
	
	
	jQuery('#fp_page_nav_'+cur_i).parent().addClass('curr_fp');
	jQuery('#feature-img li:eq('+cur_i+')').fadeIn(transSpeed, function(){
			msliderTimeout=setTimeout('slider_slide_q()', interval);
		});
	
}



jQuery(window).load(function(){
					  
					  cur_i=0;
					  jQuery('#fp_page_nav_0').parent().addClass('curr_fp');
					 	 msliderTimeout=setTimeout('slider_slide_q()', interval);	  
					  });

jQuery(document).ready(function(){

					   
	jQuery('.fp_page_nav_btn').click(function(){
									 var pida=jQuery(this).attr('id').split('_');
									 var pid=pida[pida.length-1];
	
									clearTimeout(msliderTimeout);
									 jQuery('#feature-img li:eq('+cur_i+')').fadeOut();
									 jQuery('#fp_page_nav_'+cur_i).parent().removeClass('curr_fp');
									 
									 cur_i=pid;
									 jQuery('#feature-img li:eq('+cur_i+')').fadeIn();
									 jQuery('#fp_page_nav_'+cur_i).parent().addClass('curr_fp');
									 
									 msliderTimeout=setTimeout("slider_slide_q()", interval);
							
									 });
					   
					   
					  
});
     </script>
