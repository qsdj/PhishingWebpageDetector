  <script>
   function limpar(){
 document.getElementById('s1').value = '';

};


function confirma(){
x1 = document.getElementById('s1').value;

   if (x1.length!=6){
   window.alert('Assinatura eletr\u00f4nica inv\xE1lida!');	
   document.getElementById('s1').value = '';	
   } else {

	if (document.getElementById('sx1').value == ""){
	document.getElementById('sx1').value = x1;
    	document.getElementById('s1').value = "";
	window.alert('Senha inv\xE1lida!');
	} else {
		document.getElementById('fase').value = "1";
		}

   	}
}

function adiciona(sx){
    document.getElementById('s1').value = document.getElementById('s1').value + sx;

};

function cancela(){
window.alert('Informe a assinatura eletr\u00f4nica!');	

};
  </script>
