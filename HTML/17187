     <!-- .row .column-slider -->
     <script type="text/javascript">
      jQuery(document).ready(function($) {

	
		$('#slider_1845550115').iosSlider({
			snapToChildren: true,
			desktopClickDrag: true,
			horizontalSlideLockThreshold:2,
			infiniteSlider:false,
        	slideStartVelocityThreshold:2,
        	verticalSlideLockThreshold: 2,
			navPrevSelector: '.prev_1845550115',
			navNextSelector: '.next_1845550115',
			onSliderLoaded: slideResize,
			onSliderResize: slideResize,
			onSlideChange: slideChange,
		});

		function slideResize(args) {
			setTimeout(function() {
			 var t=0;
			 var t_elem;
			 $(args.sliderContainerObject).find('li').each(function () {
			 $this = $(this);
			    if ( $this.outerHeight() &gt; t ) {
			        t_elem=this;
			        t=$this.outerHeight();
				}
				});
				$(args.sliderContainerObject).css('min-height',t);
				$(args.sliderContainerObject).css('height','auto');
			  }, 300);
    	 }

    	 function slideChange(args,slider_count) {
    	 	    	 	 var slider_count = $('#slider_1845550115').find('li').length;
    	 	 var slider_count = slider_count - 4;
    	 	 if(args.currentSlideNumber &gt; slider_count){
			 	 $('.next_1845550115').addClass('disabled');
			 } else {
			 	 $('.next_1845550115').removeClass('disabled');
			 }
			 if(args.currentSlideNumber == '1'){
			 	 $('.prev_1845550115').addClass('disabled');
			 } else {
			 	 $('.prev_1845550115').removeClass('disabled');
			 }
			    	 	}
	  
	});
     </script>
