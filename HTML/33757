	$('#banner_list').cycle({
		fx : 'fade',
		speed : 2000,
		timeout: 8000,
		before : function() {
			var i = $(this).children().attr('alt');
			var navigation = $('#navigation img');
			for(var j = 0; j &lt; navigation.length; j++) {
				if(j == i) {
					navigation[j].src = "../image/home/homepage-select.png";
				} else {
					navigation[j].src = "../image/home/homepage-noselect.png";
				}
			}
		},
		startingSlide : 0,
		page : '#navigation',
		pagerAnchorBuilder : function(idx, slide) {
			return '#navigation img:eq(' + (idx) + ')';
		}
	});
