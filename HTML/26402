               function doSearch(){
							if (document.smallSearchForm.words.value=='') alert("Вы не указали критерий поиска");
							else {
								var href = "/?page=homesearch&amp;words="+document.smallSearchForm.words.value;
								href=href.replace(/\s/gi,'+');
								window.location.href=href;
							}
