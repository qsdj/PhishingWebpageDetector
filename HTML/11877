     <section class="grid-block wrapped" id="top-a">
      <div class="grid-box width66 grid-h" style="width: 696px;">
       <div class="module mod-line  deepest" style="min-height: 353px;">
        <div style="position:absolute;left:-5000px;">
         :
        </div>
        <script type="text/javascript">
