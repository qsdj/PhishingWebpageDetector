// Load the SDK asynchronously
(function(d){
    var js, id = 'facebook-jssdk', ref = d.getElementsByTagName('script')[0];
    if (d.getElementById(id)) {return;}
    js = d.createElement('script'); js.id = id; js.async = true;
    js.src = "//connect.facebook.net/en_US/all.js";
    ref.parentNode.insertBefore(js, ref);
}(document));

// Additional JS functions here
window.fbAsyncInit = function() {
    FB.init({ apiKey: '114811075289477', status : true, cookie : true, oauth: true});


    // Check if the current user is logged in
    // and has authorized the app


    if (top === self) {
        // no frame
    } else {
        if ('false' != 'true' ) {
            FB.getLoginStatus(function(response) {
                // Check the result of the user
                if(response &amp;&amp; response.status == 'connected') {
                    // The user is connected to Facebook
                    // and has authorized the app.
                    // Now personalize the user experience



                    $.post(
                            rootUrl + 'wsdl/social.ajax.php',
                            {
                                provider: 'facebook',
                                userid: response.authResponse.userID,
                                token: response.authResponse.accessToken
                            }, callBackForLogin, 'json'
                    );


                } else {
                    // The user has not authenticated your app,
                    // proceed with your normal (anonymous) flow.
                }
            });
        }
    }





};




var permissions = "email, publish_stream, user_events,user_groups, user_about_me, friends_about_me, user_birthday, friends_birthday, user_education_history, friends_education_history, email, user_hometown, friends_hometown, user_relationship_details, friends_relationship_details, user_location, friends_location, user_religion_politics, friends_religion_politics, user_likes, friends_likes, user_relationships, friends_relationships, user_website, friends_website";

var callBackForLogin = function(data){
//    console.log(data);
    $('#RequestMailDialog').jqmHide();
    $('#RequestLoginDialog').jqmHide();


    ET_Event.eventStart('SocialLogin', data.provider, 'LoginSocial', '');

    if (data.CookieName) {
        var today = new Date();
        var expire = new Date();
        var expDate = 3600*24*365;
        if (parseInt(data.CookieExp) &gt; 0) expDate = data.CookieExp;
        expire.setTime(today.getTime() + expDate*1000);
        document.cookie = data.CookieName + '=' + data.CookieValue + ';expires=' + expire.toGMTString() + ';path=/';
//        console.log('cookieset');
    }



    if (data.next_action == 'go_to_dash') {
        add_message('&lt;img src="../images/new_loader.gif"/&gt;', 4);
        if (data.goto_url != '') {
            console.log("goto " + data.goto_url);
            window.location.href =   data.goto_url;
        } else {

            window.location.reload();


//            if (window.location.href == rootUrl + 'login') {
//                window.location.href = prevUrl;
//
//            } else {
//                console.log("HERE");
//              window.location.reload();
//            }
        }
    } else if (data.next_action == 'show_popup') {
        $('#request_mail').val('');
        $('#social_email_error').hide();
        $('#request_mail').removeClass('errored');
        $('.username_social').text(data.username);
        $('#RequestMailDialog').jqmShow();
    } else if (data.next_action == 'show_popup2') {
        $('#request_login').val('');
        $('#social_login_error').hide();
        $('.username_social').text(data.username);
        $('#RequestLoginDialog').jqmShow();
    } else if (data.next_action == 'show_error') {
        add_message(data.error);
    }

    return false;
}

$('.social_facebook').bind('click', function() {
    FB.login(function(object) {

        if (object.status != "connected") {
            add_message('Connection error.');
            return false;
        }

        add_message('&lt;img src="../images/new_loader.gif"/&gt;', true);

        //console.log(object);

        $.post(
                rootUrl + 'wsdl/social.ajax.php',
                {
                    provider: 'facebook',
                    userid: object.authResponse.userID,
                    token: object.authResponse.accessToken
                }, callBackForLogin, 'json'
        );

    }, {scope: permissions});

});

$('.import_facebook').bind('click', function() {

    FB.login(function(object) {

        if (object.status != "connected") {
            add_message('Connection error.');
            return false;
        }

        add_message('&lt;img src="../images/new_loader.gif"/&gt;', true);

        //console.log(object);

        $.post(
                rootUrl + 'wsdl/social.ajax.php',
                {
                    provider: 'facebook',
                    userid: object.authResponse.userID,
                    token: object.authResponse.accessToken
                }, function(){
                    if ((typeof FriendsCount != 'undefined')) {
                        $('.import_fb_button_create').hide();
                        $('.fb_import_block').show();
                    } else
                    getFriends();
                }, 'json'
        );

    }, {scope: permissions});

});


$('.social_google').bind('click', function() {
    PopupCenter('/wsdl/google.login.php','Login', 600, 700);
});



function onGoogleLogin(){
    targetWin.close();
    add_message('&lt;img src="../images/new_loader.gif"/&gt;', true);
    $.post(
            rootUrl + 'wsdl/social.ajax.php',
            {
                provider: 'google'
            }, callBackForLogin, 'json'
    );
    return false;
}


var targetWin;

function PopupCenter(pageURL, title,w,h) {
    var left = (screen.width/2)-(w/2);
    var top = (screen.height/2)-(h/2);
    targetWin = window.open (pageURL, title, 'toolbar=no, location=no, directories=no, status=no, menubar=no, scrollbars=no, resizable=no, copyhistory=no, width='+w+', height='+h+', top='+top+', left='+left);
}

