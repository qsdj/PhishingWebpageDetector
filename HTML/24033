  <script>
   $(document).ready(function(){
  			$("#student_visa").hide();
  			$("#sponsor_visa").hide();
  			$("#partner_visa").hide();
  			$("#employer_nomination_scheme_visa").hide();
  			$("#skilled_regional_visa").hide();
  			$("#skilled_nominated_visa").hide();
  			$("#parent_visa").hide();
  			$("#refugee_visa").hide();
  			
  			$("#visas a.student_visa").click(function(){
  				$("#visas a").css("color", "#F8F8F8");
  				$("#visas a.student_visa").css("color", "#383838");
  				$("#australian_flag").fadeOut(500);
  				$("#sponsor_visa").fadeOut(500);
  				$("#partner_visa").fadeOut(500);
  				$("#employer_nomination_scheme_visa").fadeOut(500);
  				$("#skilled_regional_visa").fadeOut(500);
  				$("#skilled_nominated_visa").fadeOut(500);
  				$("#parent_visa").fadeOut(500);
  				$("#refugee_visa").fadeOut(500);
  				$("#student_visa").fadeIn(500);
  			});
  			
  			$("#visas a.sponsor_visa").click(function(){
  				$("#visas a").css("color", "#F8F8F8");
