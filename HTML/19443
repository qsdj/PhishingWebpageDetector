ame:focus,.component-block .component-content .user input#username:focus,.component-block .component-content .user input#password:focus,.component-block .component-content .user input#passwd:focus,.component-block .component-content .user input#password2:focus,.rt-joomla .contact input.required:focus,.rt-joomla .contact textarea.required:focus,.rt-joomla .user .inputbox:focus{border:1px solid #cab301}.menutop &gt; .fusion-js-subs &gt; .fusion-submenu-wrapper.level2 .rt-arrow-pointer{border-top-color:#000}.component-block .inputbox:focus,.component-block textarea:focus,.inputbox:focus,textarea:focus,.rt-joomla .contact input.required:focus,.rt-joomla .contact textarea.required:focus,.rt-joomla .user .inputbox:focus{box-shadow:inset 0 1px 3px rgba(202,179,1,0.6),0 0 8px rgba(202,179,1,0.6)}body ul.circle-checkmark li:before,body ul.square-checkmark li:before,body ul.checkmark li:after,body ul.triangle-small li:after,body ul.triangle li:after,body ul.triangle-large li:after,body ul.circle-small li:after,body ul.circle li:after,body ul.circle-large li:after{border-color:#cab301}.layout-slideshow .sprocket-features-pagination li.active:before{border-right-color:#cab301}.layout-top .sprocket-tabs-nav li.active{border-bottom-color:#cab301}.layout-bottom .sprocket-tabs-nav li.active{border-top-color:#cab301}.layout-left .sprocket-tabs-nav li.active{border-right-color:#cab301}.layout-right .sprocket-tabs-nav li.active{border-left-color:#cab301}body,.box4 .rt-block,#rt-footer,#rt-copyright,.pattern-item,.component-block .box1 .module-surround{background-color:#000}.component-block a,#rt-content-bottom a,#rt-content-top a{color:#3d3d3d}.component-block .readon,#rt-content-bottom .readon,#rt-content-top .readon{background-color:#3d3d3d}#rt-main-container{background-color:#1a1a1a}.title3 .module-title{background-color:#0d0d0d}.sprocket-bottom-gradient{background:linear-gradient(bottom,rgba(26,26,26,1) 0%,rgba(26,26,26,0) 100%);background: -moz-linear-gradient(bottom,rgba(26,26,2