	function closeDiv()  
	{
	// Reset the timer
	_toResetTimeOut();
	callToServerResetTimer();
	// Close the time out layer 
	closeMyPopup();
	}
	var IFrameTimeObj;
var resetTimeOutURLApp;
if(this["systemResetTimeoutServletURL"] != null)
{
	
	resetTimeOutURLApp = systemResetTimeoutServletURL;
}
else
{
	resetTimeOutURLApp = baseUrl + "/resetTimeout.do";
}
function callToServerResetTimer()
{	
	if (!document.createElement) {return true};
	var IFrameDoc;	
	var URL = resetTimeOutURLApp;
	if (!IFrameTimeObj &amp;&amp; document.createElement) {
		try {
			var tempIFrameTime=document.createElement('iframe');
			tempIFrameTime.setAttribute('id','IFrameTime');
			tempIFrameTime.style.border='0px';
			tempIFrameTime.style.width='0px';
			tempIFrameTime.style.height='0px';
			IFrameTimeObj = document.body.appendChild(tempIFrameTime);
			
			if (document.frames) {
				IFrameTimeObj = document.frames['IFrameTime'];
			}
		} catch(exception) {
			iframeHTML='&lt;iframe id="IFrameTime" style="';
			iframeHTML+='border:0px;';
			iframeHTML+='width:0px;';
			iframeHTML+='height:0px;';
			iframeHTML+='"&gt;&lt;\/iframe&gt;';
			document.body.innerHTML+=iframeHTML;
			IFrameTimeObj = new Object();
			IFrameTimeObj.document = new Object();
			IFrameTimeObj.document.location = new Object();
			IFrameTimeObj.document.location.iframe = document.getElementById('IFrameTime');
			IFrameTimeObj.document.location.replace = function(location) {
				this.iframe.src = location;
			}
		}
	}
	
	if (navigator.userAgent.indexOf('Gecko') !=-1 &amp;&amp; !IFrameTimeObj.contentDocument) {
		setTimeout('callToServerResetTimer("'+theFormName+'")',10);
		return false;
	}	
	if (IFrameTimeObj.contentDocument) {
		IFrameDoc = IFrameTimeObj.contentDocument; 
	} else if (IFrameTimeObj.contentWindow) {
		IFrameDoc = IFrameTimeObj.contentWindow.document;
	} else if (IFrameTimeObj.document) {
		IFrameDoc = IFrameTimeObj.document;
	} else {
		return true;
	}
	IFrameDoc.location.replace(URL);
	return false;
}			
//	document.write('&lt;iframe id="Myiframe" frameborder="0" style="position: absolute\; z-index: 10\; display: none\;"&gt;');
   
    if(((navigator.userAgent.indexOf("Firefox")!=-1) || (navigator.userAgent.indexOf("Chrome")!=-1)) &amp;&amp; !isTransparentSession)
	
    {
	document.write('&lt;iframe id="Myiframe" src="javascript:false;" frameborder="0" style="position: absolute\; z-index: 10\; display: none\;"&gt;');
	document.write('&lt;\/iframe&gt;');
	document.write('&lt;Div id="mypopup" style="position: absolute\; display:none\;z-index: 11\; width: 310px\; background:#FFFFFF\; border: 1px solid #000\;"&gt;');
	document.write(TimeOutImage); 
	document.write('&lt;Div&gt;');
	document.write('&lt;Table border=0 summary=""&gt;');
    //Timeout Warning Message
    document.write('&lt;TR&gt;&lt;TD style="height:19px"&gt;&lt;\/TD&gt;&lt;\/TR&gt;');
    //document.write('&lt;TR valign=top&gt;&lt;TD style="padding: 0px 7px 0px 8px\; height:157px"&gt;\n');
    document.write('&lt;TR valign=top&gt;&lt;TD style="padding: 0px 7px 0px 8px\; height:155px"&gt;\n');
	document.write('&lt;p class="text2"&gt;\n'); 
    /*if(isTransparentSession)
    {    	
   		document.write(STR_CCTIMEOUTMESSAGE_START + ' ' + _toCalculateMinutes(_toGraceDelayTime + _toDelayTime) + ' ' + STR_CCTIMEOUTMESSAGE_END);    	
    }
    else
    {*/
	   	document.write(STR_BANKINGTIMEOUTMESSAGE);  
    //}
	//Display 'Ok' button
	document.write('&lt;\/p&gt;\n');
	document.write('&lt;\/TD&gt;&lt;\/TR&gt;\n');
	document.write('&lt;TR valign="top"&gt;&lt;TD align="center" style="height: 20px\;" &gt;\n');
    getButton(BTN_OK, 'javascript:closeDiv();', '', '', '/sas/sas-docs/images/', '', 'btn1', '', document );
    document.write('&lt;\/td&gt;&lt;\/tr&gt;\n'); 
   // document.write('&lt;TR&gt;&lt;TD style="height: 22px"&gt;&lt;\/TD&gt;&lt;\/TR&gt;');
   document.write('&lt;TR&gt;&lt;TD style="height: 25px"&gt;&lt;\/TD&gt;&lt;\/TR&gt;');
