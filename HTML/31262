            <!-- Plugins: BeforeDisplay -->
            <!-- K2 Plugins: K2BeforeDisplay -->
            <div class="catItemHeader">
             <!-- Item title -->
             <h2 class="catItemTitle ItemTitle">
              <a href="/component/k2/item/254-the-easiest-way-to-generate-regular-residual-income-online-google-adsense">
               The Easiest Way to Generate Regular Residual Income Online - Google Adsense
              </a>
             </h2>
             <ul class="vina-header-toolbar">
              <!-- Item category name -->
              <li>
               <div class="catItemCategory">
                <em class="icon-folder-open icon-color">
                </em>
                <a href="/component/k2/itemlist/category/9-blogging-and-monetization">
