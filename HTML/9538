  <script type="text/javascript" language="javascript">
   function check(form)
{
//alert("test");
var err_head  = "Following fields are required\n\n";
var err_info  = "";
var sr_no     = 1;

var name  			= document.getElementById("name").value;
var email 			= document.getElementById("email").value;
var mobile_num 		= document.getElementById("mobile_num").value;
var city	 		= document.getElementById("city").value;
var letters_code	= document.getElementById("letters_code").value;


if(name=="")
{
 	err_info=err_info+(sr_no++)+")  "+"Please enter your name\n";
}

if(email=="")
{
	err_info=err_info+(sr_no++)+")  "+"Please enter your email...\n";
}

if(mobile_num=="")
{
	err_info=err_info+(sr_no++)+")  "+"Please enter your mobile number...\n";
} 
if(mobile_num != "")
{
   if(isNaN(mobile_num))
     {
	  err_info=err_info+(sr_no++)+")  "+"Only Integers are Allowed in Mobile No.\n";
	 }	 
}
if(city=="")
{
	err_info=err_info+(sr_no++)+")  "+"Please enter your city...\n";
} 
if(letters_code=="")
{
	err_info=err_info+(sr_no++)+")  "+"Please enter the captcha code...\n";
} 
 

if(err_info)
{
 alert(err_head+err_info);
 return false;
}

}
  </script>
  <script type="text/javascript" language="JavaScript">
   function refreshCaptcha()
{
	var img = document.images['captchaimg'];
	img.src = img.src.substring(0,img.src.lastIndexOf("?"))+"?rand="+Math.random()*1000;
}
