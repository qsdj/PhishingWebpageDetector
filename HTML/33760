    <script type="text/javascript">
     $(document).ready(function(){
            $(".mod_show").css("height","100px");
            $(".mod_show").css("width","100px");
        })
        function show_list(obj){
            if( obj == 'open'){
                $(".show_list_open").hide();
                $(".show_list_close").show();
                $(".mod_tit").show();
            }else{
                $(".show_list_open").show();
                $(".show_list_close").hide();
                $(".mod_tit").hide();
            }
        }
    </script>
