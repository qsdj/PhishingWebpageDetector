jQuery(document).ready(function() 
{  var checkpagename = ""; 

if(checkpagename != "home" &amp;&amp; checkpagename != "tuv-rated" &amp;&amp; checkpagename != "about-us" &amp;&amp; checkpagename != "truss-quality"){
    jQuery('div.page-title h1, div.page-title h2, div.block .block-title strong span').each(function() { 
        var h = jQuery(this).html(); 
        var index = h.indexOf(' '); 
        if(index == -1) { 
            index = h.length; 
        } 
        jQuery(this).html('&lt;span class="firstWord"&gt;' + h.substring(0, index) + '&lt;/span&gt;' + h.substring(index, h.length)); 
    }); 
	}
	
	jQuery('.configurable-price-from-label').hide();
});
