  <script src="https://www.natoa.org/web/assets/js/jquery.nicescroll.min.js">
  </script>
  <script type="text/javascript">
   $(document).ready(function(){
		$("#boxscroll").niceScroll({touchbehavior:false,cursorcolor:"#676767",cursoropacitymax:1,cursorwidth:10,background:"#b7b7b7",autohidemode:false});	
		$("#home").height($(window).height()-110); 
		$(".callbacks img").height($(window).height());
		$(".callbacks .caption").height($(window).height()-110);
		var hm_height = $('#slider1').height()/2;
		hm_height = hm_height - 50;
		$('.callbacks_nav').css('top', hm_height + 'px');
	  	$('.textcol1').height($('.textcol1').height());
		$('.textcol2').height($('.textcol1').height());
	});
	window.onresize = function(event) {
		$("#home").height($(window).height()-110); 
		$(".callbacks img").height($(window).height());
		$(".callbacks .caption").height($(window).height()-110);
		var hm_height = $('#slider1').height()/2;
		hm_height = hm_height - 50;
		$('.callbacks_nav').css('top', hm_height + 'px');
	  	$('.textcol1').height($('.textcol1').height());
		$('.textcol2').height($('.textcol1').height());
	};
  </script>
  <script src="https://www.natoa.org/web/assets/js/jquery.flexisel.js" type="text/javascript">
  </script>
  <script>
   $(window).load(function() {
		$("#flexiselDemo1").flexisel();   
		$('.mob_menu').click(function(){
			$('.navWrapper').slideToggle();
		});
		$('.log_tab').click(function(){
			$('.popup_sec').fadeIn('fast');
		});
		$('.close_btn,.overlay_sec,.cle').click(function(){
			$('.popup_sec').fadeOut('slow');
		});
	});
