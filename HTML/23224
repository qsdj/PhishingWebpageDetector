            <script type="text/javascript">
             //&lt;![CDATA[
var webform_10 = new VarienForm('webform_10', 0);
var webform_10_submit = function(){
	var form = webform_10;
	if(form.validator &amp;&amp; form.validator.validate()){
		form.submit();
				$('webform_10_submit_button').hide();
		$('webform_10_sending_data').show();
			}
};

var iframe = $('webform_10_iframe');
iframe.observe('load',function(){
	var doc = this.contentDocument ? this.contentDocument: this.contentWindow.document;
	var json = {success:false};
	if(doc.body.innerHTML.unfilterJSON())
		json = doc.body.innerHTML.evalJSON();
	else return;
	if(json.success &gt; 0){
		if(json.script){
			eval(json.script);
			return;
		}
		if(json.redirect_url){
			$('webform_10_progress_text').update('Redirecting');
			window.location = json.redirect_url;
			return;
		}
		$('webform_10_progress_text').update('Complete');
		Effect.Fade('webform_10_form',{
			duration: 0.5, from:1, to:0,
			afterFinish: function(){
				var successText = json.success_text.unescapeHTML();
				$('webform_10_success_text').update(successText).show();
				Effect.Fade('webform_10_success_text',{
					duration:0.5, from:0, to:1
				});
				if(0) $('webform_10_success_text').scrollTo();
			}
			
		});
	} else {
		if($('webform_10_sending_data'))
			$('webform_10_sending_data').hide();
		if($('webform_10_submit_button'))
			$('webform_10_submit_button').show();
		if(json.errors &amp;&amp; typeof(json.errors) == "string"){
			Dialog.alert(json.errors.unescapeHTML(),{
				title: "Error(s) occured",
				className: "alphacube",
				width:300, 
				buttonClass: "button",
				okLabel: "Close",
				destroyOnClose: true, 
				recenterAuto:true
			})
		} else {
			alert('Error(s) occured');
		}
		if(json.script){
			eval(json.script);
		}
	}
});
//]]&gt;
            </script>
