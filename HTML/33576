     function toggleHeaderLinks() {
		var jsHelpDivId = document.getElementById("jsHelpDiv");
		var noJsHelpDivId = document.getElementById("noJsHelpDiv");

		jsHelpDivId.style.display="inline";
		noJsHelpDivId.style.display="none";
	}
