        <script>
         //&lt;![CDATA[
    var store = new google.maps.LatLng(42.1451239, -87.8622877);
    var marker;
    var map;

    function initialize() {
        var mapOptions = {
            zoom: 15,
            mapTypeControl:false,
            streetViewControl: false,
            zoomControl:false,
            mapTypeId: google.maps.MapTypeId.ROADMAP,
            center: store
        };

        map = new google.maps.Map(document.getElementById('block-map'), mapOptions);

        marker = new google.maps.Marker({
            map: map,
            draggable: false,            
            animation: google.maps.Animation.DROP,
            position: store,
            icon: 'http://ellingtonbox.com/skin/frontend/zonda/default//images/icons/icon_gmap_marker.png'
        });
    }

    google.maps.event.addDomListener(window, 'load', initialize);
    google.maps.event.addDomListener(window, 'resize', initialize);
    //]]&gt;
        </script>
