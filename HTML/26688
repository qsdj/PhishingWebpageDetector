   tinyMCE.init({
                mode : "textareas",
                theme : "simple"
            });
  </script>
  <!-- /TinyMCE -->
  <script type="application/javascript">
   // Returns text statistics for the specified editor by id
function getStats(id) {
    var body = tinymce.get(id).getBody(), text = tinymce.trim(body.innerText || body.textContent);

    return {
        chars: text.length,
        words: text.split(/[\w\u2019\'-]+/).length
    };
}



function sendEventMail(uid,lid)
{
	var maildsc = tinyMCE.get("mail_desc"+uid).getContent();
	
	if(lid=="")
	{ 
		alert("Please Login First.");
		return false;
	}
	
	if($('#subj'+uid).val().length==0)
	{
		alert("Please Enter Subject.");
		$('#sub'+uid).focus();
		return false;
	}
	
	if($('#subj'+uid).val().length&lt;11)
	{
		alert("Subject can not be less then 11 characters.");
		$('#sub'+uid).focus();
		return false;
	}
	
	 if (getStats("mail_desc"+uid).chars &lt; 11) {
        alert("Message can not be less then 11 characters.");
        return false;
     }
	 
	 if (getStats("mail_desc"+uid).chars &gt; 255) {
        alert("Message can not be more then 255 characters.");
        return false;
     }
	
	if(maildsc=="")
	{
		alert("Please Enter Message to be Sent.");
		$('#maildesc'+uid).focus();
		return false;
	}
	
	jQuery.ajax({
		  type: "POST",
		  data: "userid="+uid+"&amp;subj="+document.getElementById('subj'+uid).value+"&amp;maildesc="+maildsc+"&amp;cr=BDemail",
		  url: "includes/mail.php",
		  success: function(result) {
				  $("#sendeventmailrow"+uid).hide("100");
				  $('#msgrow'+uid).show("300");
				  alert("Message Sent Successfully.");
				  window.location.reload();
		  }
	  });
}

