  <strong class="text-info">
   <script type="text/javascript">
    function checkCaptcha()
 {
	 correct_captcha=document.getElementById('correct_captcha').value;
	 enter_captcha=document.getElementById('enter_captcha').value;
	 if(correct_captcha!=enter_captcha)
	 {
		 alert ("You Enter Wrong Number , Try another Time .");
		 return false;
	 }
	 else
	 { return true; }
 }
   </script>
