  <script>
   jQuery(function($){
	$('.select-search-state').change(function(){
		$.post('/actions?_type=_change_state', {id_state:$(this).val()}, function(data){
			location.reload();
		},'json');
	});
	$('.select-search-region').change(function(){
		$.post('/actions?_type=_change_region', {id_region:$(this).val()}, function(data){
			location.reload();
		},'json');
	});
	$('.select-search-suburb').change(function(){
		$.post('/actions?_type=_change_suburb', {id_suburb:$(this).val()}, function(data){
			location.reload();
		},'json');
	});
	
	$('#go_search').click(function(event){
		event.preventDefault();
		if (($('#search_keyword_go').val()).trim() != ""){
			$('#fsearch').submit();
		}
	});
	$('#go_small').click(function(event){
		event.preventDefault();
		if (($('#search_keyword_small').val()).trim() != ""){
			$('#fsearch_small').submit();
		}
	});
});
  </script>
