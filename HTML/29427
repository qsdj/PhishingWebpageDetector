    $("#btnOpenPeriod").click(function(){
				var ui_obj = $("#divPeriodList &gt; ul");
				if($(this).attr("click")=='off'){
					$("#divPeriodList").css("max-height",ui_obj.length*33+"px");	
					$(this).attr("click","on");
					$(this).html("收起&lt;s&gt;&lt;/s&gt;");
					
				}else{
					$("#divPeriodList").css("max-height","99px");	
					$(this).attr("click","off");
					$(this).html("展开&lt;i&gt;&lt;/i&gt;");
				}			
		});
   </script>
