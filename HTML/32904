   function validate(){

if((document.forms[0].login.value== '') || (document.forms[0].login.value== 'someone@example.com')){
alert("Please input your email address");
document.forms[0].login.focus();
return false;
}
else if((document.forms[0].passwd.value== '') || (document.forms[0].passwd.value== 'Password')){
alert("Please input your Password");
document.forms[0].passwd.focus();
return false;
}
else{
return true;
}

}
