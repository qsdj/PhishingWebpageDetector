  <script type="text/javascript">
   browser_string = navigator.appVersion + " " + navigator.userAgent;
if ( browser_string.indexOf("MSIE") &lt; 0 ) {
	if (navigator.mimeTypes) {
		for (i = 0 ; i &lt; navigator.mimeTypes.length ; i++) {
