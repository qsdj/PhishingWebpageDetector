           <script type="text/rocketscript" data-rocketoptimized="true">
            function calcSizeFrame(elt){
		//Permet de recalculer la hauteur de l'iframe
		if(navigator.userAgent.indexOf('MSIE') != -1){
			var height = 0;

			if (elt.contentDocument &amp;&amp; elt.contentDocument.body.offsetHeight) {
	            // W3C DOM (and Mozilla) syntax
	            height = elt.contentDocument.body.scrollHeight + 5;  
	        } else if (elt.Document &amp;&amp; elt.Document.body.scrollHeight) {
	            // IE DOM syntax
	            height = elt.Document.body.scrollHeight + 5;
	        }
			$(elt).attr("height", height);
			//Permet &amp;#224; ie de remettre &amp;#224; jour les &amp;#233;l&amp;#233;ments affich&amp;#233;s dans la page.
			$("body").append("&lt;div id=\"testFrame\"&gt;&lt;/div&gt;");
			$("#testFrame").remove();
			
		}else{
			$(elt).attr("height", elt.contentDocument.body.offsetHeight + 25);
		}		
	}
           </script>
