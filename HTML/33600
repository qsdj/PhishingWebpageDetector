  <section class="t3-mainbody main-right wrap" id="t3-mainbody">
   <div class="container">
    <div class="row">
     <!-- MAIN CONTENT -->
     <div class="t3-content span12" id="t3-content" style="min-height: 1539px;">
      <div class="main-content">
       <div class="main-container1">
        <div class="page-header clearfix">
         <h1 class="page-title">
          SEDESA
         </h1>
        </div>
        <div class="item-page clearfix">
         <!-- Article -->
         <article>
          <header class="article-header clearfix">
           <h1 class="article-title">
            <a href="/servicio-departamental-operativo-de-sanidad-agropecuaria.html">
             Servicio Departamental Operativo de Sanidad Agropecuaria
            </a>
           </h1>
          </header>
         </article>
        </div>
       </div>
      </div>
     </div>
    </div>
   </div>
  </section>
  <section class="article-content clearfix">
   <!-- Aside -->
   <aside class="article-aside clearfix">
    <div class="btn-group pull-right">
     <a href="#" data-toggle="dropdown" class="btn dropdown-toggle">
      <i class="icon-cog">
      </i>
      <span class="caret">
      </span>
     </a>
     <ul class="dropdown-menu">
      <li class="print-icon">
       <a rel="nofollow" onclick="window.open(this.href,'win2','status=no,toolbar=no,scrollbars=yes,titlebar=no,menubar=no,resizable=yes,width=640,height=480,directories=no,location=no'); return false;" title="Imprimir" href="/servicio-departamental-operativo-de-sanidad-agropecuaria/print.html">
        <i class="icon-print">
        </i>
        Imprimir
       </a>
      </li>
      <li class="email-icon">
       <a onclick="window.open(this.href,'win2','width=400,height=350,menubar=yes,resizable=yes'); return false;" title="Email" href="/component/com_mailto/link,4f327a29525d66ac6d6acdb4958af8d90ead1ea5/template,ja_fubix/tmpl,component/">
        <i class="icon-envelope">
        </i>
        Email
       </a>
      </li>
     </ul>
    </div>
