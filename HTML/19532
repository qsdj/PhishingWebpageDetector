     <script type="text/javascript">
      $(document).ready(function() {

	$('.sem-star').click(function(e) {
		$('.sem-star',$(this).parent()).removeClass('active');
		switch ($(this).html()) {
			case '1':
				$('.sem-star-1',$(this).parent()).addClass('active');
				break;
			case '2':
				$('.sem-star-1',$(this).parent()).addClass('active');
				$('.sem-star-2',$(this).parent()).addClass('active');
				break;
			case '3':
				$('.sem-star-1',$(this).parent()).addClass('active');
				$('.sem-star-2',$(this).parent()).addClass('active');
				$('.sem-star-3',$(this).parent()).addClass('active');
				break;
			case '4':
				$('.sem-star-1',$(this).parent()).addClass('active');
				$('.sem-star-2',$(this).parent()).addClass('active');
				$('.sem-star-3',$(this).parent()).addClass('active');
				$('.sem-star-4',$(this).parent()).addClass('active');
				break;
			case '5':
				$('.sem-star-1',$(this).parent()).addClass('active');
				$('.sem-star-2',$(this).parent()).addClass('active');
				$('.sem-star-3',$(this).parent()).addClass('active');
				$('.sem-star-4',$(this).parent()).addClass('active');
				$('.sem-star-5',$(this).parent()).addClass('active');
				break;
		}
		$('.sem-field',$(this).parent()).val($(this).html());
	});

});



$(document).ready(function() {	
	
	$('#sem-form-35').submit(function() {
		
		/* Hide the submit button */
		$('#sem-submit-35-container').hide();
		
		/* Show the submit message */
		$('#sem-submit-35-message').show();
		
		processSixeightForm('sem-form-35');
		
		return false;
	});
	
	function showError(text) {
		$('html,body').animate({
			scrollTop: $('#sem-form-35').offset().top - 50
		},1000);
		$('#sem-error-35').stop().fadeOut().html(text).slideDown().delay(5000).slideUp();
	}
	
	function showMessage(text) {
		$('#sem-message').html(text).slideDown();
	}
	
	function processSixeightForm(bID) {
				var $f = $('#' + bID);
		var formData = $f.serialize();
		$.ajax({
			type: 'POST',
			url: $f.attr('action') + '?js=1',
			dataType: 'json',
			data: formData,
			error: function(XMLHttpRequest,status,errorThrown) {
				showError('There was an error submitting this form.  Please contact the administrator of this site.');
			},
			success: function(response) {
				if(response.hasErrors == '1') {
					 /* Reset submit button */
					$('#sem-submit-35-message').hide();
					$('#sem-submit-35-container').show();
		
					/* Clear error fields */
					$('.sem-field-container').removeClass('sem-error-field');

					switch(response.errorType) {
						case 'permissions':
							showError('You do not have access to submit this form.');
							break;
						case 'captcha':
							$('#sem-captcha-image-1').load('/home/index.php/tools/packages/sixeightforms/refresh_captcha');
							showError('Incorrect validation code.  Please try again.');
							break;
						case 'validation':
							 /* Highlight the error fields */
							$.each(response.errors, function(i,error) {
								$('#sem-field-container-' + error.ffID).addClass('sem-error-field');
								$('.sem-error-field').focus(function() {
									$(this).removeClass('sem-error-field');
								});
							});
							showError('Please review the highlighted fields.');
							break;
					}
				} else if((response.hasCommerce == '1') &amp;&amp; (response.amountCharged &gt; 0)) {
					$('#sem-form-35').fadeOut('fast',function() {
						$.post('/home/index.php/tools/packages/sixeightforms/payment_form', {
								fID: 1,
								asID: response.asID,
								asC: response.asC,
								cc: response.cc,
								cID: 140,
								bID: 35							}, function(data) {
							$('#sem-form-response-35').html(data);
							$('#sem-form-response-35').fadeIn('fast',function() {
								$('html,body').animate({
									scrollTop: $('#sem-form-response-35').offset().top - 50
								},1000);
							});
						});
					});
				} else {
					if(response.hook != '') {
						$.ajax({
							type: 'GET',
							url: CCM_TOOLS_PATH + '/sixeightforms/' + response.hook + '?asID=' + response.asID,
							success: function() {
								switch(response.action) {
									case 'thankyou':
										$('#sem-form-35').fadeOut('fast',function() {
																							$('#sem-form-response-35').html(response.response);
																						$('#sem-form-response-35').fadeIn('fast');
																					});
										break;
									case 'redirect':
										window.location = response.response;
										break;
									case 'url':
										window.location = response.response;
										break;
								}
							}
						});
					} else {
						switch(response.action) {
							case 'thankyou':
								$('#sem-form-35').fadeOut('fast',function() {
																			$('#sem-form-response-35').html(response.response);
																		$('#sem-form-response-35').fadeIn('fast');
																	});
								break;
							case 'redirect':
								window.location = response.response;
								break;
							case 'url':
								window.location = response.response;
								break;
						}
					}
				}
			}
		});
	}
	
	var uploadsInProgress = 0;
	
		
	$('.sem-next-button').click(function(e) {
		e.preventDefault();
		var $section = $(this).parent().parent();
		
		if($(this).hasClass('validate-section')) {
			var formData = $('#sem-form-35').serialize();
			var buttonID = $(this).attr('name');
			$.ajax({
				type: 'POST',
				url: '/home/index.php/tools/packages/sixeightforms/validate_fields?fID=1&amp;sectionID=' + buttonID,
				dataType: 'json',
				data: formData,
				success: function(response) {
					if(response.hasErrors == '1') {
						/* Reset submit button */
						$('#sem-submit-35-message').hide();
						$('#sem-submit-35-container').show();
			
						/* Clear error fields */
						$('.sem-field-container').removeClass('sem-error-field');
						
						/* Highlight the error fields */
						$.each(response.errors, function(i,error) {
							$('#sem-field-container-' + error.ffID).addClass('sem-error-field');
							$('.sem-error-field').focus(function() {
								$(this).removeClass('sem-error-field');
							});
						});
						showError('Please review the highlighted fields.');
					} else {
						$section.fadeOut('fast',function() {
							$section.next().fadeIn();
						});
					}
				}
			});
		}
	});
	
	$('.sem-previous-button').click(function(e) {
		e.preventDefault();
		var $section = $(this).parent().parent();
		$section.fadeOut('fast',function() {
			$section.prev().fadeIn();
		});
	});
	
		
		
	
			
	$('.sem-checkbox').change(function(e) {
		if($(this).is(':checked')) {
			$(this).parent().addClass('active');
		} else {
			$(this).parent().removeClass('active');
		}
	});
	
	$('.sem-radio-button').click(function(e) {
		var id = $(this).attr('name');
		$('label','#sem-fieldset-' + id).removeClass('active');
		$(this).parent().addClass('active');
	});
});
     </script>
