		if (somepath == '/templates/bee-turbo-dark/textures/07.png') { slavename.value = p7; }
		if (somepath == '/templates/bee-turbo-dark/textures/08.png') { slavename.value = p8; }
		if (somepath == '/templates/bee-turbo-dark/textures/09.png') { slavename.value = p9; }
		
		
		if (somepath == '/templates/bee-turbo-dark/textures/20.jpg') { slavename.value = p20; }
		if (somepath == '/templates/bee-turbo-dark/textures/21.jpg') { slavename.value = p21; }
		if (somepath == '/templates/bee-turbo-dark/textures/22.jpg') { slavename.value = p22; }
		if (somepath == '/templates/bee-turbo-dark/textures/23.jpg') { slavename.value = p23; }
		if (somepath == '/templates/bee-turbo-dark/textures/24.jpg') { slavename.value = p24; }
		if (somepath == '/templates/bee-turbo-dark/textures/25.jpg') { slavename.value = p25; }
		if (somepath == '/templates/bee-turbo-dark/textures/26.jpg') { slavename.value = p26; }
		if (somepath == '/templates/bee-turbo-dark/textures/27.jpg') { slavename.value = p27; }
		if (somepath == '/templates/bee-turbo-dark/textures/28.jpg') { slavename.value = p28; }
