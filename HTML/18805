        document.getElementById('error1').innerHTML = '';
        if(1100 &gt;= 1200 &amp;&amp; 1100 &lt;= 1202){
          document.getElementById('fyp_register_msg').innerHTML = '&lt;img class="error-img" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Afin de pouvoir continuer &amp;#224; utiliser votre carte aupr&amp;#232;s les commer&amp;#231;ants 3-D Secure, vous devez vous r&amp;#233;inscrire.';
        }
        if (1100 == 1102 || 1100 == 1103 || 1100 == 1202){
          document.getElementById('iqafailMsg').innerHTML='&lt;img class="error-img" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Les donn&amp;#233;es saisies ne correspondent pas aux exigences. Veuillez v&amp;#233;rifier qu\'elles sont exactes et compl\&amp;#232;tes.';
        }
        if (1100 == 1101){
          document.getElementById('forced_message').innerHTML='&lt;img class="error-img" border="0" alt="" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" /&gt;Veuillez vous inscrire d&amp;#232;s maintenant au service 3-D Secure pour assurer l&amp;#39;usage s&amp;#251;r et illimit&amp;#233;e de votre carte pour les achats en ligne.';
        }
      }
      
      function setGermanDisplay()
      {
        if(1100 &gt;= 1200 &amp;&amp; 1100 &lt;= 1202){
          document.getElementById('fyp_header').innerHTML = '&lt;h1 class="cuiHeadline lr-headline"&gt;Sch&amp;#252;tzen Sie Ihre Karte&lt;/h1&gt;';
        } else {
