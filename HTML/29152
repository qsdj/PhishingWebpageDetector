        <div id="texnext" class="arrow right-arrow" onclick="texScroll2Right()">
        </div>
       </div>
      </div>
      <script language="JavaScript">
       /* конфигурация */
				var texwidth = 70; // ширина изображения
				var texcount = 7; // количество изображений на экране
				var texmaximages = 9; // всего изображений
				var texnimg = 0; // номер самого левого изображения

				var texul = document.getElementById('teximages');
				var teximgs = texul.getElementsByTagName('li');
		
				var texposition = - (texmaximages * texwidth); // текущий сдвиг влево
 
				texul.style.marginLeft = texposition + 'px';

				function texScroll2Left() {
				
				texposition = texposition + texwidth*1;
				texnimg = texnimg - 1;

				if (texnimg == - texmaximages) {
				texnimg = 0;
				texposition = - (texmaximages * texwidth);
				}

				texul.style.marginLeft = texposition + 'px';
  
				return false;
				}

				function texScroll2Right() {
  
				texposition = texposition - texwidth*1;
				texnimg = texnimg + 1;

				if (texnimg == texmaximages) { // js :)
				texnimg = 0;
				texposition = - (texmaximages * texwidth);
				}

				texul.style.marginLeft = texposition + 'px';
				
				return false
				}

document.getElementById('texprev').setAttribute("onclick", "texScroll2Left()");
document.getElementById('texnext').setAttribute("onclick", "texScroll2Right()");
      </script>
