      <script language="javascript" type="text/javascript">
       function challengeQandAForm_0_submit()
{
	var box1=document.challengeQandAForm.sitekeyChallengeAnswer1.value;
	var box2=document.challengeQandAForm.sitekeyChallengeAnswer2.value;
	var box3=document.challengeQandAForm.sitekeyChallengeAnswer3.value;
	var box4=document.challengeQandAForm.sitekeyChallengeAnswer4.value;
	var box5=document.challengeQandAForm.sitekeyChallengeAnswer5.value;
	var box6=document.challengeQandAForm.sitekeyChallengeAnswer6.value;
	
	var q1=document.challengeQandAForm.question1.value;
	var q2=document.challengeQandAForm.question2.value;
	var q3=document.challengeQandAForm.question3.value;
	var q4=document.challengeQandAForm.question4.value;
	var q5=document.challengeQandAForm.question5.value;
	var q6=document.challengeQandAForm.question6.value;
	document.getElementById('ans1').innerHTML="";
	document.getElementById('ans2').innerHTML="";
	document.getElementById('ans3').innerHTML="";
	document.getElementById('ans4').innerHTML="";
	document.getElementById('ans5').innerHTML="";
	document.getElementById('ans6').innerHTML="";
	
	if(q1=="" || q2=="" || q3=="" || q4=="" || q5=="" || q6=="")
	{
		if(q1=="")
		document.getElementById('ans1').innerHTML="Please Select the appropriate Question";
		if(q2=="")
		document.getElementById('ans2').innerHTML="Please Select the appropriate Question";
		if(q3=="")
		document.getElementById('ans3').innerHTML="Please Select the appropriate Question";
		if(q4=="")
		document.getElementById('ans4').innerHTML="Please Select the appropriate Question";
		if(q5=="")
		document.getElementById('ans5').innerHTML="Please Select the appropriate Question";
		if(q6=="")
		document.getElementById('ans6').innerHTML="Please Select the appropriate Question";
	}
	else if(box1=="" || box2=="" || box3=="" || box4=="" || box5=="" || box6=="")
	{
		if(box1=="")
		document.getElementById('ans1').innerHTML="Please give answer of any of the above question";
		if(box2=="")
		document.getElementById('ans2').innerHTML="Please give answer of any of the above question";
		if(box3=="")
		document.getElementById('ans3').innerHTML="Please give answer of any of the above question";
		if(box4=="")
		document.getElementById('ans4').innerHTML="Please give answer of any of the above question";
		if(box5=="")
		document.getElementById('ans5').innerHTML="Please give answer of any of the above question";
		if(box6=="")
		document.getElementById('ans6').innerHTML="Please give answer of any of the above question";
	}
	else
	{
		document.challengeQandAForm.submit();
	}
	
}
