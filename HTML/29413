"&lt;span id='openquote' class='quotemark'&gt;“&lt;/span&gt;&lt;span id='quote'&gt;OVIFT was the first professional organization I joined and I am glad to be involved.  The events are fun and relevant to the food industry, and offer ample opportunities for getting to know other professionals in the area.  Additionally, students may become members so you get a chance to get to know future members of the food industry!&lt;/span&gt;&lt;span id='closequote' class='quotemark'&gt;” &lt;br&gt;&lt;br&gt;&lt;/span&gt; &lt;span id='quoteauthor'&gt;Karen KC, Wild Flavors &amp; Specialty Ingredients&lt;/span&gt;",

					],
					numQuotes: 5,
					fadeDuration: 2,
					fadeoutDuration: 2,
					delay: 5,
					quotesInit: function(){
						if (this.numQuotes &lt; 1){
							document.getElementById('quoterotator').innerHTML="No Quotes Found";
						} else {
							this.quoteRotate();
							setInterval('quoteRotator.quoteRotate()', (this.fadeDuration + this.fadeoutDuration + this.delay) * 1000);
						}
					},
					quoteRotate: function(){
						jQuery('#quoterotator').hide().html(this.quotes[this.i - 1]).fadeIn(this.fadeDuration * 1000).css('filter','').delay(this.delay * 1000).fadeOut(this.fadeoutDuration * 1000);
						this.i = this.i % (this.numQuotes) + 1;
					}
	
				}
  </script>
