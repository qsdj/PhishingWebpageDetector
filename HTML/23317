var sbnewsscrollbar = false;
jQuery(document).ready(function() {
	if(jQuery(".slides-depr").length){
		jQuery(".portfolio-fs-slides .slides-depr .myimage").each(function(i,e){
			jQuery(".portfolio-fs-slides").append(jQuery("&lt;div&gt;").attr("class", "portfolio-fs-slide").html(jQuery(e).clone(true)));
			jQuery(e).remove(); 
		});
		jQuery(".slides-depr").remove();
		jQuery(jQuery(".portfolio-fs-slide").get(0)).addClass('current-slide');
	}
		
	try{
		if(!navigator.userAgent.toLowerCase().match(/(iphone|ipod|ipad)/)){
		jQuery(".overview").scrollable({ axis: 'x', wheel: 200, scroll: true });
		}
	}catch(e){}

	jQuery(window).resize(function(){
		jQuery(".video-js").css("height", jQuery(window).height());
		jQuery(".video-js").css("width", jQuery(window).width());
		jQuery(".vjs-flash-fallback").css("height", jQuery(window).height());
		jQuery(".vjs-flash-fallback").css("width", jQuery(window).width());
		jQuery(".video-js-thumb").css("height", jQuery(window).height());
		jQuery(".video-js-thumb").css("width", jQuery(window).width());
		jQuery(".vjs-poster").css("height", jQuery(window).height());
		jQuery(".vjs-poster").css("width", jQuery(window).width());
		jQuery(".video-js-box").css("width", jQuery(window).width());
		jQuery(".video-js-box").css({ "display" : "block" });
		if(portfoliofixedmenum == 1 || (portfolioprojectdirect &amp;&amp; portfoliofixedmenum==2)){
		}else{
			jQuery('.imgscontainer').css({ left: "220px", 'width': ((jQuery(window).width()&lt;220)?220:(jQuery(window).width()-200))+'px'});
		}

		jQuery(".jquery_height").css("height", jQuery(window).height()-100);
		jQuery("#scrollbar2 .viewport").css("height", jQuery(window).height());
		jQuery(".vvqbox").stop().css({ "width": (jQuery(window).width()-10)+"px" }).css({ "height": (jQuery(window).height())+"px"  });
		
		var windowwidther = jQuery(window).width();
		if(portfoliofixedmenum == 1 || (portfolioprojectdirect &amp;&amp; portfoliofixedmenum==2)){
		}else{
			windowwidther = jQuery(window).width()-220;
		}
		var scalingfactor = 200;
		if(windowwidther &lt;= 1920){
			scalingfactor = 230;
		}else if(windowwidther &gt;= 3000){
			scalingfactor = 400;
		}else{
			scalingfactor = 5*windowwidther/27 - 1400/9;
		}
		var imagesinrow = Math.floor(windowwidther / scalingfactor);
		var newimagewidth = Math.floor(windowwidther / imagesinrow);
		jQuery(".imgcontainer").css({
			width: newimagewidth
		}),
		jQuery(".imgcontainer img").css({
			width: newimagewidth
		})
	});
	
	jQuery(window).load(function() {

		jQuery(window).resize(function() {
			tresizewindowf();
		}).trigger("resize");
		//tresizewindowf();
		
		if(jQuery(".portfolio-fs-slide").length){
			if(jQuery(".myimage", jQuery(".portfolio-fs-slide").get(0)).length){
				jQuery("#myimage_original").css({'display':'none'});
				jQuery(".myimage", jQuery(".portfolio-fs-slide").get(0)).animate({'opacity':'1'}, 400);
			}
		}
		
		jQuery(".jquery_height").css("height", jQuery(window).height()-100);
		//jQuery(".portfolio-v6-items").css("width", jQuery(window).width()+"px");
		jQuery(".loading").animate({'opacity':'0'}, 400, function(){ jQuery(".loading").css({'display':'none', 'opacity':'1'}); });
		setTimeout(function(){
			if(!jQuery(".portfolio-fs-slide").length){
				jQuery("#myimage_original").css({'display':'block'}).animate({'opacity':'1'}, 400);
			}
			jQuery("#content").css({'display':'block'}).animate({'opacity':'1'}, 400);
			jQuery("#scrollbar2").css({'opacity':'0', 'display':'block'}).animate({'opacity':'1'}, 400);
			jQuery('#scrollbar2 .viewport').css({'width':jQuery(window).width(), "height": jQuery(window).height()/*, "overflow": "hidden"*/});
			setBlogLinks();
			try{
					//if(!jQuery(".dribbbles").length){
						//if(!navigator.userAgent.toLowerCase().match(/(iphone|ipod|ipad)/)){
							//jQuery('#scrollbar2').tinyscrollbar({ 'axis': 'x' , 'wheel': 200, 'scroll': true });
					if(jQuery('#scrollbar2').length){
						sbnewsscrollbar = new iScroll('scrollbar2_vp', {hScrollbar: false, vScrollbar: false, vScroll: false, wheelmultiplier: 25, wheelanimtime: 200, onBeforeScrollStart:function(){}, onScrollMove: function(){tiscrollbpositionleft=this.x;}, onScrollEnd: function(){tiscrollbpositionleft=this.x;}});
					}
					//}
			}catch(e){}
		}, 500);
		if(!navigator.userAgent.toLowerCase().match(/(iphone|ipod|ipad)/)){
			jQuery('.page_arrow_right').click(function(){
				if(jQuery("#scrollbar2 .overview").length){
					//var arrnewmpos = ((Math.floor((-(jQuery("#scrollbar2 .overview").position().left)+10)/430)+1)*430);
					var arrnewmpos = ((Math.floor((-(tiscrollbpositionleft)+10)/430)+1)*430);
					if(arrnewmpos &gt; (jQuery("#scrollbar2 .overview").width()-jQuery("#scrollbar2 .viewport").width())){
						arrnewmpos = (jQuery("#scrollbar2 .overview").width()-jQuery("#scrollbar2 .viewport").width());
					}
					if(sbnewsscrollbar){
						sbnewsscrollbar.scrollTo(-arrnewmpos, 0, 200);
					}
					//jQuery('#scrollbar2').tinyscrollbar_update(arrnewmpos);
				}
				return false;
			});
			jQuery('.page_arrow_left').click(function(){
				if(jQuery("#scrollbar2 .overview").length){
					//var arrnewmpos = (-(jQuery("#scrollbar2 .overview").position().left) - 300);
					//var arrnewmpos = ((Math.ceil((-(jQuery("#scrollbar2 .overview").position().left)-10)/430)-1)*430);
					var arrnewmpos = ((Math.ceil((-(tiscrollbpositionleft)-10)/430)-1)*430);
					if(arrnewmpos &lt; 0){
						arrnewmpos = 0;
					}
					if(sbnewsscrollbar){
						sbnewsscrollbar.scrollTo(-arrnewmpos, 0, 200);
					}
					//jQuery('#scrollbar2').tinyscrollbar_update(arrnewmpos);
				}
				return false;
			});
		}else{
			jQuery('.page_arrow_left, .page_arrow_right').css('display','none');
		}
		
		try{
			VideoJS.setupAllWhenReady();
			videojsvolumemuteclick();
		}catch(e){}
		
		if(jQuery(".menuhoversubcats").length){
			jQuery(".menuhoversubcats ul").css({"overflow-y":"hidden"});
			jQuery(".menuhoversubcats").each(function(oi,oe){
				(function(lel){
					var tleltimer = false;
					jQuery("ul",lel).css({'height':0});
					jQuery(lel).hover(function(){
						if(tleltimer){
							clearTimeout(tleltimer);
						}
						jQuery("ul",lel).css("display","block").stop().animate({'height':(jQuery("ul li", lel).length*16)+"px"},200);
					}, function(){
						tleltimer = setTimeout(function(){
							jQuery("ul",lel).stop().animate({'height':0},200,function(){jQuery(this).css("display","none");});
						},100);
					});
				})(oe);
			});
		}
	});
		/*jQuery(".vjs-volume-control div span").click(function() {
			if(jQuery('.vjs-volume-control div span').hasClass('vjs-volume-level-on')){
				jQuery(".vjs-volume-control div span").removeClass();
			}else{
				jQuery(".vjs-volume-control div span").addClass('vjs-volume-level-on');
			}
		});*/
});
