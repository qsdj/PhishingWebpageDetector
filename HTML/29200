  <script>
   //&lt;![CDATA[

        /* Foresee Wrapper Code To Prevent Foresee/Userzoom code from both firing for the same person */
        /* The code makes use of a cookie (survey_cx) and a JS variable (survey_cx) to ensure that Foresee code will not conflict with UserZoom code when a page has both sets of code running.
         Note that userzoom code will only be enabled on a page by page basis and will not be deployed in the footer like Foresee.
         */

        var surveycookie = $.cookie("survey_cx"); // grab survey_cx cookie
        if (surveycookie == null) { // only proceed if it doesn't exist
            if (typeof survey_cx == "undefined" || survey_cx != true) { // even if the cookie doesn't exist, if the survey_cx variable is set to true elsewhere, we won't proceed
                // foresee code ends here
                if(isDesktop())
                {
                
                    var hostName = URLParser.protocol + '//' + URLParser.host;

                    if(URLParser.host == "www.adobe.com" || URLParser.host == "www.stage.adobe.com"){
