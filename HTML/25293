function initialize() {


  var mapOptions = {
	backgroundColor: '#eaf0f6',
    zoom: 5,
    center: new google.maps.LatLng(54.904861, -120.927089),
    mapTypeId:google.maps.MapTypeId.ROADMAP,
	mapTypeControl: true
  }
  var map = new google.maps.Map(document.getElementById("map"), mapOptions);
  map.setOptions({styles: StandardStyles});
  setMarkers(map);
}

function loadScript() {
  var script = document.createElement("script");
  script.type = "text/javascript";
  script.src = "http://maps.google.com/maps/api/js?sensor=false&amp;callback=initialize";
  document.body.appendChild(script);
}

window.onload = loadScript;

function showDiv(d){
	divs = new Array("corp-offices", "airport-locations", "fuel-dealers");
	for(i = 0; i &lt; divs.length; i++){
		document.getElementById(divs[i]).style.display = "none";
	}
	document.getElementById(d).style.display = "block";
}
function rowOver(id){
	document.getElementById("title" + id).style.backgroundColor = "#004474";
	document.getElementById("address" + id).style.backgroundColor = "#dadbdd";
}
function rowOut(id){
	document.getElementById("title" + id).style.backgroundColor = "#0067a9";
	document.getElementById("address" + id).style.backgroundColor = "#eaebed";
}
function goto(id){
	url="index.php?pid=" + id;
	window.open(url,"_self")
}
