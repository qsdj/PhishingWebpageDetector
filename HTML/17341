  <script language="JavaScript">
   function MontaEmail()
{
    if ((document.formEmail.login.value == "infocin") || 
       (document.formEmail.login.value == "ricardo") ||
       (document.formEmail.login.value == "gilberto") || 
       (document.formEmail.login.value == "emerson")
       (document.formEmail.login.value == "nataly")
       (document.formEmail.login.value == "cleiton"))
    {
	   
       var v_email;
	   v_email = document.formEmail.login.value;
       v_email = v_email + "@infocin.com.br";
	   document.formEmail.login.value = v_email;
    }
}
  </script>
