  <script type="text/javascript">
   jQuery(function($) {

				jQuery( '.dropdown_product_cat' ).change( function() {
					if ( jQuery(this).val() != '' ) {
						var this_page = '';
						var home_url  = 'http://enyasophya.com/';
						if ( home_url.indexOf( '?' ) &gt; 0 ) {
							this_page = home_url + '&amp;product_cat=' + jQuery(this).val();
						} else {
							this_page = home_url + '?product_cat=' + jQuery(this).val();
						}
						location.href = this_page;
					}
				});
			
});
  </script>
