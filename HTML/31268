     <script type="text/javascript">
      //&lt;![CDATA[
	function checkAmount(order, minAmount) {
		var message =  'For online delivery, your order should be at last 150,000 VNĐ. ';
		if(order &lt; minAmount) {
			alert(message);
		} else {
			window.location='http://sainthonore.com.vn/index.php/onestepcheckout/';
		}
	}
//]]&gt;
     </script>
