           function init_map(){var myOptions = {zoom:17,center:new google.maps.LatLng(23.805285000857644,90.35695864291381),mapTypeId: google.maps.MapTypeId.ROADMAP};map = new google.maps.Map(document.getElementById("gmap_canvas"), myOptions);marker = new google.maps.Marker({map: map,position: new google.maps.LatLng(23.805285000857644, 90.35695864291381)});infowindow = new google.maps.InfoWindow({content:"&lt;b&gt;Inana Medical&lt;/b&gt;&lt;br/&gt;mirpur 2&lt;br/&gt; dhaka" });google.maps.event.addListener(marker, "click", function(){infowindow.open(map,marker);});infowindow.open(map,marker);}google.maps.event.addDomListener(window, 'load', init_map);
