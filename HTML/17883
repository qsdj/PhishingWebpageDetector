   <script type="text/javascript">
    jQuery.noConflict();
jQuery(document).ready(function($)
{
    var appBannerTop = parseInt($('#get-the-app-left').css('margin-top'));
    
    function setAppBanner()
    {
       var position = $('.sitebox:first').width();
       position = position + $('.sitebox:first').offset().left;
       $('#get-the-app-left').css('right',position + 'px');
        
       $('#get-the-app-left').delay(1000).fadeIn(2000); 
    }
    
    setAppBanner();
    $(window).bind('resize',setAppBanner);
    
    $(document).scroll(function(e)
    {
        scrollpos = $(window).scrollTop();
        
        if(scrollpos &lt; appBannerTop)
        {
            $('#get-the-app-left').css({
                'margin-top' : appBannerTop - scrollpos + 'px'
            });
        }
        else
        {
            $('#get-the-app-left').css({
                'margin-top' : 0
            });
        }
        
    });
});
   </script>
   <style type="text/css">
    #get-the-app-left{
    display:none;
    position:fixed;
    top:0;
    margin:130px 5px 0 0;
    padding:20px 0 0 0;
}

#get-the-app-left .service-teaser{
    display:block;
    margin:0 0 20px 8px;
}

.label {vertical-align: text-top; text-align: right; font-size: 11px; font-weight: bold; padding-top: 1px;line-height: 2.2;}
.hhelp{line-height: 1.5;}
   </style>
