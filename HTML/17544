         <script type="text/javascript">
          //&lt;![CDATA[
		jQuery(document).ready(function ($) {
			$('#sj_adsm_carousel_20843481531448459531').each(function(index, element) {
				$(this)[index].slide = null;
			});
			;(function(el){
				var $element = $(el);
				$element.each(function(){
					var $this = $(this), options = options = !$this.data('modal') &amp;&amp; $.extend({}, $this.data());
					$this.jsmartcarousel(options);
					
					$this.bind('jsmartslide', function (e) {
						var index = $(this).find(e.relatedTarget).index();
						
						$('[data-jsmartslide]').each(function () {
							var $nav = $(this), $navData = $nav.data(), href, $target = $($nav.attr('data-target') || (href = $nav.attr('href')) &amp;&amp; href.replace(/.*(?=#[^\s]+$)/, ''));
							if (!$target.is($this)) return;
							if (typeof $navData.jsmartslide == 'number' &amp;&amp; $navData.jsmartslide == index) {
								$nav.addClass('sel');
							} else {
								$nav.removeClass('sel');
							}
							setSlidingTo($element);
						});
					});
										$this.touchSwipeLeft(function () {
							$this.jsmartcarousel('next');
						}
					);
					$this.touchSwipeRight(function () {
							$this.jsmartcarousel('prev');
						}
					);
									});
				
				var timer;
				function firstLoad(){
					if(timer)  clearTimeout(timer);
					timer = setTimeout(function(){
						var  $loading = $('.adssl-loading',$element);
						$('.adssl-controls-wrap',$element).css('overflow','visible');
						$loading.remove();
						$element.removeClass('adssl-preload');
						$element.css('height',$('.adssl-inner &gt; .item',$element).height());
						setSlidingTo($element);
					},1000);
				}
				$(window).load(function(){
					firstLoad();
				});
				
				$(window).resize(function(){
					$element.css('height',$('.adssl-inner &gt; .item',$element).height());
					setSlidingTo($element);
				});
				
				function setSlidingTo( $element){
					var tabs = $('.ctr-page',$element),
						_index = tabs.filter('.sel').index(),
						num_thumb = 3;
					var tabs_holder = tabs.eq(0).parent();
					var tabs_container = tabs_holder.parent();
					var $item = $('.adssl-inner',$element),
						$height_ctr_wrap = tabs.filter('.sel').parents('.adssl-controls-wrap',$element).height(),
						$height_ctr_page = $height_ctr_wrap / num_thumb	;
					if (tabs_container &amp;&amp; tabs_holder){
						tabs_container.css({height: function(){
							return $height_ctr_wrap
						}});
						tabs_container.css({position: 'relative'});
						tabs_holder.css({position: 'relative'});
					}
					$('.ctr-page-inner',tabs).css('height',$height_ctr_page-18);
					tabs.css('height',$height_ctr_page);
					var viewport   = {};
					viewport.top  = 0;
					viewport.bottom = tabs_container.height() ;
					
					var visible   = {};
					visible.top  = viewport.top  - parseInt( tabs_holder.position().top ) ;
					visible.bottom = viewport.bottom - parseInt( tabs_holder.position().top ) ;
				
					var posUpdate = function(){
						visible.top  = viewport.top  - parseInt( tabs_holder.position().top )  ;
						visible.bottom = viewport.bottom - parseInt( tabs_holder.position().top ) ;
						
					};
					
					if (_index &gt;= 0){
						if (_index == 0){
							_index = 1;
						}
						var prevTop = tabs.eq(_index-1).position().top;
						if (prevTop &lt; visible.top ){
							tabs_holder.stop().animate({
								top: '+='+(visible.top-prevTop)+'px'
							}, {
								duration: 500,
								complete: posUpdate,
								queue:false 
							});
						}
					}
					 if (_index &lt; tabs.length){
						if (_index == tabs.length-1){
							_index = tabs.length-2;
						}
						var nextBottom = tabs.eq(_index+1).position().top + tabs.eq(_index+1).height() ;
						if (nextBottom &gt; visible.bottom || (_index !=1 &amp;&amp; _index == tabs.length-2 )  ){
							tabs_holder.stop().animate({
								top: '-='+(nextBottom-visible.bottom)+'px'
							}, {
								duration: 500,
								complete: posUpdate,
								queue:false 
							});
						}
					}
				}
				
			})('#sj_adsm_carousel_20843481531448459531')
		});
		//]]&gt;
         </script>
        </div>
