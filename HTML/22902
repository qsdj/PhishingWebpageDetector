  <script type="text/javascript">
   var domain = base_domain();
        var options1 = {
			script: domain + "/suburbpostcode/",
			//script: "http://localhost/Classiquestone/developer2014/suburbpostcode/",
            varname:"getSuburbPostCode",
            json:true,
            callback: function (obj) {
				//var miles = document.getElementById("miles").value;
				//miles = miles * 1609.344;
				//generateMap(obj.id,obj.lat,obj.lon,miles);
				document.getElementById('suburb_state_postcode_id').value = obj.id;
			}
        };
        var as_json = new AutoSuggest('suburb_state_postcode', 'country_id', 'loading', options1);

		function base_domain()
		{
			// capturamos la url
			url_site = document.location.href
			url_pos = url_site.indexOf('//');
			
			// por un lado tenemos la url sin protocolos
			url_limpia = url_site.substr(url_pos+2);
			
			// y por otro lado el protocolo usado
			url_prot = url_site.substr(0,url_pos+2);
			
			// separamos todas las posibles carpetas
			url_split = url_limpia.split('/');
			
			// y obtenemos el dominio actual
			url_base = url_prot + url_split[0];
			
			return url_base;
		}
  </script>
  <script type="text/javascript">
   function getAddressCustomer(id)
{
	var addressData = new Array(); 
	
	addressData[0] = {type:"Residence", contact:"", company:"", email:"", phone:"", address:"", suburb_state_postcode:"", suburb_state_postcode_id:"", country_id:""};
	
	
		
	if(id=="")
	{
		var countryCtrl = document.getElementById("country_id");
	    var countElements = countryCtrl.length;
	    for (i = 0; i &lt; countElements; i++) {
		  if (countryCtrl[i].value == "") {
			 countryCtrl[i].selected = true;
		  }   
	    }
		
		var pbCtrl = document.getElementById("destinationResPostalBox");
		for (i = 0; i &lt; pbCtrl.length; i++) {
		  if (pbCtrl[i].value == "N") {
			 pbCtrl[i].selected = true;
		  }   
	    }
		
		var uCtrl = document.getElementById("destinationResUnattended");
		for (i = 0; i &lt; uCtrl.length; i++) {
		  if (uCtrl[i].value == "N") {
			 uCtrl[i].selected = true;
		  }   
	    }
		
		var iCtrl = document.getElementById("destinationResInside");
		for (i = 0; i &lt; iCtrl.length; i++) {
		  if (iCtrl[i].value == "N") {
			 iCtrl[i].selected = true;
		  }   
	    }
		
		var lACtrl = document.getElementById("destinationResLimitedAccess");
		for (i = 0; i &lt; lACtrl.length; i++) {
		  if (lACtrl[i].value == "N") {
			 lACtrl[i].selected = true;
		  }   
	    }
		
		document.getElementById("Business").checked=false;
		document.getElementById("Residence").checked=true;
		document.getElementById("contact").value="";
		document.getElementById("company").value="";
		document.getElementById("email").value="";
		document.getElementById("phone").value="";
		document.getElementById("address").value="";
		document.getElementById("suburb_state_postcode").value="";
		document.getElementById("suburb_state_postcode_id").value=0;
		
		document.getElementById("divNewAddress").className="";
	}
	else
	{
			if(addressData[id].type=="Business")
			{
				document.getElementById("Business").checked=true;
				document.getElementById("Residence").checked=false;
			}
			else
			{
				if(addressData[id].type=="Residence" || addressData[id].type=="")
				{
					document.getElementById("Business").checked=false;
					document.getElementById("Residence").checked=true;
				}
			}
		document.getElementById("contact").value=addressData[id].contact;
		document.getElementById("company").value=addressData[id].company;
		document.getElementById("email").value=addressData[id].email;
		document.getElementById("phone").value=addressData[id].phone;
		document.getElementById("address").value=addressData[id].address;
		document.getElementById("suburb_state_postcode").value=addressData[id].suburb_state_postcode;
		document.getElementById("suburb_state_postcode_id").value=addressData[id].suburb_state_postcode_id;
		
		var countryCtrl = document.getElementById("country_id");
	    var countElements = countryCtrl.length;
	    for (i = 0; i &lt; countElements; i++) {
		  if (countryCtrl[i].value == addressData[id].country_id) {
			 countryCtrl[i].selected = true;
		  }   
	    }
		
		var pbCtrl = document.getElementById("destinationResPostalBox");
		for (i = 0; i &lt; pbCtrl.length; i++) {
		  if (pbCtrl[i].value == addressData[id].destinationResPostalBox) {
			 pbCtrl[i].selected = true;
		  }   
	    }
		
		var uCtrl = document.getElementById("destinationResUnattended");
		for (i = 0; i &lt; uCtrl.length; i++) {
		  if (uCtrl[i].value == addressData[id].destinationResUnattended) {
			 uCtrl[i].selected = true;
		  }   
	    }
		
		var iCtrl = document.getElementById("destinationResInside");
		for (i = 0; i &lt; iCtrl.length; i++) {
		  if (iCtrl[i].value == addressData[id].destinationResInside) {
			 iCtrl[i].selected = true;
		  }   
	    }
		
		var lACtrl = document.getElementById("destinationResLimitedAccess");
		for (i = 0; i &lt; lACtrl.length; i++) {
		  if (lACtrl[i].value == addressData[id].destinationResLimitedAccess) {
			 lACtrl[i].selected = true;
		  }   
	    }
		
		document.getElementById("divNewAddress").className="hidden";
	}

}

	function change_quantity()
	{
		$.post('/actions?_type=_cart_update', $('#form_cart_products').serialize(), function(){
			location.reload();
		},'json');	
	}
  </script>
  <script type="text/javascript">
   jQuery(function($) {
	$.post('/shipping-calculate-require', $('#form_cart').serialize(), function(data){
		if (data.result) {
			$('#DivShippingCalculate').removeClass("hidden");
		}
		else {
			$('#DivShippingCalculate').addClass("hidden");
		}
	}, 'json');

	$('#myCarousel').carousel();
	$('.btn_quit').click(function(e) {
		e.preventDefault();
		$.post('/quitCart', {item:$(this).data('id')}, function(res){
			location.reload();
		},'json');
	});	
	$('#cart_update').click(function(e){
		e.preventDefault();
		$.post('/actions?_type=_cart_update', $('#form_cart').serialize(), function(res){
			location.reload();
		},'json');	
	});
	$.box1;
	$('#cart_login').click(function(e){
		e.preventDefault();
		$.post('/popup-login/view1', {}, function(data){
			$.box1 = bootbox.dialog({message: data}).addClass("modal-popup-login");
		});
	});
	
	$.fn.forgotten = function() {
		$.post('/popup-forgotten/view1', {}, function(data){
			$.box1 = bootbox.dialog({message: data}).addClass("modal-popup-login");
		});
	}
	
	$('#calculate_shipping').click(function(e) {
		$('#calculate_shipping').addClass("hidden");
		$('#img_loading_calculate').removeClass("hidden");
		e.preventDefault();
		$.post('/shipping-calculate', $('#form_cart').serialize(), function(data){
			if (data.result == 'ok') {
				location.reload();
				//$('.info-shipping-calculate').html(data.message);
			}
			else {
				$('#img_loading_calculate').addClass("hidden");
				$('#calculate_shipping').removeClass("hidden");
				$('.info-shipping-calculate').html(data.message);
			}
		}, 'json');
	});
	
	$('#apply_discount_code').click(function(e) {
	if(document.getElementById("gift_voucher").value!="" &amp;&amp; document.getElementById("discount_code").value!="")
	{
		alert("Please enter gift voucher or discount (Only one).");
	}
	else
	{
		if(document.getElementById("gift_voucher").value!="" || document.getElementById("discount_code").value!="")
		{
			$('#apply_discount_code').addClass("hidden");
			$('#img_loading_vouchers').removeClass("hidden");
			e.preventDefault();
			$.post('/apply-discount-vouchers', $('#form_discount_voucher').serialize(), function(data){
				if (data['exist']) {
					location.reload();
					//$('.info-shipping-calculate').html(data.message);
				}
				else {
					$('#img_loading_vouchers').addClass("hidden");
					$('#apply_discount_code').removeClass("hidden");
					$('.info-discount-voucher').html(data.message);
				}
			}, 'json');
		}
		else
		{
			alert("Please enter gift voucher or discount (Only one).");
		}
	}	

	});
	
	$('#save_delivery_address').click(function(e) {		
		e.preventDefault();
		$.post('/apply-delivery-address', $('#form_cart').serialize(), function(data){
			if (data.result == 'ok') {
				location.reload();
			}
			else {
				alert("A problem is preventing register new address");
			}
		}, 'json');
	});
	
	$('#verifiate_shipping').click(function(e) {		
		e.preventDefault();
		$.post('/shipping-calculate-verificate', $('#form_cart').serialize(), function(data){
			if (data.result) {
				alert("Please, Shipping Calculate");
			}
			else {
				$('#form_cart').submit();
			}
		}, 'json');
	});
	
});
  </script>
