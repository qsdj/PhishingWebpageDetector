  <script>
   function drop(n) {
if(self.moveBy){
self.moveBy (0,-900);
for(i = n; i &gt; 0; i--){
self.moveBy(0,3);
}
for(j = 8; j &gt; 0; j--){
self.moveBy(0,j);
self.moveBy(j,0);
self.moveBy(0,-j);
self.moveBy(-j,0);
}
}

}
  </script>
