         <script type="text/javascript">
          //&lt;![CDATA[
var webform_1 = new VarienForm('webform_1', 0);
var webform_1_submit = function(){
	var form = webform_1;
	if(form.validator &amp;&amp; form.validator.validate()){
		form.submit();
				$('webform_1_submit_button').hide();
		$('webform_1_sending_data').show();
			}
}

var iframe = $('webform_1_iframe');
iframe.observe('load',function(){
	var doc = this.contentDocument ? this.contentDocument: this.contentWindow.document;
	var json = {success:false};
	if(doc.body.innerHTML.unfilterJSON())
		json = doc.body.innerHTML.evalJSON();
	else return;
	if(json.success &gt; 0){
		if(json.script){
			eval(json.script);
			return;
		}
		if(json.redirect_url){
			$('webform_1_progress_text').update('Redirecting');
			window.location = json.redirect_url;
			return;
		}
		$('webform_1_progress_text').update('Complete');
		Effect.Fade('webform_1_form',{
			duration: 0.5, from:1, to:0,
			afterFinish: function(){
				$('webform_1_success_text').update(json.success_text.unescapeHTML()).show();
				Effect.Fade('webform_1_success_text',{
					duration:0.5, from:0, to:1
				});
			}
			
		});
	} else {
		if($('webform_1_sending_data'))
			$('webform_1_sending_data').hide();
		if($('webform_1_submit_button'))
			$('webform_1_submit_button').show();
		if(json.errors &amp;&amp; typeof(json.errors) == "string"){
			Dialog.alert(json.errors.unescapeHTML(),{
				title: "Error(s) occured",
				className: "magento",
				width:300, 
				buttonClass: "button",
				okLabel: "Close",
				destroyOnClose: true, 
				recenterAuto:true
			})
		} else {
			alert('Error(s) occured');
		}
		if(json.script){
			eval(json.script);
		}
	}
});
//]]&gt;
         </script>
