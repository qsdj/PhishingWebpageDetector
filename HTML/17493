  function putFocusOnHeadingEnrollEsa()
	{
	var enabledDiv;
		if(document.getElementById('step-2')!=null &amp;&amp; document.getElementById('step-2').style.display=='block')
		{
			enabledDiv=document.getElementById('step-2');
		}
		if(document.getElementById('step-3')!=null &amp;&amp; document.getElementById('step-3').style.display=='block')
		{
			enabledDiv=document.getElementById('step-3');
		}
		if(document.getElementById('step-4')!=null &amp;&amp; document.getElementById('step-4').style.display=='block')
		{
			enabledDiv=document.getElementById('step-4');
		}
		if(enabledDiv!=undefined)
		{
		var nodeList=enabledDiv.childNodes;
		nodeList[1].focus();
		}
	}
	
  var resetTimerDisplay = function(){
  	document.getElementById('countdown').innerHTML = "59";
  }
  var showAreUStilOverlay = function() {	
  		time_left = 59;
  		startCounter = 1;
  		cinterval = setInterval('time_dec', 1000);
    	document.getElementById('AreUStilThrOverlayPanelScreen').style.display = "block";
    	if(document.getElementById('productheader')!=null &amp;&amp; (document.getElementById('productheader').style.opacity == "1" || document.getElementById('productheader').style.opacity == "")){
    		screenBlocked=true;
    		document.getElementById('OverlayPanelAreUStil').style.display = "block";
  			document.getElementById('OverlayPanelAreUStil').focus();
    		document.getElementById('OverlayPanelAreUStil').style.zIndex = "25000";
    		MyAppleID.util.overlay.setScreenSize();
    	}else{
    		dimOtherOverlays();
    		document.getElementById('OverlayPanelAreUStil').style.display = "block";
  			document.getElementById('OverlayPanelAreUStil').focus();
    		document.getElementById('OverlayPanelAreUStil').style.zIndex = "25000";
    	}
  		if(document.getElementById('main') !=null)
    	document.getElementById('main').setAttribute("aria-hidden","true");
    	if(document.getElementById('columnFirstSidebar')!=null)
  		document.getElementById('columnFirstSidebar').setAttribute("aria-hidden","true");
  		if(document.getElementById('columnLastMain')!=null)
	    document.getElementById('columnLastMain').setAttribute("aria-hidden","true");
	    if(document.getElementById('globalheader')!=null)
      	document.getElementById('globalheader').setAttribute("aria-hidden","true");
      	if(document.getElementById('globalfooter')!=null)
      	document.getElementById('globalfooter').setAttribute("aria-hidden","true");
      	if(document.getElementById('productheader')!=null)
   		document.getElementById('productheader').setAttribute("aria-hidden","true");
   		document.getElementById('continueLink').focus();
    	}	

  var removeAreUStilOverlay = function() {	
    		document.getElementById('OverlayPanelAreUStil').style.display = "none";
    		document.getElementById('AreUStilThrOverlayPanelScreen').style.display = "none";
    		if(screenBlocked == true){
	    		screenBlocked=false;
	    		MyAppleID.util.overlay.setScreenSize();
	    		if(document.getElementById('columnLastMainHeading')!=null)
                    document.getElementById('columnLastMainHeading').focus();
                else if(document.getElementById('productheader')!=null)
                    document.getElementById('productheader').focus();
    		}else{
    		
    			enableOtherOverlays();
    			screenBlocked=false;
    		}
    	if(document.getElementById('productheader')!=null &amp;&amp; (document.getElementById('productheader').style.opacity == "1" || document.getElementById('productheader').style.opacity == "")){
        	if(document.getElementById('main') !=null)
    	       document.getElementById('main').setAttribute("aria-hidden","false");
    	   if(document.getElementById('columnFirstSidebar')!=null)
  		        document.getElementById('columnFirstSidebar').setAttribute("aria-hidden","false");
  		   if(document.getElementById('columnLastMain')!=null)
	           document.getElementById('columnLastMain').setAttribute("aria-hidden","false");
	       if(document.getElementById('globalheader')!=null)
      	       document.getElementById('globalheader').setAttribute("aria-hidden","false");
      	   if(document.getElementById('globalfooter')!=null)
      	       document.getElementById('globalfooter').setAttribute("aria-hidden","false");
      	   if(document.getElementById('productheader')!=null)
   		       document.getElementById('productheader').setAttribute("aria-hidden","false");
   		}
   		
   		
    	}
