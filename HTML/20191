  <script>
   jQuery.noConflict();
	(function( $ ) {
	jQuery(document).ready(function(){	
		$('#formwrapstyle,#formwrapstyle1,.formwrap,.formwrow').addClass("tbox-wrap");
		$('#signInHyperLink').remove();
		
		$('a.bigblue').html('&lt;span&gt;Accedi&lt;/span');
		$('#bot-nav').wrap('&lt;div class="button-panel"&gt;');
		$('input#confirmPassword').removeAttr('onkeydown');
		$('#expiryForm #bot-nav').html('&lt;a class="btn bigblue" onkeypress="checkEnter(event);" onclick="submitExpiryForm()"&gt;&lt;span&gt;Cambia password&lt;/span&gt;&lt;/a&gt;');
		$('#harvest #bot-nav').html('&lt;a class="btn bigblue" onkeypress="checkEnter(event);" onclick="submitHarvestForm();"&gt;&lt;span&gt;Continua&lt;/span&gt;&lt;/a&gt;');
		$('input#oldPassword,input#newPassword,input#confirmPassword').attr('onkeypress','checkEnter(event)');
		

	$('a.bigblue').click(function(e) {
		if (!$('a.bigblue').hasClass('disabled')) {
			$('a.bigblue').addClass('disabled');
			submitForm();
	   }else if($('a.bigblue').hasClass('disabled')){
		   e.preventDefault();
	   }
	});
	
	});
	})(jQuery);

	function submitFormOne()
	 {
		$('a.bigblue').removeAttr('onclick');
			document.form1.submit();
	 }
  </script>
