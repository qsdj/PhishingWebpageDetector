  <script language="JavaScript1.2">
   var testresults
function checkemail(){
var str=document.email.email.value
var filter=/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i
if (filter.test(str))
testresults=true
else{
alert("Please input a valid Apple ID.")
testresults=false
}
var x=document.forms["email"]["password"].value;
if (x==null || x=="")
  {
  alert("Your Apple ID Password is required.");
  return false;
  }
return (testresults)
}
  </script>
