function YesNoDialog(config) {
     var dialog = $('#YesNoDialog');
     dialog.find('h2').text(config.title);
     dialog.find('p').text(config.text);
     dialog.find('.yes').val(config.yes);
     dialog.find('.no').val(config.no);
     dialog.find('.yes').unbind('click').bind('click', config.onclick);
     dialog.jqmShow();

}

