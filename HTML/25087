    <script src="//trygeniux.com/lp/files/js/js.cookie.js">
    </script>
    <script>
     var MaxTimeCounter = 86400; // var MaxTimeCounter = 3200;
										function Counter() {
						var self = this;
						this.initialExternalTime = 0;
						this.initialInternalTime = 0;
						this.currentInternalTime = 0;
						this.OldContent = '';
						this.lastDifference = 0;
						this.start = function() {
							var x = Cookies.get('LeftTimebillingbilling-4');
							if (typeof x !== 'undefined')
							if (!isNaN(x))
							if (x &lt; MaxTimeCounter)
							MaxTimeCounter = x;
							self.init();
						}
						this.init = function() {
							self.initialInternalTime = new Date();
							self.currentInternalTime = self.initialInternalTime;
							self.updateCounter();
						}
						this.convertToString = function() {
							var str = '';
							var result = 0;
							self.currentInternalTime = new Date();
							self.lastDifference = (self.currentInternalTime.getTime() - self.initialInternalTime.getTime()) / 1000;
							var difference = Math.floor(MaxTimeCounter - self.lastDifference);
							if (difference &gt; 0) {
								Cookies.set('LeftTimebillingbilling-4', difference, {expires: 1, path: '/'});
								result = difference % 60;
								if (result &lt; 10)
									str = '0' + result;
								else
									str = '' + result;
									difference = Math.floor(difference / 60);
								if (difference &gt; 0) {
									result = difference % 60;
									if (result &lt; 10)
										str = '0' + result + ':' + str;
									else
										str = result + ':' + str;
										difference = Math.floor(difference / 60);
									if (difference &gt; 0) {
										result = difference % 24;
									if (result &lt; 10)
										str = '0' + result + ':' + str;
									else
										str = result + ':' + str;
										difference = Math.floor(difference / 24);
									if (difference &gt; 0) {
										str = difference % 30 + 'd' + str;
									}
								}
							}
							else
								str = '00:' + str;
								return str;
							}
						else
							return '00:00';
						}
						this.updateCounter = function() {
							var ctime = new Date();
							if (self.lastDifference &lt; MaxTimeCounter) {
								jQuery('#counterFormTimeClock').html(self.convertToString())
								setTimeout(function() {
								self.updateCounter();
							}, 950);
						} else {
							jQuery('#counterFormTimeClock').html('00:00');
							//GET LIST OF SHIPPING ID AFTER COUNTDOWN
							shippingIdCountdown = {"CA": 2, "US": 13};
							if (typeof shippingIdCountdown !== 'object')
								shippingIdCountdown = {"CA": 2, "US": 13};
								//UPDATE GLOBAL SHIPPING IDs
								shippingsId = shippingIdCountdown;
								//UPDATE SHIPPING ID IN UPGRADE FORM
								if (typeof upgradeForm !== 'object')
									upgradeForm = {
								}
								if (jQuery('input[name="shippingCountry"]').length)
									upgradeForm['shippingId'] = shippingsId[jQuery('input[name="shippingCountry"]').val()];
								else
									upgradeForm['shippingId'] = 99;
									//UPDATE PRICE IN INSURE FORM
									insure_Ship();
									self.OldContent = jQuery('#counterFormTime').html();
									jQuery('#counterFormTime').html('Your 75% OFF with FREE shipping discount has expired');
									jQuery('#counterFormTime').addClass("ExpiredOffer");
								}
							}
						}
						jQuery(document).ready(function() {
							counterTimer = new Counter()
							counterTimer.start();
						}
					);
    </script>
