   function reportAbuse(lid,ltp,lurl)
{
  if(confirm("Are you sure about sending correction alert for this listing?"))
  {
	  jQuery.ajax({
		  type: "POST",
		  data: "lid="+lid+"&amp;ltp="+ltp+"&amp;lurl="+lurl,
		  url: "includes/reporting.php",
		  success: function(result) {
			  alert(result);
		  }
	  });
}
}

		
function checkuser1(str)
{
	
	if(document.getElementById("user_name").value!="")
	{
  var xmlhttp;
  if(str=="")
  { document.getElementById("chkusr1").innerHTML='&lt;input type="hidden" id="userexist" name="userexist" value="N"/&gt;'; return;}	
  
  document.getElementById("loaduser1").style.visibility = 'visible';
  
  if(window.XMLHttpRequest)
  { xmlhttp = new XMLHttpRequest(); }
  else
  { xmlhttp = new ActiveXObject("Microsoft.XMLHTTP"); }
  
 xmlhttp.open("GET","backcheck.php?ue="+str,true);
 xmlhttp.send();
   
  xmlhttp.onreadystatechange=
  function()
  {
	if(xmlhttp.readyState==4 &amp;&amp; xmlhttp.status==200)
	{ 
	  document.getElementById("chkusr1").innerHTML=xmlhttp.responseText; 
	  //alert(document.getElementById("chkusr").innerHTML);
	  if(document.getElementById("chkusr1").innerHTML!="")
	  {  
		  //alert("abc"+document.getElementById("chkusr").innerHTML+"abc");
		  document.getElementById("errmsg").innerHTML="";
		document.getElementById("user_name").focus();
		document.getElementById("pws").disabled=true;  
		
	  } 
	  else if(document.getElementById("chkusr1").innerHTML==""){    
		
		  document.getElementById("pws").removeAttribute("disabled");
		  document.getElementById("pws").focus();
		  }
	  document.getElementById("loaduser1").style.visibility = 'hidden'; 
	}   
  } 
	}
	else
	{
		 document.getElementById("chkusr1").innerHTML="Username cannot be empty."
		document.getElementById("user_name").focus();
		document.getElementById("pws").disabled=true; 
	}
}

function loading_mailbox()
{
	document.getElementById("mailbox").click();	
	
}

//Edited on:29.08.2014	
function loginnow1()
{

  var usr=document.getElementById("user_name").value;

  var pwd=document.getElementById("pws").value;
  var xmlhttp;
  if(window.XMLHttpRequest)
  { xmlhttp = new XMLHttpRequest(); }
  else
  { xmlhttp = new ActiveXObject("Microsoft.XMLHTTP"); }
 
 xmlhttp.open("GET","backcheck.php?log="+usr+"&amp;pwd="+pwd,true);
 xmlhttp.send();
   
  xmlhttp.onreadystatechange=
  function()
  {
	if(xmlhttp.readyState==4 &amp;&amp; xmlhttp.status==200)
	{ 
		if(xmlhttp.responseText!="")
		{
			
			document.getElementById('loginerror').innerHTML=xmlhttp.responseText;
		}
		else
		{
			
			window.location.reload();
		}
	}  
  }
}
