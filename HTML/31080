         <!-- Plugins: BeforeDisplayContent -->
         <!-- K2 Plugins: K2BeforeDisplayContent -->
         <!-- Item Image -->
         <div class="itemImageBlock">
          <span class="itemImage">
           <a title="Click to preview image" href="/media/k2/items/cache/7af0193b8351bd1eb04c09dfe5731fa3_XL.jpg" rel="{handler: 'image'}" class="modal">
            <img style="width:450px; height:auto;" alt="Tatap Muka Bupati Nagekeo dengan Anggota Satuan Polisi Pamong Praja" src="/media/k2/items/cache/7af0193b8351bd1eb04c09dfe5731fa3_L.jpg" />
           </a>
          </span>
          <div class="clr">
          </div>
