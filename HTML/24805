     <script language="JavaScript">
      function itemuse_insert()
{
    var f = document.fitemuse;
    var id = document.getElementById('itemuse');

    if (!g4_is_member)
    {
        id.style.display = 'none';	
        alert("회원이상 이용가능 게시판입니다,회원가입후 이용해주세요");

    } else {
       id.style.display = 'block';
    }
}

function itemuse_update(idx)
{

    var id = document.getElementById('itemuse'+idx);
    var w = document.getElementById('itemuse');
    w.style.display = 'none';

    if (id.style.display == 'none') {
      id.style.display = 'block';
		return;
    }else if (id.style.display == 'block') {
      id.style.display = 'none';
		return;
	}

}

function itemuse_delete(f, idx)
{
    var id = document.getElementById('itemuse');

    f.w.value = 'd';
    f.wr_id.value = document.getElementById('tmp_wr_id'+idx).value;

    if (g4_is_member)
    {
        if (confirm("삭제하시겠습니까?"))
            f.submit();
    }
    else 
    {
        id.style.display = 'none';
        document.getElementById('itemuse_password'+idx).style.display = 'block';
    }
}
     </script>
