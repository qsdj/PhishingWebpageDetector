            function formValidate(f) {
               if (document.formvalidator.isValid(f)) {
                  return true;
               }
               else {
                  alert("Please enter all required fields!");
               }
               return false;
            }

            window.addEvent('domready', function(){
                    var saveSlide = new Fx.Slide('save-panel');
                    $('saveslidein').addEvent('click', function(e){
                        document.getElementById('save-panel').style.display = 'block';
                        e = new Event(e);
                        saveSlide.slideIn();
                        e.stop();
                    });
                    $('saveslideout').addEvent('click', function(e){
                        e = new Event(e);
                        saveSlide.slideOut();
                        e.stop();
                    });
                    saveSlide.hide();
            });
            /*]]&gt;*/
