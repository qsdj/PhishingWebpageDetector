  <script type="text/javascript">
   var portfoliobindevents = ["mousedown", "mousemove", "mouseup"];
	if('ontouchstart' in window){
		portfoliobindevents = ["touchstart", "touchmove", "touchend"];
	}else{
		portfoliobindevents = ["mousedown", "mousemove", "mouseup"];
	}
	var portfoliofixedmenum = 2;var portfolioprojectdirect = false;	var ajaxloadingproc = false;
	
	jQuery( document ).ready( function() {
		var $body = jQuery('body'); //Cache this for performance
		
		var scrollbar_width = jQuery('.dribbbles li').length;
		var scrollbar_width_news = (jQuery('.overview_news .excerpt-blog').length);
		jQuery('#scrollbar2 .overview_dribbble').css({'width' : (420*scrollbar_width)+(1/2)*jQuery(window).width()+"px" });
		jQuery('#scrollbar2 .overview_news').css({'width' : (390*scrollbar_width_news)+(1/2)*jQuery(window).width()+"px" });
		
		var setBodyScale = function() {
			var scaleFactor = 0.35,
				scaleSource = $body.width(),
				maxScale = 1400,
				minScale = 575;

			var fontSize = scaleSource * scaleFactor; //Multiply the width of the body by the scaling factor:

			if (fontSize &gt; maxScale) fontSize = maxScale;
			if (fontSize &lt; minScale) fontSize = minScale; //Enforce the minimum and maximums

			jQuery('.page-title').css('font-size', fontSize + '%');
		}
		
		var setDescriptionScale = function() {
			var scaleFactor = 0.1,
				scaleSource = $body.width(),
				maxScale = 220,
				minScale = 180;

			var fontSize = scaleSource * scaleFactor; //Multiply the width of the body by the scaling factor:

			if (fontSize &gt; maxScale) fontSize = maxScale;
			if (fontSize &lt; minScale) fontSize = minScale; //Enforce the minimum and maximums

			jQuery('.page-description').css('font-size', fontSize + '%');
		}
		
		var setPageTitleScale = function() {
			var scaleFactor = 0.35,
				scaleSource = $body.width(),
				maxScale = 1400,
				minScale = 575;

			var fontSize = scaleSource * scaleFactor; //Multiply the width of the body by the scaling factor:

			if (fontSize &gt; maxScale) fontSize = maxScale;
			if (fontSize &lt; minScale) fontSize = minScale; //Enforce the minimum and maximums

			jQuery('.page_description').css('font-size', fontSize + '%');
		}
	
		jQuery(window).resize(function(){
			setBodyScale();
			setDescriptionScale();
			setPageTitleScale();
		});
		
		//Fire it when the page first loads:
		setBodyScale();
		setDescriptionScale();
		setPageTitleScale();
	});
	
	jQuery(document).ready(function() {
		jQuery("#header, .slides, .slides .myimage, #footer, .content-projectc, body").each(function(){
			jQuery(this).attr('unselectable', 'on').css({'-moz-user-select':'none', '-webkit-user-select':'none', 'user-select':'none' }).each(function(){ this.onselectstart = function(){ return false; }; });
		});
	});
	
	var menumousetouch = {'isopenspan':false, 'controlactive':false, 'preventdefault':false, 'allowpropagate':false, 'startposition':[-1,-1], 'endposition':[-1,-1]};
	function ontouchmenudrag_nprevent(e){
		menumousetouch.preventdefault = false;
		ontouchmenudrag(e);
	}
	function ontouchmenudrag_prevent(e){
		menumousetouch.preventdefault = true;
		ontouchmenudrag(e);
	}
	function ontouchmenudrag(e){
		//menumousetouch.controlactive = true;
		menumousetouch.startposition = [-1,-1];
		menumousetouch.endposition = [-1,-1];
		jQuery("body").unbind(portfoliobindevents[1], ontouchmenumove).bind(portfoliobindevents[1], ontouchmenumove);
		jQuery("body").unbind(portfoliobindevents[2], ontouchmenudrop).bind(portfoliobindevents[2], ontouchmenudrop);
		e.cancelBubble = true;
		if(menumousetouch.preventdefault){
			e.preventDefault();
		}
		e.stopPropagation();
	}
	function ontouchmenumove(e){
		//if(menumousetouch.controlactive){
			e.preventDefault();
			e.stopPropagation();
			var t = false;
			if(e.originalEvent.touches &amp;&amp; e.originalEvent.touches[0]){
				t = e.originalEvent.touches[0];
			}else if(e.originalEvent.changedTouches &amp;&amp; e.originalEvent.changedTouches[0]){
				t = e.originalEvent.changedTouches[0];
			}else{
				t = e;
			}
			if(t){
				if(t.pageX){
					if(menumousetouch.startposition[0] == -1){
						menumousetouch.startposition[0] = t.pageX;
					}else{
						menumousetouch.endposition[0] = t.pageX;
						var portfolioseposdelta = menumousetouch.endposition[0] - menumousetouch.startposition[0];
						if(!menumousetouch.isopenspan){
							if(portfolioseposdelta &gt; 0 &amp;&amp; portfolioseposdelta &lt; 210){
								jQuery('#header').stop().css({ marginLeft: -210+portfolioseposdelta });
								jQuery('#footer').stop().css({ marginLeft: -210+portfolioseposdelta });
							}
						}else{
							if(portfolioseposdelta &lt; 0 &amp;&amp; portfolioseposdelta &gt; -210){
								jQuery('#header').stop().css({ marginLeft: portfolioseposdelta });
								jQuery('#footer').stop().css({ marginLeft: portfolioseposdelta });
							}
						}
					}
				}
			}
		//}
	}
	function ontouchmenudrop(e){
		jQuery("body").unbind(portfoliobindevents[1], ontouchmenumove);
		jQuery("body").unbind(portfoliobindevents[2], ontouchmenudrop);
		if(menumousetouch.startposition[0] != -1 &amp;&amp; menumousetouch.endposition[0] != -1){
			var portfolioseposdelta = menumousetouch.endposition[0] - menumousetouch.startposition[0];
			if(portfolioseposdelta){
				if(Math.abs(portfolioseposdelta) &gt;= 20){
					if(portfolioseposdelta &gt; 0){
						menumousetouch.isopenspan = true;
						jQuery('#header').stop().animate({ marginLeft: "0px" }, 300);
						jQuery('#footer').stop().animate({ marginLeft: "0px" }, 300);
						jQuery('#footer_copyright').css({ padding: '20px 30px' });
						jQuery('.imgscontainer').stop().animate({ left: "220px" }, 300);
					}else{
						menumousetouch.isopenspan = false;
						jQuery('#header').stop().animate({ marginLeft: "-210px" }, 300);
						jQuery('#footer').stop().animate({ marginLeft: "-230px" }, 300);
						jQuery('.imgscontainer').stop(true,true).animate({ left: "0px" }, 300);
						jQuery('#footer_copyright').stop().css({ padding: '20px 0 20px 30px' });
					}
				}else{
					if(menumousetouch.isopenspan){
						jQuery('#header').stop().animate({ marginLeft: "0px" }, 300);
						jQuery('#footer').stop().animate({ marginLeft: "0px" }, 300);
					}else{
						jQuery('#header').stop().animate({ marginLeft: "-210px" }, 300);
						jQuery('#footer').stop().animate({ marginLeft: "-230px" }, 300);
					}
				}
				menumousetouch.allowpropagate = false;
				//e.preventDefault();
				e.stopPropagation();
			}else{
				menumousetouch.allowpropagate = true;
			}
		}else{
			menumousetouch.allowpropagate = true;
		}
		//menumousetouch.controlactive = false;
	}
	function onhovertouchclickopen(){
		menumousetouch.isopenspan = true;
		jQuery('#header').stop().animate({ marginLeft: "0px" }, 300);
		jQuery('#footer').stop().animate({ marginLeft: "0px" }, 300);
		jQuery('#footer_copyright').css({ padding: '20px 30px' });
		jQuery('.imgscontainer').stop().animate({ left: "220px" }, 300); //.animate({ width: "100%" }, 300);
	}
	function onhovertouchclickclose(){
		menumousetouch.isopenspan = false;
		jQuery('#header').stop().animate({ marginLeft: "-210px" }, 300);
		jQuery('#footer').stop().animate({ marginLeft: "-230px" }, 300);
		jQuery('.imgscontainer').stop(true,true).animate({ left: "0px" }, 300); //.animate({ width: "105%" }, 300);
		//setTimeout(function(){
			jQuery('#footer_copyright').stop().css({ padding: '20px 0 20px 30px' });
		//}, 10);
	}
	function onhovertouchbindevents(){
		if('ontouchstart' in window){
			jQuery("#header, #footer, .header-arrow").click(function(e){
				if(menumousetouch.allowpropagate){
					if(menumousetouch.isopenspan){
						onhovertouchclickclose();
					}else{
						onhovertouchclickopen();
					}
				}
				e.stopPropagation();
			});
			//menumousetouch.controlactive = false;
			jQuery("#header, #footer").bind(portfoliobindevents[0], ontouchmenudrag_nprevent);
			jQuery(".header-arrow").bind(portfoliobindevents[0], ontouchmenudrag_prevent);
			//jQuery("body").bind(portfoliobindevents[2], ontouchmenudrop);
		}else{
			jQuery("#header, #footer").hover(onhovertouchclickopen, onhovertouchclickclose);
		}
	}
	function onhovertouchunbindevents(){
		if('ontouchstart' in window){
			jQuery("#header, #footer, .header-arrow").unbind("click");
			jQuery("body").unbind(portfoliobindevents[1], ontouchmenumove);
			jQuery("body").unbind(portfoliobindevents[2], ontouchmenudrop);
			jQuery("#header, #footer").unbind(portfoliobindevents[0]);
			jQuery(".header-arrow").unbind(portfoliobindevents[0]);
		}else{
			jQuery("#header, #footer").unbind("hover");
		}
	}
	jQuery(document).ready(function() {
		if(portfoliofixedmenum == 1 || (portfolioprojectdirect &amp;&amp; portfoliofixedmenum==2)){
			jQuery('.imgscontainer').css({ left: "0px", 'width': jQuery(window).width()+'px'});
			onhovertouchbindevents();
		}else{
			menumousetouch.isopenspan = true;
			jQuery('#header').css({ marginLeft: "0px" });
			jQuery('#footer').css({ marginLeft: "0px" });
			jQuery('#footer_copyright').css({ padding: '20px 30px' });
			jQuery(".header-arrow").css({'display':'none'});
			if(portfolioprojectdirect){
				jQuery('#content').css({'left':'0'});
			}else{
				jQuery('#content').css({'left':'210px'});
			}
			jQuery('.page_description').css({'left':'210px'});
			jQuery('.cooming-soon-content').css({'left':'210px'});
		//	jQuery('#scrollbar2').css({'position':'relative','left':'210px'});
			jQuery('#scrollbar2 .viewport').css({ left: "220px", 'width': (((jQuery(window).width()&lt;210)?210:jQuery(window).width())-210)+'px'});
			jQuery('.imgscontainer').css({ left: "220px", 'width': ((jQuery(window).width()&lt;220)?220:(jQuery(window).width()-200))+'px'});
			
			//trigger resize proj
		}
	});
	
		function setSlideDescriptionScale() {
			var currentmargin = jQuery(window).width()*0.04;
			if(currentmargin &lt;= 70){ currentmargin = 70; }			
			if(portfoliocurrentslideactive &amp;&amp; portfolioslidescountnleft(portfoliocurrentslideactive).screenleft){
				jQuery('.current_sli_desc').stop().css({'margin-left':(portfolioslidescountnleft(portfoliocurrentslideactive).screenleft+currentmargin+0.15*currentmargin)+"px"});
				jQuery('.current_sli_desc').stop().css({ 'bottom': (currentmargin+0.15*currentmargin)+"px" });
			}else{
				jQuery('.current_sli_desc').stop().css({ 'margin-left': (currentmargin+0.15*currentmargin)+"px" });
				jQuery('.current_sli_desc').stop().css({ 'bottom': (currentmargin+0.15*currentmargin)+"px" });
			}
		}
		function setSlideDescriptionLinks() {
			jQuery('.current_sli_desc A').click(function(){
				window.open(jQuery(this).attr('href'));
				return false;
			});
		}
		function setBlogLinks() {
			jQuery('.excerpt-blog A').click(function(){
				window.open(jQuery(this).attr('href'));
				return false;
			});
		}
		
		function setPageTitleScale() {
			var currentmargin = jQuery(window).width()*0.07;
			if(currentmargin &lt;= 80){ currentmargin = 80; }
			jQuery('.page_description').stop().css({ 'margin-left': currentmargin+"px" });
			if(jQuery(window).height() &lt;= 600){ currentmargin = 40; }
			jQuery('.page_description').stop().css({ 'top': currentmargin-(0.2*currentmargin)+"px" });
		} 
	
		var portfoliohistorywpurl = "";
		var portfoliocurrentprojectid = 1;
		var portfoliocurrentslideactive = 0;
		var portfolioreqsetproject = function(prevnext){
			ajaxloadingproc = true;
			portfoliokillifaceevents();
			setTimeout(function() { 
				$.ajax({ url: 'http://antonyfirth.com/wp-admin/admin-ajax.php',
					data: {'action': 'nextprevproject', 'projectid': portfoliocurrentprojectid, 'prevnext': prevnext},
					//dataFilter: function(data,type){
						//alert(data);
					//},
					success: function(data){
						if(data.err){
							ajaxloadingproc = false;
							alert(data.errmsg);
						}else{
							var databgimagecp = "";
							var datatxtcolorcp = "";
							var datatitle = "";
							try{
								portfoliocurrentprojectid = data.projectid; 
								jQuery(".portfolio-fs-slides").css('display','none').empty();
								jQuery(".portfolio-fs-slides").append(data.content);
								jQuery(".portfolio-fs-slides .myimage").each(function(i,e){
									jQuery(".portfolio-fs-slides").append(jQuery("&lt;div&gt;").attr("class", "portfolio-fs-slide").html(jQuery(e).clone(true)));
									jQuery(e).remove();
								});
								jQuery(".portfolio-fs-slides").prepend('&lt;div class="portfolio-fs-slide current-slide"&gt;&lt;img class="myimage" alt="" src="" style="display:block;opacity:0;" /&gt;&lt;div id="content" class="content-projectc"&gt;&lt;div class="portfolio-excerpt" style="opacity:0;"&gt;&lt;h1 class="page-title" style="letter-spacing:-4px;float:left;"&gt;&lt;/h1&gt;&lt;div class="portfolio-meta portfolio-cats" style="float:left;margin: 14px 0 0 10px;"&gt;&lt;/div&gt;&lt;div style="clear:both;"&gt;&lt;/div&gt;&lt;div style="font-size:11px;line-height:27px;max-width:600px;width: 100%;"&gt;&lt;h4 class="page-description" style="padding-bottom:10px;width:700px;"&gt;&lt;/h4&gt;&lt;/div&gt;&lt;ul class="portfolio-meta"&gt;&lt;li class="portfolio-date"&gt;&lt;span class="portfolio-meta-heading"&gt;DATE&lt;/span&gt; &lt;span class="page-exdate"&gt;&lt;/span&gt;&lt;/li&gt;&lt;li class="portfolio-client"&gt;&lt;span class="portfolio-meta-heading"&gt;CLIENT&lt;/span&gt; &lt;span class="page-exclient"&gt;&lt;/span&gt;&lt;/li&gt;&lt;li class="portfolio-agency"&gt;&lt;span class="portfolio-meta-heading"&gt;AGENCY&lt;/span&gt; &lt;span class="page-exagency"&gt;&lt;/span&gt;&lt;/li&gt;&lt;/ul&gt;&lt;/div&gt;&lt;div class="clear"&gt;&lt;/div&gt;&lt;/div&gt;&lt;/div&gt;');
								jQuery(".portfolio-excerpt .page-title").text(data.title);
								jQuery("title").text(data.title);
								jQuery(".portfolio-excerpt .page-description").html(data.desc);
								if(!data.date){
									jQuery(".portfolio-date").css({'display':'none'});
								}else{
									jQuery(".portfolio-date").css({'display':'block'});
									jQuery(".portfolio-excerpt .page-exdate").text(data.date);
								}
								if(!data.client){
									jQuery(".portfolio-client").css({'display':'none'});
								}else{
									jQuery(".portfolio-client").css({'display':'block'});
									jQuery(".portfolio-excerpt .page-exclient").html(data.client);
								}
								if(!data.agency){
									jQuery(".portfolio-agency").css({'display':'none'});
								}else{
									jQuery(".portfolio-agency").css({'display':'block'});
									jQuery(".portfolio-excerpt .page-exagency").html(data.agency);
								}
								jQuery(".portfolio-excerpt .portfolio-cats").text(data.categories.join(", "));
								datatxtcolorcp = data.txtcolor;
								databgimagecp = data.bgimage;
								datatitle = data.title;
								window.history.pushState({}, data.title, ((portfoliohistorywpurl)?("/"+portfoliohistorywpurl):"")+"/portfolio/"+data.postname+"/");
							}catch(e){}
							portfoliocurrentslideactive = 0;
							if(jQuery(".socialikonsg").length){
								jQuery(".socialikonsg").css({'display':'block', 'opacity':'0'});
							}else{
								jQuery("body").append(jQuery("&lt;div style=\"opacity:0;\" class=\"socialikonsg\"&gt;&lt;a href=\"javascript:void(null);\" class=\"twitter\" title=\"Twitter\"&gt;t&lt;/a&gt; &lt;a href=\"javascript:void(null);\" class=\"facebook\" title=\"Facebook\"&gt;f&lt;/a&gt; &lt;a href=\"javascript:void(null);\" class=\"googleplus\" title=\"Google+\"&gt;g&lt;/a&gt;&lt;/div&gt;"));
								try{
									jQuery(".socialikonsg a[title]").tooltip({"position": "bottom center", "tipClass": "jqttooltip", "effect":"r_fadeslide"});
								}catch(e){}
							}
							jQuery(".socialikonsg .twitter").attr("href", "https://twitter.com/share?url="+escape(window.location.href)+"&amp;text="+escape(datatitle));
							jQuery(".socialikonsg .facebook").attr("href", "http://www.facebook.com/sharer.php?u="+escape(window.location.href)+"&amp;t="+escape(datatitle));
							jQuery(".socialikonsg .googleplus").attr("href", "https://plus.google.com/share?url="+escape(window.location.href));
							jQuery(".portfolio-fs-slides").css({'left':0}).css('display','block');
							jQuery(".portfolio-arrow-right, .portfolio-arrow-left").css('display','block');
							jQuery(".portfolio-arrow-right").css({ "height": (jQuery(window).height())+"px" }).css({ width: '50%' });
							jQuery(".portfolio-arrow-left").css({ "height": (jQuery(window).height())+"px" }).css({ width: '50%' });
							if(datatxtcolorcp == "#ffffff"){
								jQuery(".myimage", jQuery(".portfolio-fs-slide").get(0)).addClass('text_white');
								jQuery("#content", jQuery(".portfolio-fs-slide").get(0)).addClass('contenttextwhite');
								jQuery(".portfolio-arrow-left").removeClass('portfolio-arrow-left-first');
								jQuery(".portfolio-arrow-left").removeClass('portfolio-arrow-left-white');
								jQuery(".portfolio-arrow-right").removeClass('portfolio-arrow-right-last');
								jQuery(".portfolio-arrow-left").addClass('portfolio-arrow-left-first-white');
								if(jQuery(".portfolio-fs-slide").length &gt; 1){
									jQuery(".portfolio-arrow-right").removeClass('portfolio-arrow-right-last-white');
									jQuery(".portfolio-arrow-right").addClass('portfolio-arrow-right-white');
								}else{
									jQuery(".portfolio-arrow-right").removeClass('portfolio-arrow-right-white');
									jQuery(".portfolio-arrow-right").addClass('portfolio-arrow-right-last-white');
								}
								jQuery(".socialikonsg a").css({'color':'#ffffff'});
							}else{
								jQuery(".portfolio-arrow-left").removeClass('portfolio-arrow-left-first-white');
								jQuery(".portfolio-arrow-left").removeClass('portfolio-arrow-left-white');
								jQuery(".portfolio-arrow-right").removeClass('portfolio-arrow-right-white');
								jQuery(".portfolio-arrow-right").removeClass('portfolio-arrow-right-last-white');
								jQuery(".portfolio-arrow-left").addClass('portfolio-arrow-left-first');
								if(jQuery(".portfolio-fs-slide").length &gt; 1){
									jQuery(".portfolio-arrow-right").removeClass('portfolio-arrow-right-last');
								}else{
									jQuery(".portfolio-arrow-right").addClass('portfolio-arrow-right-last');
								}
								jQuery(".socialikonsg a").css({'color':'#464646'});
							}
							jQuery(".current_sli_desc").css({'opacity':'0'});
							try{
								VideoJS.setupAllWhenReady();
								videojsvolumemuteclick();
							}catch(e){}
							if(databgimagecp){
								jQuery(".myimage", jQuery(".portfolio-fs-slide").get(0)).load(function(e){
									jQuery(this).unbind(e);
									setTimeout(function(){
										//tresizewindowf();
										jQuery(window).trigger("resize");
										jQuery(".loading").stop().animate({'opacity':'0'}, 500, function(){
											jQuery(".loading").css({'display':'none'});
											jQuery(".portfolio-excerpt").stop().css({opacity: 0}).animate({ opacity: 1 }, 400);
											jQuery(".myimage", jQuery(".portfolio-fs-slide").get(0)).stop().css({ opacity: 1 });
											jQuery(".socialikonsg").stop().animate({'opacity':1},400);
											portfolioinitifaceevents();
											ajaxloadingproc = false;
										});
									}, 500);
								}).error(function(e){
									jQuery(this).unbind(e);
									setTimeout(function(){
										jQuery(window).trigger("resize");
										jQuery(".loading").stop().animate({'opacity':'0'}, 500, function(){
											jQuery(".loading").css({'display':'none'});
											jQuery(".portfolio-excerpt").stop().css({opacity: 0}).animate({ opacity: 1 }, 400);
											jQuery(".myimage", jQuery(".portfolio-fs-slide").get(0)).stop().css({ opacity: 1 });
											jQuery(".socialikonsg").stop().animate({'opacity':1},400);
											portfolioinitifaceevents();
											ajaxloadingproc = false;
										});
									}, 500);
								}).attr("src", databgimagecp);
							}else{
								setTimeout(function(){
									jQuery(window).trigger("resize");
									jQuery(".loading").stop().animate({'opacity':'0'}, 500, function(){
										jQuery(".loading").css({'display':'none'});
										jQuery(".portfolio-excerpt").stop().css({opacity: 0}).animate({ opacity: 1 }, 400);
										jQuery(".socialikonsg").stop().animate({'opacity':1},400);
										portfolioinitifaceevents();
										ajaxloadingproc = false;
									});
								}, 500);
							}
							if(portfoliofixedmenum == 2){
								if(menumousetouch.isopenspan){
									jQuery('.imgscontainer').css({ left: "0px", 'width': jQuery(window).width()+'px'});
									//menumousetouch.isopenspan = false;
									onhovertouchclickclose();
