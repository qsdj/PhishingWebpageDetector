function csv_moveToEmail1(obj)
{

	if(obj.id == 'email_address2' &amp;&amp; csv_getField('email_address1').value == '')
	{
		csv_getField('email_address1').focus();

	}
}
function chk_same_email(obj)
{

	if(csv_getField('email_address1') &amp;&amp; csv_getField('email_address2') &amp;&amp; csv_isValidEmail('email_address1'))
	{
		if((obj.id == 'email_address1' &amp;&amp; csv_getField('email_address2').value != '') || obj.id=='email_address2')
		{

			   if (csv_findFieldVal('email_address1') != csv_findFieldVal('email_address2'))
			   {

				alert("Re entered E-mail address is different");
				csv_getField('email_address2').value='';
				csv_getField('email_address2').focus();

			   }

		}
	}
}

function csv_isValidEmail(id)
{
	var str = csv_getField(id).value;
  	if (window.RegExp)
	{
    	var reg1str = "(@.*@)|(\\.\\.)|(@\\.)|(\\.@)|(^\\.)";
	    var reg2str = "^.+\\@(\\[?)[a-zA-Z0-9\\-\\_\\.]+\\.([a-zA-Z]{2,3}|[0-9]{1,3})(\\]?)$";
	    var reg1 = new RegExp(reg1str);
	    var reg2 = new RegExp(reg2str);
	    if (!reg1.test(str) &amp;&amp; reg2.test(str))
	      return true;  // all ok
	   return false;
  	}
	else
	{
    if(str.indexOf("@") &gt;= 0)
      	return true;	// all ok
	return false;
  }
}

