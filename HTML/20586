	function trocarPag(n){
		$('#listadeprod').html('&lt;div   style="text-align:center;"&gt;&lt;img style="margin-top:15%;" src="admin/imagens/load.gif"  alt=""/&gt;&lt;/div&gt;');
		$.post('admin/produtos.php', {categoria : "index.html", pag:n, numpag:(tl*ta), ordem:ordem, busca:busca} , function(resposta) {
				if (resposta != false) {					
					$('#listadeprod').html(resposta);
					$('#botaopaf'+(btativo)).css("background", "");
					btativo=n;
					$('#botaopaf'+(btativo)).css("background", "#BBB");
				} else{
					alert("Erro! Verifique sua conexão com a internet.");
				}
		});		
	}	
function mudarordem(n){ 
	ordemAnte=ordem;
	ordem=n;
	dadosgerais();
}
function atribuirvr(valor){ 
	$("#campobusca").val(valor);
	preBusca(valor);
	ativo=0;
}

function exeBusca(valor){ 
    if(valor=="") valor=$("#campobusca").val();
	busca=valor;
	dadosgerais();
	ativo=0;
}

function preBusca(s){

}

function preloop(){	
	s=$("#campobusca").val();
	//alert("||||||"+s);
	if(s.length&gt;2 &amp;&amp; s!=temp){
		$.post('admin/produtos.php', {categoria : "index.html", buscapre:s} , function(resposta) {
					if (resposta != false) {
						//alert(resposta);
						$("#pretexto").html(resposta);
						temp=s; 
					} 
		});
	}else{
		if(s.length&lt;2) $("#pretexto").html("");
	}
	
	setTimeout("preloop();",900);
}

function ativoinativo(){
	if(ativo==0){
		ativo=1;
	}else{
		ativo=0;
	}
	
}
function saircampo(){
	if(ativo==0) $("#pretexto").html("");
	}
function dadosgerais(){
	  	$('#produtos').html('&lt;div id="fundocotAD"  style="text-align:center;"&gt;&lt;img style="margin-top:15%;" src="admin/imagens/load.gif"  alt=""/&gt;&lt;/div&gt;');
		tl=((larguradiv/210) | 0);
		ta=(((auturadiv-95)/314) | 0);
		$.post('admin/produtos.php', {contpag:1,categoria : "index.html", numpag:(tl*ta), busca:busca} , function(resposta) {
				if (resposta != false) {
					quant=resposta;
				} 
		});
		$.post('admin/produtos.php', {categoria : "index.html", pag:1, numpag:(tl*ta), ordem:ordem, busca:busca} , function(resposta) { 
				if (resposta != false) {
					resposta='&lt;div style="z-index:9999999; width:100%;height:43px;border-bottom:solid #999 2px;margin:0;margin-bottom:0px;"&gt;&lt;div style="float:left;"&gt;&lt;p style="font-family:\'Gill Sans\', \'Gill Sans MT\', \'Myriad Pro\', \'DejaVu Sans Condensed\', Helvetica, Arial, sans-serif;margin:0;margin-top:12px;"&gt;Ordenar por: &lt;select style="height:22px;" onChange="mudarordem(this.value);" id="ordemmudar";&gt;&lt;option value="ultimosadd"&gt;Últimos Adicionados&lt;/option&gt;  &lt;option value="menorpreco"&gt;Menor preço&lt;/option&gt;&lt;option value="maiorpreco"&gt;Maior preço&lt;/option&gt;&lt;option value="nomeaz"&gt;Nome A-Z&lt;/option&gt;&lt;option value="nomeza"&gt;Nome Z-A&lt;/option&gt;&lt;/select&gt;&lt;/p&gt;&lt;/div&gt;&lt;div style="float:right;margin-right:5px;"&gt;&lt;input  type="text" placeholder="Pesquisar..." style="width:240px;height:22px;margin-top:7px;padding-left:2px" id="campobusca" onBlur="saircampo();" onKeyUp="preBusca(this.value);" value="'+busca+'" title="Pesquise aqui!" maxlength="120"&gt;&lt;input onClick="exeBusca(\'\');" type="button" style="cursor:pointer;" value="Buscar"&gt;&lt;div id="pretexto" style="width:244px; height:auto;z-index:9999999;position:absolute;  background:#FFF;border-top:none;border:1px #CCC solid;"&gt; &lt;/div&gt;&lt;/div&gt;&lt;/div&gt;&lt;div id="listadeprod" style="height:'+(314*ta+10)+'px;margin-left:'+(((larguradiv-(212*tl)-25) * 0.5) | 0)+'px;"&gt;'+resposta+'&lt;/div&gt;&lt;div style="width:100%;height:43px;border-top:solid #999 2px;margin:0;"&gt;&lt;p style="width:90%;margin-left:5%;height:26px; text-align:center;margin-top:7px;overflow: hidden; "&gt;';
					for(var i=1;i&lt;=quant;i++){
						resposta+='&lt;input id="botaopaf'+(i)+'" class="botaopag"';
						if(i==1) resposta+='style="background:#BBB;"';;
						resposta+='type="button" onClick="trocarPag('+(i)+');" value="'+(i)+'"&gt;';
					}
					resposta+='&lt;/p&gt;&lt;/div&gt;'; 
					$('#produtos').html(resposta);
					$("#ordemmudar option[value='"+ordemAnte+"']").attr("selected", false);
					$("#ordemmudar option[value='"+ordem+"']").attr("selected", true);
					preloop();
				} 
		 });	 	
	
}
