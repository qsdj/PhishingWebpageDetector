   function validation()
{
var ssk=document.form1;

if(ssk.lname.value=='')
{
alert("Enter Your Name");
ssk.lname.focus();
return false;
}

if(ssk.email.value=='')
{
alert("Enter E-mail Address");
ssk.email.focus();
return false;
}
if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(form1.email.value))
{
//ok;
}
else
{
alert("Invalid E-mail Id! Pleas Re-Enter");
ssk.email.focus();
return false;
}

if(ssk.message.value=='')
{
alert("Enter Your Message");
ssk.message.focus();
return false;
}
}
  </script>
