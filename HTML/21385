						if (!window.hasOwnProperty('lens_static_resources')) return;
						window.lens_dynamic_loaded_scripts = {"google-maps-api":"http:\/\/maps.google.com\/maps\/api\/js?sensor=false&amp;amp;language=en","wpgrade-main-scripts":"http:\/\/fotocerrado.com.br\/wp-content\/themes\/lens\/theme-content\/js\/main.js","contact-form-7":"http:\/\/fotocerrado.com.br\/wp-content\/plugins\/contact-form-7\/includes\/js\/scripts.js","pixlikes-plugin-script":"http:\/\/fotocerrado.com.br\/wp-content\/plugins\/pixlikes\/js\/public.js","pixproof-plugin-script":"http:\/\/fotocerrado.com.br\/wp-content\/plugins\/pixproof\/js\/public.js"};
