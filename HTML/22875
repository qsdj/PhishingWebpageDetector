  <script type="text/javascript">
   window.$ = jQuery

		var dynamic_styles = ' .full-width-56634338e92d8 { min-height:100px; padding:200px 0 200px; margin-bottom:31px; } #background-layer--56634338e92d8 { background-image:url(http://nordicspice.se/wp-content/uploads/frontpagenordic.jpg); background-position:left top; background-repeat:repeat; ; } .full-width-56634338ecfc8 { min-height:0px; padding:60px 0 40px; margin-bottom:0px; border:1px solid #ffffff;border-left:none;border-right:none; } #background-layer--56634338ecfc8 { background-position:left top; background-repeat:no-repeat; position: fixed;; } .full-width-566343390076a { min-height:100px; padding:119px 0 114px; margin-bottom:0px; } #background-layer--566343390076a { background-image:url(http://nordicspice.se/wp-content/uploads/nordic-spice-middle.jpg); background-position:left top; background-repeat:no-repeat; background-attachment: fixed;; } ';
		var dynamic_styles_ids = (["ajax-56634338e92d8","ajax-56634338ecfc8","ajax-566343390076a"] != null) ? ["ajax-56634338e92d8","ajax-56634338ecfc8","ajax-566343390076a"] : [];

		var styleTag = document.createElement('style'),
			head = document.getElementsByTagName('head')[0];

			//console.log('jhonny')
		styleTag.type = 'text/css';
		styleTag.setAttribute('data-ajax', '');
		styleTag.innerHTML = dynamic_styles;
		head.appendChild(styleTag);

		$('.mk-dynamic-styles').each(function() {
			$(this).remove();
		});

		function ajaxStylesInjector() {
			$('.mk-dynamic-styles').each(function() {
				var $this = $(this),
					id = $this.attr('id'),
					commentedStyles = $this.html();
					styles = commentedStyles
							 .replace('&lt;!--', '')
							 .replace('--&gt;', '');


				if(dynamic_styles_ids.indexOf(id) === -1) {
					$('style[data-ajax]').append(styles);
					$this.remove();
				}

				dynamic_styles_ids.push(id);
			});
		};

		abb.modules.theme_header.init({id: 'mk-header',height: '90',stickyHeight: '55',stickyOffset: 'header',hasToolbar: 'false',}); 
abb.init();
  </script>
