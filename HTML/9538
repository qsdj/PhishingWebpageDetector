    <script type="text/javascript">
     var currentImage;
    var currentIndex = -1;
    var interval;
    function showImage(index){
        if(index &lt; $('#bigPic div').length){
        	var indexImage = $('#bigPic div')[index]
            if(currentImage){   
            	if(currentImage != indexImage ){
                    $(currentImage).css('z-index',2);
                    clearTimeout(myTimer);
                    $(currentImage).fadeOut(250, function() {
					    myTimer = setTimeout("showNext()", 5000);
					    $(this).css({'display':'none','z-index':1})
					});
                }
            }
            $(indexImage).css({'display':'block', 'opacity':1});
            currentImage = indexImage;
            currentIndex = index;
            $('#thumbs_banner li').removeClass('active');
            $($('#thumbs_banner li')[index]).addClass('active');
        }
    }
    
    function showNext(){
        var len = $('#bigPic div').length;
        var next = currentIndex &lt; (len-1) ? currentIndex + 1 : 0;
        showImage(next);
    }
    
    var myTimer;
    
    $(document).ready(function() {
	    myTimer = setTimeout("showNext()", 5000);
		showNext(); //loads first image
        $('#thumbs_banner li').bind('click',function(e){
        	var count = $(this).attr('rel');
        	showImage(parseInt(count)-1);
        });
	});
    </script>
   </div>
