   <script type="text/javascript">
    function addNewsletter(){
	var email = $('#TB_ajaxContent #myEmail').val();
	var filter = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
    if (filter.test(email)){
		$.ajax({
				url: 'index.php?route=common/home/addEmail', 
				type: 'post',
				data: {'myEmail':email},
				dataType: 'json',			
				success: function(json) {	
					$.ajax({
						url: 'index.php?route=common/send_email',
						type: 'post',
						data: {'email':email},
						dataType: 'json',			
						success: function(json) {
							//tb_remove();
						}
					});
				}
		});
		tb_remove();
        return true;
	}else {
        alert("Please input a valid contact email address!");
        return false;
    }
}

function addNewsletter_1(){
	var email = $('#TB_ajaxContent #myEmail').val();
	var filter = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
    if (filter.test(email)){
		$.ajax({
				url: '../eshop/index.php?route=common/home/addCustomerInformation', 
				type: 'post',
				data: $('#TB_ajaxContent #lilyCupForm input[type="text"],#TB_ajaxContent #lilyCupForm textarea,#TB_ajaxContent #lilyCupForm input[type="checkbox"]:checked'),
				dataType: 'json',			
				success: function(json) {
					
				}
		});
		tb_remove();
        return true;
	}else {
        alert("Please input a valid contact email address!");
        return false;
    }	
	
}
   </script>
