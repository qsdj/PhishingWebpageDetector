          document.getElementById('tandcCheck').innerHTML = 'Ich akzeptiere die';
        }
        document.getElementById('success').value = 'Weiter';
        document.getElementById('help').innerHTML = 'Hilfe';
        document.getElementById('error1').innerHTML = '';
        if(1100 &gt;= 1200 &amp;&amp; 1100 &lt;= 1202){
          document.getElementById('fyp_register_msg').innerHTML = '&lt;img class="error-img" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Damit Sie Ihre Karte weiterhin im Internet einsetzen k&amp;#246;nnen, m&amp;#252;ssen Sie sich nochmals registrieren.';
        }
        if (1100 == 1102 || 1100 == 1103 || 1100 == 1202){
          document.getElementById('iqafailMsg').innerHTML='&lt;img class="error-img" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Ihre Angaben entsprechen nicht den Vorgaben. Bitte &amp;#252;berpr&amp;#252;fen Sie diese auf Korrektheit und Vollst&amp;#228;ndigkeit.';
      
        }
        if (1100 == 1101){
            document.getElementById('forced_message').innerHTML='&lt;img class="error-img" border="0" alt="" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" /&gt;Damit Sie Ihre Karte weiterhin sicher und uneingeschr&amp;#228;nkt im Internet einsetzen k&amp;#246;nnen, m&amp;#252;ssen Sie sich jetzt f&amp;#252;r 3-D Secure anmelden.';
        }
      }
      
      function setItalianDisplay()
      {
        if(1100 &gt;= 1200 &amp;&amp; 1100 &lt;= 1202){
          document.getElementById('fyp_header').innerHTML = '&lt;h1 class="cuiHeadline lr-headline"&gt;Proteggete la vostra carta&lt;/h1&gt;';
