       <script type="text/rocketscript" data-rocketoptimized="true">
        flag = false;
			
			$(function(){
				
				
				
				jQuery.base64.is_unicode = false;
					
				$("#corrigerClavier").click(function(){
					$("#CodeId").val("");
					$(".postClavier").val("");
				});
			
				$(".validerClavier").click(function(){					
					
					if(this.id =="validerClavierELCL"){
						$("#identifiantRouting").val("ELCL");
					}else if(this.id =="validerClavierCLA"){
						$("#identifiantRouting").val("CLA");
					}else{
						$("#identifiantRouting").val("CLI");
					}		
					
					var aleatoire = "5";		
					$("#identifiant").val($("#identifiantIdForm").val());
							
					
						//On sauvegarde ou non l'identifiant BEL
						saveIdentifiantBel($("#identifiant").val());
					
							
					if($("#postClavier").length != 0){
						$("#postClavierXor").val(encodeBase64LCL(myXOR($("#postClavier").val(), aleatoire)));
					}else{
						$("#CodeIdXor").val(encodeBase64LCL(myXOR($("#CodeId").val(), aleatoire)));
					}
					$("#formAuthenticate").submit();	
				});	
			});
       </script>
