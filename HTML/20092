   var $ = jQuery.noConflict();
	function refreshCaptchaReserv(){
		$.ajax({
			url:'http://isfsecurities.com/wp-content/themes/hoxa/ajax-captcha.php',
			dataType:'json',
			data:{
				refresh	: 'refresh',
			},
			success:function(o){
				$(".num11").val(o[0]);
				$(".num22").val(o[1]);
			}
		});
	}
	$(function(){
		refreshCaptchaReserv();
		$(".num11").prop('disabled', true);
		$(".num22").prop('disabled', true);
	
	});
	
	$(document).ready(function(){
		$(".contact_get_btn_reserv").click(function(){
			var txtval = $(".captchavalue_reserv").val();
			var num1 = parseInt($(".num11").val());
			var num2 = parseInt($(".num22").val());
			var newtxtval = txtval;
			var sumboth = num1+num2;
			
			if(newtxtval != '')
			{
				var totalsum = parseInt(newtxtval);
				if(totalsum != sumboth)
				{
					$(".error_msg_reserv").css("display","inline-block");
					return false;
				}
				else{
					$(".error_msg_reserv").css("display","none");
				}
			}
		});
	});
	
	function refreshCaptchaQuick(){
		$.ajax({
			url:'http://isfsecurities.com/wp-content/themes/hoxa/ajax-captcha.php',
			dataType:'json',
			data:{
				refresh	: 'refresh',
			},
			success:function(o){
				$(".num1").val(o[0]);
				$(".num2").val(o[1]);
			}
		});
	}
	$(function(){
		refreshCaptchaQuick();
		$(".num1").prop('disabled', true);
		$(".num2").prop('disabled', true);
	
	});
	
