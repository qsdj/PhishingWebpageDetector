  <script type="text/javascript">
   (function () {
        var base = document.getElementById('unsupported-browsers'),
            modal = document.getElementById('unsupported-browsers-modal'),
            modalBackdrop = document.getElementById('unsupported-browsers-backdrop'),
            dismiss = document.getElementById('unsupported-browsers-dismiss');

        function isBrowserUnsupported() {
            var userAgent = navigator.userAgent,
                unsupportedBrowsers = [/MSIE\s6\.0/i, /MSIE\s7\.0/i, /MSIE\s8\.0/i],
                i = 0;

            for (i; i &lt; unsupportedBrowsers.length; i++) {
                if (unsupportedBrowsers[i].test(userAgent)) {
                    return true;
                }
            }
            return false;
        }

        function setCookie() {
            var tomorrow = new Date();
            tomorrow.setDate(tomorrow.getDate() + 1);
            document.cookie = [
                'unsupportedbrowser=1',
                '; expires=' + tomorrow.toUTCString(),
                '; path=/',
                '; domain=' + document.domain
            ].join('')
        }

        function getCookie(cname) {
            var name = cname + '=';
            var ca = document.cookie.split(';');
            for (var i = 0; i &lt; ca.length; i++) {
                var c = ca[i];
                while (c.charAt(0) === ' ') {
                    c = c.substring(1)
                }
                if (c.indexOf(name) !== -1) {
                    return c.substring(name.length,c.length);
                }
            }
            return '';
        }

        function showModal() {
            var images = modal.getElementsByTagName('img');

            for (var i = 0; i &lt; images.length; i++) {
                images[i].src = images[i].getAttribute('data-src');
            }

            modal.className = modal.className + ' fade in';
            modalBackdrop.className = modalBackdrop.className + ' fade in';
        }

        function hideModal() {
            modal.className = modal.className.replace(' fade in', '');
            modalBackdrop.className = modalBackdrop.className.replace(' fade in', '');
        }

        function attachHandlers() {
            dismiss.onclick = hideModal;
        }

        if (isBrowserUnsupported()) {
            if (getCookie('unsupportedbrowser') !== '') {
                return;
            }

            showModal();
            attachHandlers();
            setCookie();
        }
    })();
  </script>
