      <div class="content">
       <article class="post-137 post type-post status-publish format-standard has-post-thumbnail hentry category-administracao post-listing">
        <div class="single-post-thumb">
         <img width="620" height="330" alt="Administração Pública" class="attachment-slider wp-post-image" src="http://sitedoadministrador.com.br/wp-content/uploads/2015/04/Administra%C3%A7%C3%A3o-P%C3%BAblica-620x330.jpg" />
        </div>
        <div class="post-inner">
         <h1 itemtype="http://schema.org/Thing" itemscope="" itemprop="itemReviewed" class="name post-title entry-title">
          <span itemprop="name">
           Administração Pública
          </span>
         </h1>
         <div class="entry">
          <!-- Quick Adsense WordPress Plugin: http://quicksense.net/ -->
          <div style="float:left;margin:10px 10px 10px 0;">
           <script src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js" async="">
           </script>
           <!-- stadm-artigostopo -->
           <ins data-ad-slot="2072432805" data-ad-client="ca-pub-8596328875598739" style="display:inline-block;width:200px;height:90px" class="adsbygoogle" data-adsbygoogle-status="done">
            <ins style="display:inline-table;border:none;height:90px;margin:0;padding:0;position:relative;visibility:visible;width:200px;background-color:transparent" id="aswift_2_expand">
             <ins style="display:block;border:none;height:90px;margin:0;padding:0;position:relative;visibility:visible;width:200px;background-color:transparent" id="aswift_2_anchor">
              <iframe width="200" height="90" frameborder="0" style="left:0;position:absolute;top:0;" name="aswift_2" id="aswift_2" onload="var i=this.id,s=window.google_iframe_oncopy,H=s&&s.handlers,h=H&&H[i],w=this.contentWindow,d;try{d=w.document}catch(e){}if(h&&d&&(!d.body||!d.body.firstChild)){if(h.call){setTimeout(h,0)}else if(h.match){try{h=s.upd(h,i)}catch(e){}w.location.replace(h)}}" allowfullscreen="true" scrolling="no" allowtransparency="true" hspace="0" vspace="0" marginheight="0" marginwidth="0">
              </iframe>
             </ins>
            </ins>
           </ins>
           <script>
            (adsbygoogle = window.adsbygoogle || []).push({});
           </script>
           <script src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js" async="">
           </script>
           <!-- stadm-artigostopo -->
           <ins data-ad-slot="2072432805" data-ad-client="ca-pub-8596328875598739" style="display:inline-block;width:200px;height:90px" class="adsbygoogle" data-adsbygoogle-status="done">
            <ins style="display:inline-table;border:none;height:90px;margin:0;padding:0;position:relative;visibility:visible;width:200px;background-color:transparent" id="aswift_3_expand">
             <ins style="display:block;border:none;height:90px;margin:0;padding:0;position:relative;visibility:visible;width:200px;background-color:transparent" id="aswift_3_anchor">
              <iframe width="200" height="90" frameborder="0" style="left:0;position:absolute;top:0;" name="aswift_3" id="aswift_3" onload="var i=this.id,s=window.google_iframe_oncopy,H=s&&s.handlers,h=H&&H[i],w=this.contentWindow,d;try{d=w.document}catch(e){}if(h&&d&&(!d.body||!d.body.firstChild)){if(h.call){setTimeout(h,0)}else if(h.match){try{h=s.upd(h,i)}catch(e){}w.location.replace(h)}}" allowfullscreen="true" scrolling="no" allowtransparency="true" hspace="0" vspace="0" marginheight="0" marginwidth="0">
