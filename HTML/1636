  <script type="text/javascript">
   function weekly_pick_pass(plan,price)
		{
		$('#hid_plan').val(plan);
		$('#hid_plan_price').val(price);
		$('#weekly_price').show();
		$('#monthly_price').hide();
		$('#yearly_price').hide();
		}
		function monthly_pick_pass(plan,price)
		{
		$('#hid_plan').val(plan);
		$('#hid_plan_price').val(price);
		$('#weekly_price').hide();
		$('#monthly_price').show();
		$('#yearly_price').hide();
		}
		function yearly_pick_pass(plan,price)
		{
		$('#hid_plan').val(plan);
		$('#hid_plan_price').val(price);
		$('#weekly_price').hide();
		$('#monthly_price').hide();
		$('#yearly_price').show();
		}
	function subscribe(a,b,e,f)
	{	
		var c						=	$('#hid_plan').val();
		var d						=	$('#hid_plan_price').val();
		var acc_bal					=	'0';
		var available_amount_f		=	'0';
		
		//alert(d);
		if(a=="") { alert("Please Login before you select the subscription package"); return false; 	}
		if(b=='') { 	alert("Sorry you cannot buy your packages here");return false; }
		if(b!='') 
		{
		if(parseFloat(acc_bal)==0 || parseFloat(acc_bal)&lt;0 &amp;&amp;  parseFloat(available_amount_f)==0)
		{
		alert("Your account balance is $0.00 USD and you have to deposit some amount to purchase this package."); 
		return false; 
		}
		if(parseFloat(d)&gt;parseFloat(acc_bal))
		{
		if(parseFloat(d)&gt;parseFloat(available_amount_f))
		{
		alert("You have insufficient balance to purchase this package.Your account balance is $"+acc_bal+" USD"); 
		return false; 
		}
		}
		
		if(parseFloat(acc_bal)==0)
		{
		if(parseFloat(d)&gt;parseFloat(available_amount_f))
		{
		alert("You have insufficient balance to purchase this package.Your free credit balance is $"+available_amount_f+" USD"); 
		return false; 
		}
		}
		
		}
		
		var answer = confirm ("Are you sure you want to purchase this package ")
		if (answer)
		{
		$.ajax({
		url:'add_subscription.php?subscribe='+c+'&amp;amt='+d+"&amp;uid="+e+"&amp;sid="+f,
		success:function(res)
		{
		//alert(res);
		if(res.trim()=='error')
		{
		alert("This Handicapper have holidays at the moment. Please choose other Handicapper.");
		window.location.href='myaccount.php';
		}
		else
		{
		window.location.href='cart.php?subscribe='+res;
		}
		}
		});

		} 
	} 	

	function add_gurantee_picks(pick_id,d,pick_uid)
	{
		var ses_id					=	'';
		var hid_i					=	'1';
		var acc_bal					=	'0';
		var available_amount_f		=	'0';
		var num_subscription		=	'0';
		
		//alert(acc_bal);
		//alert(d);
		if(ses_id=="")
		{
		alert('Please Login before you select this pick');
		return false;
		}
		if(parseFloat(acc_bal)==0 &amp;&amp; parseFloat(available_amount_f)==0)
		{
		alert("Your account balance is $0.00 USD and you have to deposit some amount to purchase this pick."); 
		return false; 
		}
		if((parseFloat(d)&gt;parseFloat(acc_bal)))
		{
		if(parseFloat(d)&gt;parseFloat(available_amount_f))
		{
		alert("You have insufficient balance to purchase this pick.Your account balance is $"+acc_bal+" USD"); 
		return false; 
		}
		}
		if(parseFloat(acc_bal)==0)
		{
		if(parseFloat(d)&gt;parseFloat(available_amount_f))
		{
		alert("You have insufficient balance to purchase this pick.Your account balance is $"+available_amount_f+" USD"); 
		return false; 
		}
		}
		var answer = confirm ("Are you sure you want to add this pick to your cart ")
		if(answer)
		{
		$.ajax({
		url:"add_cart.php?pickid="+pick_id,
		success:function(res)
		{
		var res_split		=	 res.split(',');
		if(res_split[0].trim()=='error')
		{
		alert("This pick is already in your cart");
		$('#cart_div').html(res_split[1]);
		}
		if(res_split[0].trim()=='success')
		{
		alert("This pick is added in your cart");
		//window.location.href="pick_cart.php";
			$('#cart_div').html(res_split[1]);
		$('#pick_choose_cart'+pick_id).hide()
		$('#icon_added'+pick_id).show()
		
		//$('#icon_added'+pick_id).removeClass('icon-shopping-cart icon-white');
		//$('#icon_plus'+pick_id).addClass('icon-ok icon-white');
		}
		}
		});
		}
		else return false;
		}
	

      


		
		
		
