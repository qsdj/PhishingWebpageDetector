		});
				jQuery("body").stop().css({ 'overflow-y' : 'auto' });
				/* jQuery(".pf_img").hover(function() {
			jQuery(this).stop().animate({ opacity: "0.65" }, 300);
		},function(){
			jQuery(this).stop().animate({ opacity: "1" }, 300);
		}); */
		
