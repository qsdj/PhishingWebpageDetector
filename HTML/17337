var link;
function mostra_empresa()
{
   var sel1, link1;
   sel1=document.form_opcoes.empresa.selectedIndex;
   link1=document.form_opcoes.empresa.options[sel1].value;
   parent.link=link1;
   if (parent.link1=="nada")
      {
       document.form_opcoes.reset();
       return false;
      }
   else
      {
       document.location.href=link1;
      }
}
function mostra_tempresa()
{
   var sel1, link1;
   sel1=document.formt_opcoes.tempresa.selectedIndex;
   link1=document.formt_opcoes.tempresa.options[sel1].value;
   parent.link=link1;
   if (parent.link1=="nada")
      {
       document.formt_opcoes.reset();
       return false;
      }
   else
      {
       document.location.href=link1;
      }
}
//--&gt;
