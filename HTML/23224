     function initProLabelsTooltip(){
        var ProlabelsTooltip = new ProLabelsTooltip({
            'background' : '#000000',
            'color'      : '#FFFFFF'
        });
    }

    if (Prototype.Browser.IE) {
        Event.observe(window, 'load', function(){
            initProLabelsTooltip();
        });
    } else {
        document.observe("dom:loaded", function(){
            initProLabelsTooltip();
        });
    }
