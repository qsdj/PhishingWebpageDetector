   <script type="text/javascript" language="javascript">
    function check_search()
{
	var err_head  = "Following fields are required\n\n";
	var err_info  = "";
	var sr_no     = 1;
	
	var search_box  			= document.getElementById("s").value;

	if(search_box=="")
		{
			err_info=err_info+(sr_no++)+")  "+"Search can not be Empty\n";
		}
	
	if(err_info)
		{
			alert(err_head+err_info);
			return false;
		}

}
   </script>
