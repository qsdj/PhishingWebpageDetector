  <script type="text/javascript">
   function loginalt() {
                        var changeLink = document.getElementById('login').innerHTML;
                        changeLink = changeLink.replace("http://wordpress.org/", "http://www.ontariorvda.ca");
                        changeLink = changeLink.replace("Powered by WordPress", "Ontario RV Dealers Association");
                        document.getElementById('login').innerHTML = changeLink;
                }
                window.onload=loginalt;
  </script>
