  <script>
   slider.init();
  </script>
  <script>
   var slideIndex = 1;
var slideMax = 0;
var slideTimer;

$(function(){
	//$('.slideContent').css('display', 'none');
	$('.SlideContents').each(function(i){
		var $this = $(this);
		var index = $this.attr('index');
		//$this.css('left', 0 - ((index - 1) * 789));
		if ( index == 1 ) 
			$this.css("display", "block");
		slideMax++;
	});
	
	Slide(1);
});

function AutoSlide()
{
	Slide(slideIndex + 1);
}

function Slide(i)
{
	clearTimeout(slideTimer);
	
	slideIndex = i;
	if(slideIndex &gt; slideMax) slideIndex = 1;
	$('.SlideContents').each(function(i){
		var $this = $(this);
		var index = $this.attr('index');
		//$this.animate({'left': 0 - ((index - slideIndex) * 789)});
		if(i == (slideIndex - 1))
			$this.fadeIn("slow");
		else
			$this.fadeOut("slow");
	});
	
	$('.Slide .SlideMenus ul li').each(function(i){
		var $this = $(this);
		if(i == (slideIndex - 1))
			$this.addClass('SlideSelect');
			//$this.css('border','black');
		else
			$this.removeClass('SlideSelect');
	});
	
	slideTimer = setTimeout(AutoSlide, 5000);
}
  </script>
  <style>
   .Slide { position:relative;overflow:hidden;width:1100px;height:285px;margin:30px auto 30px auto;font:400 11px 'NanumGothic', '나눔고딕','NanumGothicWeb', '맑은 고딕', 'Malgun Gothic', Dotum; }
.Slide .SlideContents { position:absolute;width:1098px;height:250px;display:none;z-index:1;background-color:#fbfbfb;border:1px solid #000;border-top:none }
.Slide .SlideMenus { width:1100px;height:34px;z-index:2 }
.Slide .SlideMenus ul { }
.Slide .SlideMenus ul li { float:left;cursor:pointer;list-style:none;width:273px;height:32px;line-height:32px;text-align:center;background-color:#fbfbfb;border:1px solid #dddddd;border-bottom:1px solid #000;font-size:14px }
.SlideSelect { color:#000;font-weight:bold;border-top:1px solid #000000 !important;border-left:1px solid #000000 !important;border-right:1px solid #000000 !important;border-bottom:1px solid #fbfbfb !important }
.Slide .SlideContents .goods { width:219px;margin-top:35px;float:left;text-align:center;line-height:17px }
