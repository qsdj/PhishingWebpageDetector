   jQuery(document).ready(function() {
        /**
         * JS FOR FORM
         */        
        jQuery('input[type="button"].btnformsubmit').click(function(){
            reqname  = jQuery(this).attr('name');                      
            formname = jQuery(this).parents('form').attr('id');
            
            
            jQuery.ajax({
                url         :'http://feelingsmultimedia.com/requesthandler',
                data        : 'req='+reqname+'&amp;' +jQuery('#'+formname).serialize(),
                type        : 'POST',
                beforeSend  :function(){
                    if(formname == 'add_user'){
                        jQuery('.prof_process_add_user').show();
                    } else {
                        jQuery('.prof_process').show();
                    }
                    jQuery('.prof_msg_display').hide();
                    
                },
                complete:function(){
                    
                    if(formname == 'add_user'){
                        jQuery('.prof_process_add_user').hide();
                    } else {
                        jQuery('.prof_process').hide();
                    }
                },
                success:function(data){                                        
                    jQuery('#exe').html(data);
                },
                error:function(){
                    alert('error');
                }
            });

        });
        
        //jQuery('.fancybox').fancybox();
        
        jQuery('ul.slide-option').slideUp();
        jQuery('.mobile-edition').click(function() {
            jQuery('ul.slide-option').slideToggle();
        });
        
    });

