  <script type="text/javascript">
   function browserRedirect() {
	var sUserAgent = navigator.userAgent.toLowerCase();
	var bIsIpad = sUserAgent.match(/ipad/i) == "ipad";
	var bIsIphoneOs = sUserAgent.match(/iphone os/i) == "iphone os";
	var bIsMidp = sUserAgent.match(/midp/i) == "midp";
	var bIsUc7 = sUserAgent.match(/rv:1.2.3.4/i) == "rv:1.2.3.4";
	var bIsUc = sUserAgent.match(/ucweb/i) == "ucweb";
	var bIsAndroid = sUserAgent.match(/android/i) == "android";
	var bIsCE = sUserAgent.match(/windows ce/i) == "windows ce";
	var bIsWM = sUserAgent.match(/windows mobile/i) == "windows mobile";
	//document.writeln("您的浏览设备为：");
	if (bIsIpad || bIsIphoneOs || bIsMidp || bIsUc7 || bIsUc || bIsAndroid || bIsCE || bIsWM) {
	   return true;
	} else {
	   return false;
	}
}

if(browserRedirect()){	
	$(document).ready(function() {
		var url = window.location.pathname;
		var n = url.lastIndexOf('/');
		var web_file_name = url.substring(n+1, url.length);
		if(web_file_name == '' || web_file_name == 'index.php'){
		$('div,a,h2,h1,h3,p,span,li,ul,b,strong').css({'font-size':'1.2em','font-weight':'normal'});
		$('div.nav div.links a strong').css('font-size','21px');
		$('.subscribers table tr td, .subscribers form input').css('font-size','1.4em');
		$('.subscribers table tr td.font_c_mod1').css('width','176px');
		$('.subscribers table tr td input.searchsubmit').css('width','76px');
		$('.subscribers table tr td.foot_mod_bt').css('width','332px');
		$('#product span').css('font-size','12px');
		$('.title').css('padding-bottom','20px');
		$('.nav img').css('display','none');
		$('#right1').css('margin-top','60px');
		$('div.nlinks span,#slidesfooter a,div.nlinks p').css('font-size','20px');
		//$('.nav .links a').css('padding','0 8px 30px');
		$('div.footer img').css({'width':'92px','height':'26px'});
		$('div.nlinks img').css({'width':'120px','height':'26px'});
