       jQuery(document).ready(function(){jQuery("#mini-cart-wrapper-mobile").click(function(){if(jQuery('.mini-cart-content').hasClass('skip-active')){jQuery(".mini-cart-content").addClass("skip-active");}else{jQuery(".mini-cart-content").removeClass("skip-active");}});});function closeDeal(){jQuery('.deal-slider-block').hide();jQuery('.deal-slider-block').html('');}
					   function openDeal(){jQuery('.deal-slider-block').html('');jQuery('.loader-header').show();var base_url='http://www.goalsquad.com/';var url=base_url+'dailydeals/dailydeals/getSilder';jQuery.ajax({url:url,type:'post',dataType:'json',success:function(response){jQuery('.loader-header').hide();jQuery('.deal-slider-block').show();jQuery('.deal-slider-block').html(response);jQuery(document).click(function(event){if(!jQuery(event.target).closest('.deal-slider-block').length){if(jQuery('.deal-slider-block').is(":visible")){jQuery('.deal-slider-block').hide();jQuery('.deal-slider-block').html('');}}});}});}
