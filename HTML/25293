function setMarkers(map) {
			var image = new google.maps.MarkerImage('images/efc-icon.png',
		new google.maps.Size(50, 67),
		new google.maps.Point(0,0),
		new google.maps.Point(0, 50));
		var myLatLng = new google.maps.LatLng(56.1957, -117.614224);
		marker0 = new google.maps.Marker({
        position: myLatLng,
        map: map,
        icon: image,
        animation: google.maps.Animation.DROP,
		title: 'Executive Flight Centre Peace River, AB',
        zIndex: 1		
    });
	infoWindow0 = new google.maps.InfoWindow({ 
	  content: '&lt;div class="infoWindow"&gt;&lt;h3&gt;Sky North Aviation Fuels&lt;/h3&gt;&lt;table cellpadding="4" cellspacing="0"&gt;&lt;tr&gt;  &lt;td&gt;Airport Code: &lt;/td&gt;  &lt;td&gt;CYPE&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;  &lt;td valign="top"&gt;Address: &lt;/td&gt;  &lt;td&gt;Peace River Municipal Airport &lt;br /&gt;    Box 677 &lt;br /&gt;    Grimshaw, AB &lt;br /&gt;T0H 1W0 &lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;  &lt;td valign="top"&gt;Phone #: &lt;/td&gt;  &lt;td&gt;1-780-624-1911&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;  &lt;td valign="top"&gt; Fax #: &lt;/td&gt;  &lt;td&gt;1-780-624-1155&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;  &lt;td valign="top"&gt; Toll Free #: &lt;/td&gt;  &lt;td&gt;1-800-661-1911&lt;/td&gt;&lt;/tr&gt;&lt;tr&gt;  &lt;td valign="top"&gt; Unicom: &lt;/td&gt;  &lt;td&gt;129.25&lt;/td&gt;&lt;/tr&gt; &lt;/table&gt;&lt;/div&gt;'//,
	  //maxWidth: 400 
	});
	google.maps.event.addListener(marker0 , 'click', function(){ 
	 infoWindow0.open(map,marker0);
	});
			var image = new google.maps.MarkerImage('images/efc-icon.png',
		new google.maps.Size(50, 67),
		new google.maps.Point(0,0),
		new google.maps.Point(0, 50));
		var myLatLng = new google.maps.LatLng(58.838822, -122.581801);
		marker1 = new google.maps.Marker({
        position: myLatLng,
        map: map,
        icon: image,
        animation: google.maps.Animation.DROP,
		title: 'Sky North Aviation Fuels Fort Nelson',
        zIndex: 2		
    });
