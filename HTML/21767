  <script type="text/javascript">
   $(window).addEvent('load', function(){	
			(function(){
                var vtMaxHeight1 = 0;                                 
                $ES('#vt_main_inside').each(function( item ){                                 
                    if( item.offsetHeight.toInt()  &gt; vtMaxHeight1 ){
                        vtMaxHeight1 =  item.offsetHeight.toInt();
                    }
                });
                if( vtMaxHeight1 &gt; 0 ) {$ES('#vt_main_inside div.vt_auto_cols').setStyle('height', vtMaxHeight1);}              
            }).delay(100);                                   
    });
  </script>
