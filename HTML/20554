   var iAlreadySubmitted = 0
function doCheck(){
  if  (iAlreadySubmitted==0){
     // submitting for first time - set flag to stop extra submissions
     iAlreadySubmitted=1
     // submit the form
     document.eventForm.submit()
  }
}
