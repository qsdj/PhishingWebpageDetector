          document.getElementById('tandcText').innerHTML = '&lt;A HREF="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/ubs_toc_it.pdf" onClick="window.open(this.href); return false;"&gt;le disposizioni per l&amp;#145;uso di 3-D Secure.&amp;nbsp;&lt;span class="pdficon"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/pdf.png" border="0"&gt;&lt;/span&gt;&lt;/a&gt;';
          document.getElementById('tandcCheck').innerHTML = 'Accetto';
        }
        document.getElementById('success').value = 'Continua';
        document.getElementById('help').innerHTML = 'Aiuto';
        document.getElementById('error1').innerHTML = '';
        if(1100 &gt;= 1200 &amp;&amp; 1100 &lt;= 1202){
          document.getElementById('fyp_register_msg').innerHTML = '&lt;img class="error-img" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Affinch&amp;#233; possiate utilizzare nuovamente la vostra carta per acquisti online dovete registrarvi di nuovo.';
        }
      
        if (1100 == 1102 || 1100 == 1103 || 1100 == 1202){
          document.getElementById('iqafailMsg').innerHTML='&lt;img class="error-img" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;I vostri dati non corrispondono. Controllate se sono corretti e completi.';
      
        }
        if (1100 == 1101){
