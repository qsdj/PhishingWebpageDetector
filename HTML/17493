function Down() { 
		sec--;    
		if (sec == -1) { sec = 59; min--; }
		if (sec == 0 &amp;&amp; min == 0) {
		alert("We're sorry, but due to an extended period of inactivity you have been logged out of the AppleID Creation process. Click OK to return to the sign in page.");
		window.location.href = document.getElementById('jive').value;
	}
	else down = setTimeout("Down()", 1000);
	}
	function timeIt() {
		min = 5;
		sec = 0;
		Down();
	}
	//Code added to avoid overlapping of autofill data with placeholder
	 Event.observe(window, 'load', function() {
	 var appleId=document.getElementById("accountname");
	 var password=document.getElementById("accountpassword");
	 if(appleId!=null)
	 {
	 var placeholderId=appleId.nextSibling;
	 if(appleId.value.length&gt;0)
	 placeholderId.style.display="none";
	 }
	 if(password!=null)
	 {
	 var placeholderPass=password.nextSibling;
	 if(password.value.length&gt;0)
	 placeholderPass.style.display="none";
	 }

     });
   </script>
