        document.getElementById('card').innerHTML = 'Num&amp;#233;ro de carte';
 
        if((1100 &gt;= 1100) &amp;&amp;  (1100 &lt;= 1103)) {
          document.getElementById('tandcText').innerHTML = '&lt;A HREF="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/ubs_toc_fr.pdf" onClick="window.open(this.href); return false;"&gt;les dispositions relatives &amp;#224; 3-D Secure.&amp;nbsp;&lt;span class="pdficon"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/pdf.png" border="0"&gt;&lt;/span&gt;&lt;/a&gt;';
