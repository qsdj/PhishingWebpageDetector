   <script type="text/javascript">
    $(function(){
			$(window).scroll(function(){
				if($(this).scrollTop()&gt;600){
				  $('#back_top').fadeIn();
				}
				else{
				  $('#back_top').fadeOut();
				}
			});
			$("#back_top").click(function (e) {
				e.preventDefault();
				$('body,html').animate({scrollTop:0},800,'swing');
			});
        });
   </script>
