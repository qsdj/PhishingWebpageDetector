                  ( function ($) {
					$( document ).ready( function ()
					{
						$( '#accordion_Qr34ZT .panel-collapse' ).click( function(e ){
							var collapse_item = $( '#accordion_Qr34ZT '+this.hash )
							$( '#accordion_Qr34ZT .panel-body' ).each(function(){
