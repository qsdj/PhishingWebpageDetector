    <script type="text/javascript">
     &lt;!--
$(window).load(function() {
	boss_quick_shop();
	
	/*$('.display').bind('click', function() {
		$('.sft_quickshop_icon').remove();
		boss_quick_shop();
	});*/
});

function boss_quick_shop(){
	
		$('.box-product&gt;li').each(function(index, value){
		
			var id_product='';
			var reloadurl=false;
			if($(".image a",this).attr('href')){
				var href_pro = $(".image a",this).attr('href'); 
			}else{ 
				var href_pro = '';
			}
			if	(href_pro){
				var check=href_pro.match("index.php"); 
			}
			var last_index = '';
			var product_id = 0;
			if(check=="index.php"){	 //direct link
				var str = href_pro.split("&amp;");
				for (var i=0;i&lt;str.length;i++){
					if(str[i].match("product_id=") =="product_id="){
					
						last_index = str[i];
						var id = last_index.split("=");
						product_id = id[1];
						break;
					}
				}
				reloadurl=true;
			}else{	//mode SEO
				var check_seo = href_pro.match("product_id=");
				if(check_seo=="product_id="){
				
					var str = href_pro.split("&amp;");
					for (var i=0; i&lt;str.length; i++){
						if(str[i].match("product_id=") == "product_id="){
							var temp = str[i].split("?");
							last_index = temp[temp.length-1]; // lay phan tu cuoi cung
							var id = last_index.split("=");
							product_id = id[1];
							break;
						}
					}
					
					
					reloadurl=true;
				}else{
				
					var str_1 = href_pro.split("/");
					var str_2 = str_1[str_1.length-1]; 
					
					var temp = str_2.split("?");
					last_index = temp[0];
					var id_index = '';
											if(last_index=="A_Little_Ballerina_Surprise_Dress"){
							id_index = "product_id=34";
						}
											if(last_index=="Criss_Cross_Romper"){
							id_index = "product_id=30";
						}
											if(last_index=="Pin_Striped_Romper"){
							id_index = "product_id=48";
						}
											if(last_index=="The_Bubble_Dress "){
							id_index = "product_id=50";
						}
											if(last_index=="Bubble-Dress"){
							id_index = "product_id=63";
						}
											if(last_index=="Tumbling_Elephant_Bubble_Dress"){
							id_index = "product_id=51";
						}
											if(last_index=="My_First_Big_Boy_Romper "){
							id_index = "product_id=41";
						}
											if(last_index=="Monkey_on_my_Romper "){
							id_index = "product_id=28";
						}
											if(last_index=="The_Snuggle_Me_Little_Kokoro_Muslin_Square "){
							id_index = "product_id=52";
						}
											if(last_index=="The_Bear_Romper "){
							id_index = "product_id=54";
						}
											if(last_index=="My_Best_Friend_Dress"){
							id_index = "product_id=40";
						}
											if(last_index=="Little_Lola_Dress"){
							id_index = "product_id=43";
						}
											if(last_index=="Little_Ballerina_Flap_Collar "){
							id_index = "product_id=32";
						}
											if(last_index=="Little_Ballerina_Dress- Printed"){
							id_index = "product_id=55";
						}
											if(last_index=="The_Cool_Monkey_Shirt _linen"){
							id_index = "product_id=56";
						}
											if(last_index=="My_Little_Monkey_Shirt"){
							id_index = "product_id=57";
						}
											if(last_index=="The_Big_Boy_Check_Shirt "){
							id_index = "product_id=58";
						}
											if(last_index=="The_Big_Boy_Linen_Shirt "){
							id_index = "product_id=59";
						}
											if(last_index=="Pleated_Pink_Girl_Dress"){
							id_index = "product_id=65";
						}
											if(last_index=="Zoom_to_the_moon_shirt "){
							id_index = "product_id=62";
						}
											if(last_index=="Criss-Cross-Romper"){
							id_index = "product_id=61";
						}
											if(last_index=="Big-Boy-Romper"){
							id_index = "product_id=66";
						}
											if(last_index=="Pin-Striped"){
							id_index = "product_id=67";
						}
											if(last_index=="Monkey-on-my-Collar-Romper"){
							id_index = "product_id=68";
						}
											if(last_index=="Bear-romper"){
							id_index = "product_id=69";
						}
											if(last_index=="Tumbling-Elephant-romper"){
							id_index = "product_id=70";
						}
											if(last_index=="Little-Ballerina-Dress-Pink"){
							id_index = "product_id=71";
						}
											if(last_index=="the_zoom_to_the_moon_linen_shirt-unisex"){
							id_index = "product_id=73";
						}
											if(last_index=="a_linen_shirt-unisex"){
							id_index = "product_id=75";
						}
											if(last_index=="cool_monkey_shirt_linen-unisex"){
							id_index = "product_id=72";
						}
											if(last_index=="a_check_shirt-unisex"){
							id_index = "product_id=74";
						}
										if(id_index!=''){
						var id = id_index.split('=');
						product_id = id[1];	
						reloadurl=true;
					}
				}
			}
			if(reloadurl){
			
				var _qsHref = '&lt;a title="Quick Shop" onclick="getModalContent('+product_id+');" class=\"sft_quickshop_icon\" data-toggle="modal" data-target="#myModal" style=\"position:absolute;\"&gt;Quick Shop&lt;/a&gt;';
				
				$('.image',this).prepend(_qsHref);	
				
				var quick_button = $('a.sft_quickshop_icon');
				
				var width_button = (quick_button.width() + 0)/2 ;
				var height_button = (quick_button.height() + 0)/2;
				
		      	$('.sft_quickshop_icon').css({
					'margin-left': - width_button + 'px',
					'margin-top': - height_button +'px',
					'top': '50%',
					'left': '50%'
				});

			}
	   });
	var content_modal = '&lt;div id="myModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;"&gt;&lt;div class="modal-dialog" style="width:90%; max-width:1000px;"&gt;&lt;/div&gt;&lt;/div&gt;&lt;div class="loading" style="position:fixed;top:50%;left:50%"&gt;&lt;/div&gt;';
	$('#content').append(content_modal);	
}

function getModalContent(product_id){
	$.ajax({
		url: 'index.php?route=module/boss_quick_shop_product/&amp;product_id=' + product_id,
		dataType: 'json',
		beforeSend: function() {
			$('.loading').html('&lt;span class="wait"&gt;&amp;nbsp;&lt;img src="catalog/view/theme/default/image/loading.gif" alt="" /&gt;&lt;/span&gt;');
			$('#myModal .modal-dialog').html('');
		},		
		complete: function() {
			$('.wait').remove();
		},
		success: function(json) {
			$('#myModal .modal-dialog').html(json['html']);
		}
	});

}
