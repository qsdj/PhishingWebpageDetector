//messages

var messager_timer;

function show_messager(){
   $('#helper_warning').animate({
       top: '+=31px'
   }); 
    $('#messagebar').slideDown();    
}

function hide_messager(){
   $('#helper_warning').animate({
       top: '-=31px'
   }); 
    $('#messagebar').slideUp();
}

function show_notificationsbar(){
    $('#notifications_popup').show();
}
function hide_notificationsbar(){
    $('#notifications_popup').hide();
}
function toggle_notificationsbar(){
    if ($(window).width() &lt; 500) {
        window.location.href = "/historyfeed";
        return;
    }
    $('#notifications_popup').toggle();

    if ($('#notifications_popup').is(':visible') &amp;&amp; $('#notifications_counter').text() != "0") {
        console.log('mark all in 2000');
        setTimeout(mark_all_as_read, 2000);

    }
}

function check_notification(){
    if ($('#notifications_counter').text()  == "0") {
        $('#notifictions_count').addClass('zero_notification');
    } else {
        $('#notifictions_count').removeClass('zero_notification');
    }
}


