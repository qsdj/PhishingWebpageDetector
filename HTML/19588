        <script type="text/javascript">
         jQuery(function(){
         doTimer()
        })
var t;
var timer_is_on=0;

function timedCount()
{
jQuery('.rg-image-nav-next').click()
t=setTimeout("timedCount()",3000);
}

function doTimer()
{
if (!timer_is_on)
{
timer_is_on=1;
timedCount(3000);
}
}

function stopCount()
{
clearTimeout(t);
timer_is_on=0;
}
        </script>
