		return false;
	}
	
	 if(document.f1.email.value=="")
  {
	window.alert('Please Enter Email.');
	document.f1.email.focus();
	return false;
  }
  if(/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(document.f1.email.value))
   	{

   	}
   	else
    {
	   alert("Invalid E-mail Address! Re-enter Your E-mail.");
	   document.f1.email.focus();

	   return false;
   	}
	
	if(document.f1.comment.value=="")
	{
		window.alert('Please Enter Comment about this Blog !');
		document.f1.comment.focus();
		return false;
	}
	if(document.f1.secCode.value=="" )
	{
		alert('Please enter the number');
		document.f1.secCode.focus();
		return false;
	}
}
