  <div id="boxes">
   <!-- Do not remove div#mask, because you'll need it to fill the whole screen -->
   <div id="mask">
   </div>
   <!-- #customize your modal window here -->
   <div style="height:auto;width:750px;" class="pb_window" id="dialog">
    <div id="product_data">
    </div>
   </div>
  </div>
  <script type="text/javascript">
   jQuery(document).ready(function() {
		var pbBaseUrl = 'http://www.goalsquad.com/productbook/index/';
		var pbClass = '.main-container .product-image';
		/* quick view text come from config */
		var pb_caption = '+ QUICK VIEW';
		if (jQuery.trim(pb_caption) == '') {
			pb_caption = 'Quickshop'; 
		}
		var prod_image = jQuery(pbClass);
		/*Create the wrapper
		prod_image.each(function() {
			if(jQuery(this).closest('table').attr('class') != 'data-table cart-table') {
				var wrap_h = jQuery(this).find('img').height();
				var wrap_w = jQuery(this).find('img').width();
				//console.log(jQuery(this).find('img'));
				jQuery(this).find('img').wrap('&lt;div class="pb_wrapper" /&gt;').after('&lt;div class="ib ic  pb_caption" style="top:85px"&gt;&lt;a href="#" class="pb_btn"&gt;&lt;/a&gt;&lt;/div&gt;');
				/*Set the wrapper height/width
				jQuery(".pb_wrapper").css({'height': wrap_h, 'width': wrap_w});
			}
		});*/


		/*Hover effect
		jQuery('.item').hover(
			function(){
				jQuery(this).find('.pb_caption').show();
				//jQuery(this).find('img').animate({opacity: ".6"}, 300);		
				jQuery(this).find('.pb_caption').animate({top:"-4px"}, 100);			
			}, function(){
				//jQuery(this).find('img').animate({opacity: "1.0"}, 300);					
				jQuery(this).find('.pb_caption').animate({top:"85px"}, 100);
			}	
		);*/

		jQuery(document).on("click", ".pb_caption", function(e) {
			e.preventDefault();
			var ID = jQuery(this).parents('div[class^="product-image"]').find('a').attr("id").split("pb_item_");
			if (!ID[1] || ID[1] == "" || ID[1] == "undefined") {
				return false;
			}
			var _this = jQuery(this);
			var org_caption = _this.find(".pb_btn").text();
				
			jQuery.ajax({
				type: "POST",
				url: pbBaseUrl+"index",
				data: "id="+ID[1],
				beforeSend: function() {
					//_this.find(".pb_btn").text("Loading...");
					//Get the screen height and width
					var maskHeight = jQuery(document).height();
					var maskWidth = jQuery(window).width();
				 
					//Set height and width to mask to fill up the whole screen
					jQuery('#mask').css({'width':maskWidth,'height':maskHeight});
					//transition effect    
					jQuery('#mask').fadeIn('fast');
					jQuery('#mask').fadeTo("slow",1.0); 
				}
			}).done(function(msg){
				_this.find(".pb_btn").text(org_caption);
				jQuery('#product_data').empty().append(msg);
				
				//Get the A tag
				var id = "#dialog";//jQuery(this).attr('href');
			 
				//Get the screen height and width
				var maskHeight = jQuery(document).height();
				var maskWidth = jQuery(window).width();
			 
				//Set height and width to mask to fill up the whole screen
				jQuery('#mask').css({'width':maskWidth,'height':maskHeight});
				 
				//transition effect    
				jQuery('#mask').fadeIn('fast');
				jQuery('#mask').fadeTo("slow",1.0); 
			 
				//Get the window height and width
				var winH = jQuery(window).height();
				var winW = jQuery(window).width();
					   
				//Set the popup window to center
				//jQuery(id).css('top',  winH/2-jQuery(id).height()/2);
				jQuery(id).css('top', '100px');
				jQuery(id).css('left', winW/2-jQuery(id).width()/2);
			 
				//transition effect
				jQuery(id).fadeIn('fast');
			});
		 
		});
		
		jQuery(document).on("click", ".pb-btn-cart-view", function() {
			var _this = jQuery(this);
			var ID = _this.attr("ID").split("pb_cart_");
			var pid = ID[1];
			var qty = jQuery('#qty_'+pid).val();
			
			_this.find("span span").html('Adding...');
			jQuery.ajax({
				type: "POST",
				url: pbBaseUrl+"addtocart",
				dataType: "json",
				data: 'product='+pid+'&amp;qty='+qty,
			}).done(function(data) {
				try {
					if (data.status == "success") {
						//jQuery('#mini-cart-wrapper-regular').html(data.sidebar);
						 if(jQuery('#mini-cart')){
							jQuery('#mini-cart').replaceWith(data.sidebar); 
						}
						jQuery('#mini-cart .mini-cart-content').show();
						/* close window */
						jQuery('#product_data').empty();
						jQuery('.pb_window').hide();
						jQuery('#popup_content_cart').html(data.productInfo);
						showPBP();
						//closePopup();
						/*_this.find("span span").html('In cart');
						jQuery(".pb_crt_cnt").html(data.cart_qty);
						jQuery(".pb_crt_total").html(data.cart_total);*/
					} else if (data.status == "redirect") {
						window.location.href = data.url;
					} else {
						jQuery("#custom-msg").css("display", "block");
						jQuery(".error_message").text(data.message);
						_this.find("span span").html('Add to cart');    
					}
				} catch(e) {
					//
				}
			});
		});
		//if close button is clicked
		jQuery(document).on("click", '#pb_close', function(e) {
			//Cancel the link behavior
			e.preventDefault();
			closePB();
		});    

		//if mask is clicked
		jQuery(document).on("click", '#mask', function(e) {
			e.preventDefault();
			closePB();
		});

		jQuery(document).keyup(function(e) {
			if (e.keyCode == 27) {
				closePB();
			}
		});
		
		function closePB() {
			jQuery('#product_data').empty();
			jQuery('#mask, .pb_window').hide();
			if(pbpStatus == 1) {
				jQuery("#toCartPopup").fadeOut("normal");
				pbpStatus = 0;
			}
		}
		function showPBP() {
			loadPBP(); 
            return false; 
        }

        var pbpStatus = 0; // set value

        function loadPBP() {
            if(pbpStatus == 0) { // if value is 0, show popup
			console.log('fff');
                jQuery("#toCartPopup").fadeIn(0500); // fadein popup div
                pbpStatus = 1; // and set value to 1
				closePopup();
            }
        }
		function closePopup() {
			setTimeout(function(){ // then show popup, deley in .50 second
				closePB();// function show popup
            }, 30000);
            return false;
		}
		jQuery("div.closeCart").click(function() {
			closePB();  // function close pop up
		});
	});
  </script>
