  <script type="text/javascript">
   $(window).load(function() {
			$('.flexslider').flexslider();
		});
		function become_handicapper()
		{
		window.location.href='page.php?pname=Help';
		}
		function lets_reg()
		{
		window.location.href='register.php';
		}
		
		
		function save_subscriber()
		{
		var sub_email	=	$('#freepick_subscript_mail').val();
		if(sub_email=="")

{
$('#freepick_subscript_mail').addClass('cls_free_picks1');

return false;

}

else

{
$('#freepick_subscript_mail').removeClass('cls_free_picks1');

}

var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
if(reg.test(sub_email) == false) 

{

$('#freepick_subscript_mail').addClass('cls_free_picks1');

return false; 

}

else

{

$('#freepick_subscript_mail').removeClass('cls_free_picks1');

}
if(sub_email!="" &amp;&amp; reg.test(sub_email) == true)
{
$.ajax({
url:"save_sub_user.php?sub_email="+sub_email,
success:function(res)
{
if(res.trim()=='success')
{
$('#free_pick_text').hide();
$('#sub_pick_btn').hide();
$('#msg_display').html('Subscribed Successfully.');
$('#msg_display').css({"color":"green"});
}
if(res.trim()=='error')
{
$('#free_pick_text').hide();
$('#sub_pick_btn').hide();
$('#msg_display').html('You already subscribed.');
$('#msg_display').css({"color":"red"});
}
}
});
}
		}
		
	function add_gurantee_picks(pick_id,d)
	{
		var ses_id					=	'';
		var acc_bal					=	'0';
		var available_amount_f		=	'0';
		
		if(ses_id=="")
		{
		alert('Please Login before you select this pick');
		return false;
		}
		if(parseFloat(acc_bal)==0 &amp;&amp; parseFloat(available_amount_f)==0)
		{
		alert("Your account balance is $0.00 USD and you have to deposit some amount to purchase this pick."); 
		return false; 
		}
		if((parseFloat(d)&gt;parseFloat(acc_bal)))
		{
		if(parseFloat(d)&gt;parseFloat(available_amount_f))
		{
		alert("You have insufficient balance to purchase this pick.Your account balance is $"+acc_bal+" USD"); 
		return false; 
		}
		}
		if(parseFloat(acc_bal)==0)
		{
		if(parseFloat(d)&gt;parseFloat(available_amount_f))
		{
		alert("You have insufficient balance to purchase this pick.Your account balance is $"+available_amount_f+" USD"); 
		return false; 
		}
		}
       var answer = confirm ("Are you sure you want to add this pick to your cart ")
		if(answer)
		{
		$.ajax({
		url:"add_cart.php?pickid="+pick_id,
		success:function(res)
		{
		var res_split		=	 res.split(',');
		if(res_split[0].trim()=='error')
		{
		alert("This pick is already in your cart");
		$('#cart_div').html(res_split[1]);
		}
		if(res_split[0].trim()=='success')
		{
		alert("This pick is added in your cart");
		//window.location.href="pick_cart.php";
			$('#cart_div').html(res_split[1]);
		$('#pick_choose_cart'+pick_id).hide()
		$('#icon_added'+pick_id).show()
		
		//$('#icon_added'+pick_id).removeClass('icon-shopping-cart icon-white');
		//$('#icon_plus'+pick_id).addClass('icon-ok icon-white');
		}
		}
		});
		}
		else return false;
		}
		
		function call_handi(hid)
		{
		window.location.href="handicapper.php?hid="+hid;
		}
