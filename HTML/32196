   <script type="text/javascript">
    $( document ).ready(function() {
 
		$(".as-cartbtn").click(function()
		{
		var id = this.id;
	    var res = id.substring(4);
		
	   	var img_src = $("#imageid_"+res).attr("src");
	   //	var name = $("#name_"+res).val();
	    var name = document.getElementById("name_" + res).innerHTML;
	    var price = document.getElementById("newprice_" + res).innerHTML;
		
   		$.ajax({
					type : 'post',
					url : 'addtocart.php',
					dataType:'json',
					data : 
					{
						pid : res,
						image : img_src,
						name : name,
						price : price,
					},
					success : function(response) 
					{
						
						var data =  "&lt;li id='tb_"+response.id+"'&gt;" +
								  		"&lt;tr&gt;&lt;img class='as-cart-thumb' style='height:50px; width:50px;' src = '"+response.image+"'/&gt;" +
								  		"&lt;div class='as-cart-info'&gt;&lt;p id='name'&gt;"+response.name + "&lt;/p&gt;" +"&lt;span id='price'&gt;"+response.price + "&lt;/span&gt;" + 
                                                                                "&lt;input type='button' class='delete' value='delete' id='delete_"+response.id+"'/&gt;&lt;/div&gt;" +
								  		
								  		
								  	   "&lt;/li&gt;";
						$("#showcart").append(data);		 

						var countup = "&lt;i class='fa fa-shopping-cart'&gt;&lt;/i&gt;" + 
											"&lt;span id='cart-total'&gt;" +
												 "&lt;span id='countu' style='padding:0px;margin:0px;'&gt;" + response.totalcount+" &lt;/span&gt;"

								  	   
						$("#countup").html(countup);		 
					}
			});
		});
		
	$(".delete").click(function()
	{
		var id = this.id;
	    var res = id.substring(7);
	   
	    
	    $.ajax({
					type : 'post',
					url : 'addtocart.php',
					dataType:'json',
					data : 
					{
						did : res,
					},
	    			success : function(response) 
					{
						if(response.status==1)
						{
							
							$("#tb_"+res).remove();
						}	
					}
	    });
	});
});
   </script>
