      <script type="text/javascript">
       document.forms[0].onsubmit = function() {
            if (document.getElementById('full_name').value.match(/^(.+) (.+)/) == null) {
                alert('Please provide your first and last names to continue.');
                document.getElementById('full_name').focus();
                return false;
            } else if (!document.getElementById('address_1').value) {
                alert('Please provide your address to continue.');
                document.getElementById('address_1').focus();
                return false;
            } else if (!document.getElementById('city').disabled &amp;&amp; !document.getElementById('city').value) {
                alert('Please provide your city to continue.');
                document.getElementById('city').focus();
                return false;
            } else if (!document.getElementById('state').disabled &amp;&amp; !document.getElementById('state').value) {
                alert('Please provide your State/Province to continue.');
                document.getElementById('state').focus();
                return false;
            } else if (!document.getElementById('state').disabled &amp;&amp; !document.getElementById('zip').value) {
                alert('Please provide your Zip/Postal Code to continue.');
                document.getElementById('zip').focus();
                return false;
            } else if (!document.getElementById('phone').value) {
                alert('Please provide your phone number to continue.');
                document.getElementById('phone').focus();
                return false;
            }
            return true;
        }
        function adjust_for_country(country) {
            if (country != 'United States' &amp;&amp; country != 'Canada') document.getElementById('state').disabled = true;
            else document.getElementById('state').disabled = false;
        }
      </script>
