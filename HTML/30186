        function showHideEula(checkBox) {
          var eulaObj = document.getElementById('toolbarEula');
          var defaultTosObj = document.getElementById('defaultTos');
          if(checkBox.checked == true) {
            eulaObj.style.display='inline';
            defaultTosObj.style.display='none';
          } else {
            eulaObj.style.display='none';
            defaultTosObj.style.display='inline';
          }
        }
        function hideEula() {
          var eulaObj = document.getElementById('toolbarEula');
          var defaultTosObj = document.getElementById('defaultTos');
          eulaObj.style.display='none';
          defaultTosObj.style.display='inline';
        }
  </script>
  <style type="text/css">
   #toolbarEula {
	DISPLAY: inline
}
