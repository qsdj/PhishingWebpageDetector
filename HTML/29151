  <script language="JavaScript">
   function ShowForm() {
	var nf = document.getElementById('nakl');
	var sf = document.getElementById('zakaznakl');
	sf.style.display = "block";
	sf.style.visibility = "visible";
	
	nf.setAttribute("onclick", "HideForm()");
	
	return false;
}

function HideForm() {
	var nf = document.getElementById('nakl');
	var sf = document.getElementById('zakaznakl');

	sf.style.display = "none";
	sf.style.visibility = "hidden";
	
	nf.setAttribute("onClick", "ShowForm()");

	return false;
}
  </script>
