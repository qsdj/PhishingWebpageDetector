   function newWindow(jsp) {
		var w = screen.width - (screen.width * 0.25);
		var h = screen.height - (screen.height * 0.25);
		var top = ((screen.height * 0.25) / 2);
		var left = ((screen.width * 0.25) / 2);
		var win = window.open(jsp, 'BancsConnect',
				'resizable=yes, scrollbars=yes, top=' + top + ', left=' + left
						+ ', width=' + w + ', height=' + h);
		win.focus();
	}
  </script>
