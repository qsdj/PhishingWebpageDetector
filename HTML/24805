                <script language="JavaScript">
                 var flenu = 0;
        function add_fileu(delete_codeu)
        {
            var uploadu_count = 3;
            if (uploadu_count &amp;&amp; flenu &gt;= uploadu_count)
            {
                alert("파일첨부는 "+uploadu_count+"개 까지만 업로드가 가능합니다.");
                return;
            }

            var objTblu;
            var objRowu;
            var objCellu;
            if (document.getElementById)
                objTblu = document.getElementById("variableFilesu");
            else
                objTblu = document.all["variableFilesu"];

            objRowu = objTblu.insertRow(objTblu.rows.length);
            objCellu = objRowu.insertCell(0);
                        if (delete_codeu)
                objCellu.innerHTML += delete_codeu;
            else
            {
                                ;
            }

            flenu++;
        }


		add_fileu('');

        function del_fileu()
        {
            // file_length 이하로는 필드가 삭제되지 않아야 합니다.
            var fileu_length = 0;
            var objTblu = document.getElementById("variableFilesu");
            if (objTblu.rows.length - 1 &gt; fileu_length)
            {
                objTblu.deleteRow(objTblu.rows.length - 1);
                flenu--;
            }
        }
                </script>
               </td>
              </tr>
