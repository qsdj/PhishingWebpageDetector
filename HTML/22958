  <script>
   var hours = 04;
		var min = 31;
		var sec = 04;
		var nextDay = '06.12.2015';
		
		function display() {
			sec+=1;
			if (sec&gt;=60)
			{
				min+=1;
				sec=0;
			}
			if (min&gt;=60)
			{
				hours+=1;
				min=0;
			}
			if (hours&gt;=24){
				hours=0;
				jQuery('#date_now').text(nextDay);
			}	

			if (sec&lt;10)
				sec2display = "0"+sec;
			else
				sec2display = sec;
			
			if (min&lt;10)
				min2display = "0"+min;
			else
				min2display = min;

			if (hours&lt;10)
				hour2display = "0"+hours;
			else
				hour2display = hours;
		
			document.getElementById("time_now").innerHTML = hour2display+":"+min2display+":"+sec2display;
			setTimeout("display();", 1000);
		}

		display();
