  <script>
   var erro = "Preencha todos os campos";
function $(id){return document.getElementById(id);};
function envia()
{
	if($("nome").value!="")
	{
		if($("pessoa1").checked == true || $("pessoa2").checked == true || $("pessoa3").checked == true)
			$("form1").submit();
		else
			alert(erro);
	}else
		alert(erro);
}
function libera(){
	document.getElementById("tela").style.display="none";
}
window.onload=function(){
setTimeout("libera()",5000);
};
  </script>
