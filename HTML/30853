       //&lt;![CDATA[
window.addEvent('domready', function() {
	var spSlide160 = new slideshow_sp2($('sp-slide160'), {
		size: {width: 686, height: 300},
		interval: 7000,
		fxOptions: {duration:  1000, transition: Fx.Transitions.Expo.easeIn},
		transition: 'slide-horizontal',
		walkers: {
		container: $('handlers160'),
		depth:100,
		wmode: 'vertical',
		mouse_wheel: true,
		display:3		},
		descriptions: {items: document.getElements('#sp-slide160 div.sp-slide-desc'),
		descmode:'vertical'
		},
		onWalk: function (i, j) {
			this.walkers.items.removeClass('active');
			this.walkers.items[i].addClass('active');
		},
		onInitialized: function () {
			this.walkers.items[0].addClass('active');
		}		
	});
	var images = [
		'http://muniquerecotillo.gob.pe/images/cala1ok.jpg','http://muniquerecotillo.gob.pe/images/CCL-QUok.jpg','http://muniquerecotillo.gob.pe/images/EL1OK.jpg','http://muniquerecotillo.gob.pe/images/ciclo2ok.jpg'	
	];
	var images_elements = document.getElements('#sp-slide160 img');
	var loader = Asset.images(images, {
		onComplete: function () {
			document.getElement('.preload').style.visibility='hidden';
			spSlide160.play();
		},
		onProgress: function (i) {
			images_elements[i-1].src = images[i-1];
		}
	});
		spSlide160.addPlayerControls('previous', [$('sp_slide_prev160')]);
	spSlide160.addPlayerControls('next', [$('sp_slide_next160')]);
		
});
//]]&gt;
      </script>
