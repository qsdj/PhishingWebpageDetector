							var collapse_item = $( '#accordion_5Kuf7o '+this.hash )
							$( '#accordion_5Kuf7o .panel-body' ).each(function(){
								$( this ).addClass( 'panel-collapse' );
							});
							collapse_item.removeClass( 'panel-collapse' );
							collapse_item.attr( 'style', '' );
						});
					});
				} )( JoomlaShine.jQuery );
                 </script>
