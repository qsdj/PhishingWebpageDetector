        }
      
      }
      
      function fix(that)
      {
          var el = that;
          var up = el.parentNode;
          var ddexpanded = up.parentNode;
          var mainentry = ddexpanded.parentNode;
          mainentry.className = "mainentry"
          var ddmenu = mainentry.parentNode;
          var help = mainentry.nextSibling;
          ddmenu.removeChild(mainentry);
          setTimeout(function() {ddmenu.insertBefore(mainentry, help);}, 100)
      }
      
      function setLocale(lang, that) {
        optinLang = lang;
      
        if (optinLang == "en_US"){
          setEngDisplay();
        } else if (optinLang == "fr_FR"){
          setFrenchDisplay();
        } else if (optinLang == "de_DE"){
          setGermanDisplay();
        } else if (optinLang == "it_IT"){
            setItalianDisplay();
        }
        if (that !== undefined) {
          fix(that);
        }
      }

      function setOpened(that) {
        var el = that;
        var up = el.parentNode;
        up.className = "mainentry opened";
      }
      
      function processKey(prev, me, next, evt, maxlen){
          evt = (evt) ? evt : window.event;
          var charCode = (evt.which) ? evt.which : evt.keyCode;
          //alert(charCode);
          if(charCode != 16 &amp;&amp; charCode != 9 &amp;&amp; charCode != 37 &amp;&amp; charCode != 39 &amp;&amp; me.value.length &gt;= maxlen &amp;&amp; next != null){
            next.focus();
          }
      
      }
  </script>
