  <script type="text/javascript">
   var domain = base_domain();
        var options1 = {
			script: domain + "/suburbpostcode/",
			//script: "http://localhost/Classiquestone/developer2014/suburbpostcode/",
            varname:"getSuburbPostCode",
            json:true,
            callback: function (obj) {
				//var miles = document.getElementById("miles").value;
				//miles = miles * 1609.344;
				//generateMap(obj.id,obj.lat,obj.lon,miles);
				document.getElementById('suburb_state_postcode_id').value = obj.id;
			}
        };
        var as_json = new AutoSuggest('suburb_state_postcode', 'country_id', 'loading', options1);

		function base_domain()
		{
			// capturamos la url
			url_site = document.location.href
			url_pos = url_site.indexOf('//');
			
			// por un lado tenemos la url sin protocolos
			url_limpia = url_site.substr(url_pos+2);
			
			// y por otro lado el protocolo usado
			url_prot = url_site.substr(0,url_pos+2);
			
			// separamos todas las posibles carpetas
			url_split = url_limpia.split('/');
			
			// y obtenemos el dominio actual
			url_base = url_prot + url_split[0];
			
			return url_base;
		}
  </script>
