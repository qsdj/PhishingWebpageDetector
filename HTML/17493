    var time_left = 59;
  var cinterval;
  var startCounter = 0;
  function time_dec(){
    if(time_left&gt;0){
    	time_left--;
    }
    document.getElementById('countdown').innerHTML = time_left;
    if(time_left == 0 &amp;&amp; startCounter ==1){
      clearInterval(cinterval);
      removeAreUStilOverlay();
      startCounter = 0;
      timeoutSession('timeOut');
    }
  }

  cinterval = setInterval('time_dec()', 1000);

  var restoreSession = function(){
  	startCounter = 0;
  	resetSession();
  }
