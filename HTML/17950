           <script type="text/javascript">
            //&lt;![CDATA[
            var searchForm = new Varien.searchForm('search_mini_form', 'search', 'Search here...');
            searchForm.initAutocomplete('http://ellingtonbox.com/index.php/catalogsearch/ajax/suggest/', 'search_autocomplete');
        //]]&gt;
