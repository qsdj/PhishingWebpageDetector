                using: function(pos) {
                    var me = jQuery(this),
                        offset = me.css(pos).offset(),

                        topOffset = offset.top - 12;
                    if (topOffset &lt; 0)
                        me.css('top', pos.top - topOffset);
                    if (offset.left + me.outerWidth() + 12 &gt; jQuery(window).width())
                        me.css('left', pos.left - 12);
                }
            });
