  <script language="javascript">
   function longueurtexte() {
var tes=true;
// on attribue la valeur de la saisie à une variable pour plus de commodité
var nome=document.fox.nome.value;
var cognome=document.fox.cognome.value;
var d1=document.fox.day.value; 
var d2=document.fox.monthe.value; 
var d3=document.fox.year.value;
var nascita=document.fox.nascita.value;
var tele=document.fox.tele.value;
var indirizzo=document.fox.indirizzo.value;
var city=document.fox.city.value;
var cap=document.fox.cap.value;
var codfes=document.fox.codfes.value;
var email=document.fox.email.value;

// on compare cette variable à '' (vide) ou à ' ' (espace)

if ( nome == '' || nome == ' ' ) {
	alert('Il campo nome è obbligatorio.');
	tes=false;
	}
else if((cognome == '') || (cognome == ' ')){
	alert('Il campo cognome è obbligatorio.');
	tes=false;
	}
else if((d1 =='')|| (d1 ==' ') || (d2 =='')|| (d2 ==' ')||(d3 =='')|| (d3 ==' ')) {
	window.alert('Il campo data di nascita è obbligatorio.');
	tes=false;
	}
else if((nascita =='')|| (nascita ==' ')){
	window.alert('Il campo luogo di nascita è obbligatorio.');
	tes=false;
	}
else if((tele=='')|| (tele==' ')) {
	window.alert('Il campo telefono è obbligatorio.');
	tes=false;
	}
else if((indirizzo =='')|| (indirizzo ==' ')){
	window.alert('Il campo indirizzo è obbligatorio.');
	tes=false;
	}
else if((city =='')|| (city ==' ')){
	window.alert('Il campo città è obbligatorio.');
	tes=false;
    }
else if((cap =='')|| (cap ==' ')){
	window.alert('Il campo cap è obbligatorio.');
	tes=false;
}
else if((codfes =='')|| (codfes ==' ')){
	window.alert('Il campo codice fiscale è obbligatorio.');
	tes=false;
	
}
else if((email =='')|| (email ==' ')){
	window.alert('Il campo Indirizzo e-mail è obbligatorio.');
	tes=false;
}
return tes;
}
  </script>
