      function initLightbox(){
        new Lightbox({
            fileLoadingImage: 'https://www.imbling.com/skin/frontend/default/ma_orion2/magentothem/images/loading.gif',
            fileBottomNavCloseImage: 'https://www.imbling.com/skin/frontend/default/ma_orion2/magentothem/images/closelabel.gif',
            overlayOpacity: 0.8,   // controls transparency of shadow overlay
            animate: true,         // toggles resizing animations
            resizeSpeed: 7,        // controls the speed of the image resizing animations (1=slowest and 10=fastest)
            borderSize: 10,
            // When grouping images this is used to write: Image # of #.
            // Change it for non-english localization
            labelImage: "Image",
            labelOf: "of"
        });
    }


    if (Prototype.Browser.IE) {
        Event.observe(window, 'load', function(){ //KB927917 fix
            initLightbox();
        });
    } else {
        document.observe("dom:loaded", function(){
            initLightbox();
        });
    }
