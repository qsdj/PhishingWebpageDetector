             <script type="text/javascript">
              function qtyDown(){
			qty_el = document.getElementById('qty');
			qty = qty_el.value;
			if( !isNaN( qty ) &amp;&amp; qty &gt; 1 ){
				qty_el.value = parseInt(qty_el.value)-1			}
			return false;
		}
		function qtyUp(){
			qty_el = document.getElementById('qty');
			qty = qty_el.value;
			if( !isNaN( qty )) {
				qty_el.value = parseInt(qty_el.value)+1			}
			return false;
		}
             </script>
