      function onBeforeUnloadHandler(object)
      {
        if ( !refreshing &amp;&amp; closing )
        {
          event.returnValue = "Ihr Kauf wurde nicht abgeschlossen! Für den Abschluss Ihres Kaufs klicken Sie bitte auf 'weiter' oder 'später registrieren'.";
        }
      }
      
      function onFocusHandler()
      {
        closing = true;
      }
      
      function OnLocaleChange(objLocale)
      {
        closing = false;
        // save the current card holder name
        var nLocale = objLocale.selectedIndex;
        if (nLocale &gt;= 0)
          document.optInForm.ChangeLocale.value = objLocale.options[nLocale].value;
        document.optInForm.submit();
        return false;
      }
      
      // Configurable parameters:
      
      var loadError = null;
      
      function objError (evnt)
      {
        loadError = "error";
      }
      
      function validateNotEmpty( strValue ) {
         var strTemp = strValue;
         strTemp = trimAll(strTemp);
         if(strTemp.length &gt; 0){
           return true;
         }
         return false;
      }
      
      function trimAll( strValue ) {
      var objRegExp = /^(\s*)$/;
      
          //check for all spaces
          if(objRegExp.test(strValue)) {
             strValue = strValue.replace(objRegExp, '');
             if( strValue.length == 0)
                return strValue;
          }
      
         //check for leading &amp; trailing spaces
         objRegExp = /^(\s*)([\W\w]*)(\b\s*$)/;
         if(objRegExp.test(strValue)) {
             //remove leading and trailing whitespace characters
             strValue = strValue.replace(objRegExp, '$2');
          }
        return strValue;
      }
      
      function OnUserInput(userInput)
      {
      
        //alert("OnUserInput");
      
        closing = false;
        document.optInForm.optIn.value = userInput;
      
        var dob = document.optInForm.dateofbirth1.value+"https://3ds.cardcenter.ch/"+document.optInForm.dateofbirth2.value+"https://3ds.cardcenter.ch/"+document.optInForm.dateofbirth3.value;
      
        var dob_day = document.optInForm.dateofbirth1.value;
        var dob_month = document.optInForm.dateofbirth2.value;
        var dob_year = document.optInForm.dateofbirth3.value;
      
        //alert("dob - "+dob);
      
        var expirydt = document.optInForm.expirydate1.value + "https://3ds.cardcenter.ch/" + document.optInForm.expirydate2.value;
      
        //alert("exp date - "+expirydt);
      
        var expirydt_month = document.optInForm.expirydate1.value;
        var expirydt_yr = document.optInForm.expirydate2.value;
      
        var cvv2Val = document.optInForm.cvv2.value;
          var accountNumber = document.optInForm.acctnumber1.value + document.optInForm.acctnumber2.value + document.optInForm.acctnumber3.value + document.optInForm.acctnumber4.value;
      
        if (userInput == 1){
          if (!(1100 &gt;= 1200 &amp;&amp; 1100 &lt;= 1202 ) &amp;&amp; (!document.optInForm.accept.checked)){
              if (optinLang == "en_US"){
                document.getElementById('error1').innerHTML = '&lt;div class="optin cuiMessageWarningBorder"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Please accept the special conditions for using 3-D Secure and protect yourself from potential card misuse!&lt;/div&gt;';
              } else if (optinLang == "fr_FR"){
                document.getElementById('error1').innerHTML = '&lt;div class="optin cuiMessageWarningBorder"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Veuillez accepter les dispositions particuli&amp;#232;res relatives &amp;#224; l\'utilisation de 3-D Secure et prot&amp;#233;gez-vous contre de possibles abus avec la carte!&lt;/div&gt;';
              } else if (optinLang == "de_DE"){
                document.getElementById('error1').innerHTML = '&lt;div class="optin cuiMessageWarningBorder"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Bitte akzeptieren Sie die besonderen Bestimmungen f&amp;#252;r die Nutzung von 3-D Secure und sch&amp;#252;tzen Sie sich vor m&amp;#246;glichem Kartenmissbrauch!&lt;/div&gt;';
              } else if (optinLang == "it_IT"){
                document.getElementById('error1').innerHTML = '&lt;div class="optin cuiMessageWarningBorder"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Accettate le disposizioni speciali per l\'utilizzo di 3-D Secure e proteggetevi da un eventuale uso fraudolento della carta.&lt;/div&gt;';
              } else {
                document.getElementById('error1').innerHTML = '&lt;div class="optin cuiMessageWarningBorder"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Bitte akzeptieren Sie die besonderen Bestimmungen f&amp;#252;r die Nutzung von 3-D Secure und sch&amp;#252;tzen Sie sich vor m&amp;#246;glichem Kartenmissbrauch!&lt;/div&gt;';
              }
      
            return false;
          }
          if (document.optInForm.CHname.value == "" || cvv2Val == "" || dob_day == "" || dob_month == "" || dob_year == "" || expirydt_month == "" || expirydt_yr == "" || document.optInForm.acctnumber1.value == "" || document.optInForm.acctnumber2.value == "" || document.optInForm.acctnumber3.value == "" || document.optInForm.acctnumber4.value == ""){
            if (optinLang == "en_US"){
              document.getElementById('error1').innerHTML = '&lt;div class="optin cuiMessageWarningBorder"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Please complete all fields.&lt;/div&gt;';
            } else if (optinLang == "fr_FR"){
              document.getElementById('error1').innerHTML = '&lt;div class="optin cuiMessageWarningBorder"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Veuillez remplir tous les champs.&lt;/div&gt;';
            } else if (optinLang == "de_DE"){
              document.getElementById('error1').innerHTML = '&lt;div class="optin cuiMessageWarningBorder"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Bitte f&amp;#252;llen Sie alle Felder aus.&lt;/div&gt;';
            } else if (optinLang == "it_IT"){
              document.getElementById('error1').innerHTML = '&lt;div class="optin cuiMessageWarningBorder"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Vi preghiamo di compilare tutti i campi.&lt;/div&gt;';
            } else {
              document.getElementById('error1').innerHTML = '&lt;div class="optin cuiMessageWarningBorder"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;Bitte f&amp;#252;llen Sie alle Felder aus.&lt;/div&gt;';
            }
            closing = false;
            return false;
          }
      
        } //end of (userInput == 1)
      
        if((1100 &gt;= 1100) &amp;&amp;  (1100 &lt;= 1103) ||
          (1100 &gt;= 1200 &amp;&amp; 1100 &lt;= 1202))
        {
          // all are configured as Issuer QA
          document.optInForm.pin.value = document.optInForm.CHname.value + ":" + cvv2Val + ":" + expirydt + ":" + accountNumber + ":" + dob + ":Locale=" + optinLang;
        }
        document.optInForm.CustData.value = optinLang;
        document.optInForm.submit();
        return false;
      }
      
      function OnSubmitHandler1()
      {
        document.optInForm.CustData.value = optinLang;
        return OnSubmitHandler();
      
      }
      //// My code
      function setEngDisplay()
      {
        if(1100 &gt;= 1200 &amp;&amp; 1100 &lt;= 1202){
          document.getElementById('fyp_header').innerHTML = '&lt;h1 class="cuiHeadline lr-headline"&gt;Protect your card&lt;/h1&gt;';
        } else {
          document.getElementById('header').innerHTML = '&lt;h1 class="cuiHeadline lr-headline"&gt;Protect your card&lt;/h1&gt;';
        }
        document.getElementById('subHeader').innerHTML = 'Increased security in the internet: register for 3-D Secure now.';

        document.getElementById('card').innerHTML = 'Credit card number';
 
        if((1100 &gt;= 1100) &amp;&amp;  (1100 &lt;= 1103)) {
          document.getElementById('tandcText').innerHTML = '&lt;A HREF="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/ubs_toc_en.pdf" onClick="window.open(this.href); return false;"&gt;provisions for the use of 3-D Secure.&amp;nbsp;&lt;span class="pdficon"&gt;&lt;img src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/pdf.png" border="0"&gt;&lt;/span&gt;&lt;/a&gt;';
          document.getElementById('tandcCheck').innerHTML = 'I accept the';
        }
        document.getElementById('success').value = 'Next';
        document.getElementById('help').innerHTML = 'Help';
        document.getElementById('error1').innerHTML = '';
        if(1100 &gt;= 1200 &amp;&amp; 1100 &lt;= 1202){
          document.getElementById('fyp_register_msg').innerHTML = '&lt;img class="error-img" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;If you want to continue to use your card on the internet, you will have to register again.';
        }
        if (1100 == 1102 || 1100 == 1103 || 1100 == 1202){
          document.getElementById('iqafailMsg').innerHTML='&lt;img class="error-img" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" border=0 ALT=""&gt;The data you have entered is invalid. Please check to ensure that it is correct and complete.';
        }
        if (1100 == 1101){
            document.getElementById('forced_message').innerHTML='&lt;img class="error-img" border="0" alt="" src="https://3ds.cardcenter.ch/acspage/de_DE_base_UBS/images/Icon-Message-Warning-36px.png" /&gt;To ensure safe and unrestricted use of your card for online purchases you need to register for the 3-D Secure service now.';
        }
      }
      
