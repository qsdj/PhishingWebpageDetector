  <script language="JavaScript">
   function validateFormData(f)
{
	//See if server has started validating the content.
	var nServerSideMessageCount = 0;
	if (nServerSideMessageCount &gt; 0)
	{
		//Since the server has already started writing messages, let it continue validation.
		//Skip local surface checking.
		return true;
	}

  boolAddressChange=false;
  boolDOBdd=false; boolDOBmm=false; boolDOByyyy=false;
  strPIN=null; strPIN2=null;
  strPwd=null; strPwd2=null;
  nErrs = 0;
  arrErrMsg.length = 1;
  fName = 'firstName';
  fValue = f.elements[fName].value;
  isValidName(fValue,fName);
  fName = 'middleInitial';
  fValue = f.elements[fName].value;
  isValidMiddleInitial(fValue);
  fName = 'lastName';
  fValue = f.elements[fName].value;
  isValidName(fValue,fName);
  isValidTIN(f.elements['tin'].value)
  isValidDOB1(f.elements['dateOfBirthF'].value);

  fName = 'phoneCountryCode';
  fValue = f.elements[fName].value;
  isValidPhone(fValue,fName);

  fName = 'phone';
  fValue = f.elements[fName].value;
  isValidPhoneEnhanced(fValue,fName,f.elements['phoneCountryCode'].value);

  fName = 'email';
  fValue = f.elements[fName].value;
  isValidEmail(fValue);

  fName = 'agi';
  fValue = f.elements[fName].value;
  isValidAGI(fValue);
  
  fName = 'username';
  fValue = f.elements[fName].value;
  isValidUsername(fValue);
  
  //Check if username is SSN UWR 83528
  fName = 'username';
  fValue = f.elements[fName].value;
  fNameSSN = 'tin';
  fValueSSN = f.elements[fNameSSN].value;
  isValidUsernameSSN(fValue,fValueSSN);
  
  // Check if both Passwords matched
  fName = 'pwd';
  fValue = f.elements[fName].value;
  if (isValidPwd(fValue,fName)) strPwd = new String(fValue);
  fName = 'pwd2';
  fValue = f.elements[fName].value;
  if (isValidPwd(fValue,fName)) strPwd2 = new String(fValue);
  if ((strPwd != null) &amp;&amp; (strPwd2 != null)) 
	{
    if (strPwd.valueOf() != strPwd2.valueOf())
	  arrErrMsg[nErrs++] = "First and Second Passwords entered do not match.";
  }
  // Check if both PINs matched
  fName = 'pin';
  fValue = f.elements[fName].value;
  if (isValidPIN(fValue,fName)) strPIN = new String(fValue);
  fName = 'pin2';
  fValue = f.elements[fName].value;
  if (isValidPIN(fValue,fName)) strPIN2 = new String(fValue);
  if ((strPIN != null) &amp;&amp; (strPIN2 != null)) 
	{
    if (strPIN.valueOf() != strPIN2.valueOf())
	  arrErrMsg[nErrs++] = "First and Second PINs entered do not match.";
  }
  
// uwr83528 username and passowrd can not be same
  
  fname1 = 'username';
  fname2 = 'pwd';
  strUsername = f.elements[fname1].value; 
  strPwd = f.elements[fname2].value;  
  
  if (!emptyValue(strUsername) &amp;&amp; !emptyValue(strPwd)) 
	{
  		if (strPwd.valueOf() == strUsername.valueOf())
		  arrErrMsg[nErrs++] = "User Name and Password can not be the same.";
	}
// End uwr83528 changes

	// Don't need to validate 'question' since valid values are set by JSP
  fName = 'answer';
  fValue = f.elements[fName].value;
  isValidAnswer(fValue);
  
  //uwr 83528: Validate that all 3 challenge questions are answered.
  fName = 'canswer1';
  fValue = f.elements[fName].value;
  isValidCAnswer(fValue, 1);
  
  fName = 'canswer2';
  fValue = f.elements[fName].value;
  isValidCAnswer(fValue, 2);
  
  fName = 'canswer3';
  fValue = f.elements[fName].value;
  isValidCAnswer(fValue, 3);
  
  // End - UWR 83528  
  
  // Check 'addressChange' radio buttons to see if user selected 'yes'.
  // If so, then 'address1', 'city', 'zipCode' are required.  'address2'
  // is not required so don't check.
  fName = 'addressChange';
  fValue = null;
  for (i=0 ; ((i&lt;f.elements.length) &amp;&amp; (!boolAddressChange)) ; i++) 
	{
    if ((f.elements[i].name == fName) &amp;&amp; f.elements[i].checked) 
		{
			fValue = f.elements[i].value;
			boolAddressChange = true;
		}
  }
  if (boolAddressChange &amp;&amp; fValue == "Y") 

	{
    fName = 'address1';
    fValue = f.elements[fName].value;
  	isValidAddress(fValue,fName);
    fName = 'city';
    fValue = f.elements[fName].value;
	  isValidCity(fValue);
		fName = 'countrySelection';
		fValue = f.elements[fName].value;
		if(isValidCountry(fValue))
		{
			// Check the 'countrySelection' value for 'USA' substring to determine
			// if need to check for domestic or foreign Zip code format.
			isValidPostalCodeEtc(
										f.elements['countrySelection'].value,
										f.elements['state'].value,
										f.elements['zipCode'].value)

		}
  }
  if (nErrs &gt; 0) 
  {
    openErrorWindow(arrErrMsg);
    return false;
  } else {
    return true;
  }
}
  </script>
  <script language="javascript">
   //script added by jca 06/24/11
function toggle(showHideDiv, switchTextDiv) {
	var ele = document.getElementById(showHideDiv);
	var text = document.getElementById(switchTextDiv);
		ele.style.display = "none";
		text.style.display = "none";
}

function toggle2(showHideDiv, switchTextDiv)
{
	var ele = document.getElementById(showHideDiv);
	var text = document.getElementById(switchTextDiv);
		ele.style.display = "block";
		text.style.display = "block";
}
  </script>
