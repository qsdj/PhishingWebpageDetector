     getById('lpmod').className = getById('lpmod').className +' show-labels';
 }
 else{
     lgnEl.placeholder = "Username or Email";
     pwdEl.placeholder = "Password";
     lgnEl.type = "email";
     lgnEl.form.setAttribute("novalidate","");
     
 }
//focus
function setFocus(){
 if(lgnEl ) {
  if(lgnEl.value.length &lt; 1) {
   lgnEl.focus();
  }
  else {
   pwdEl.focus();
  }
 }
}

if(ie){
    if(noPh){
        setFocus();
    }
}
else{
    setFocus();
}
