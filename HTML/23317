				}catch(e){}
				if(portfoliofixedmenum == 2){
					onhovertouchunbindevents();
					menumousetouch.isopenspan = true;
					jQuery('#header').stop().animate({ marginLeft: "0px" }, 300);
					jQuery('#footer').stop().animate({ marginLeft: "0px" }, 300);
					jQuery('#footer_copyright').css({ padding: '20px 30px' });
					jQuery(".header-arrow").css({'display':'none'});
					jQuery('#content').css({'left':'210px'});
					jQuery('.page_description').css({'left':'210px'});
					jQuery('.cooming-soon-content').css({'left':'210px'});
					jQuery('#scrollbar2 .viewport').css({ left: "220px", 'width': (((jQuery(window).width()&lt;210)?210:jQuery(window).width())-210)+'px'});
					jQuery('.imgscontainer').stop().animate({ top: "0px", left: "220px", 'width': ((jQuery(window).width()&lt;220)?220:(jQuery(window).width()-200))+'px'});
				}
			}
		});
		//jQuery(".slides img").stop().css({ 'margin-left': jQuery(window).width() }).css({ top: '0px' }).css({ position: 'fixed' });
		
		//jQuery(".portfolio-excerpt").stop().animate({ marginLeft: "0px" }, 500);
		
	});

function resizeimageslide(ielement,isvideo,chkpushwidthtoarr){
	var iel = jQuery(ielement);
	var fg_imgareaw = ((portfoliofixedmenum==3)?(jQuery(window).width()-220):jQuery(window).width()) /*-10*/, fg_imgareah = jQuery(window).height();
	var fg_imgsize = [iel.width(), iel.height()];
	var fg_fitscreen = false;
	if(iel.hasClass("slide_horizontal")){
		fg_fitscreen = true;
	}
	if(isvideo){
		fg_imgsize = [16,9];
	}
	if(iel.is("div")){
		fg_imgsize = [fg_imgareaw, fg_imgareah];
		resizeimageslide(jQuery("img:not(.myimage)", iel),false,false);
	}
	if(fg_imgsize[0] &amp;&amp; fg_imgsize[1] &amp;&amp; fg_imgareaw &amp;&amp; fg_imgareah){
		//if(fg_imgsize[0] &lt; fg_imgareaw || fg_imgsize[1] &lt; fg_imgareah){
		var fg_imgnewsizew=0, fg_imgnewsizeh=0;
		var fg_imgscalerx = fg_imgsize[0]/fg_imgareaw, fg_imgscalery = fg_imgsize[1]/fg_imgareah;
		if(fg_fitscreen){
			if(fg_imgscalerx &lt;= fg_imgscalery){
				fg_imgnewsizew = fg_imgsize[0]/fg_imgscalery;
				fg_imgnewsizeh = fg_imgsize[1]/fg_imgscalery;
			}else{
				fg_imgnewsizew = fg_imgsize[0]/fg_imgscalerx;
				fg_imgnewsizeh = fg_imgsize[1]/fg_imgscalerx;
			}
		}else{
			if(fg_imgscalerx &lt;= fg_imgscalery){
				fg_imgnewsizew = fg_imgsize[0]/fg_imgscalerx;
				fg_imgnewsizeh = fg_imgsize[1]/fg_imgscalerx;
			}else{
				fg_imgnewsizew = fg_imgsize[0]/fg_imgscalery;
				fg_imgnewsizeh = fg_imgsize[1]/fg_imgscalery;
			}
		}
		if(fg_imgnewsizew &amp;&amp; fg_imgnewsizeh){
			fg_imgnewsizew = Math.floor(fg_imgnewsizew);
			fg_imgnewsizeh = Math.floor(fg_imgnewsizeh);
			if(!isvideo){
				if(fg_fitscreen){
					if(fg_imgscalerx &lt;= fg_imgscalery){
						iel.css({'top':'0px', 'left':'0px', 'width':fg_imgnewsizew+'px', 'height':fg_imgnewsizeh+'px'});
						iel.parent().css({'width':fg_imgnewsizew+'px', 'height':fg_imgnewsizeh+'px'});
					}else{
						iel.css({'top':Math.floor((fg_imgareah-fg_imgnewsizeh)/2)+'px', 'left':(Math.floor((fg_imgareaw-fg_imgnewsizew)/2))+'px', 'width':fg_imgnewsizew+'px', 'height':fg_imgnewsizeh+'px'});
					}
					if(chkpushwidthtoarr){
						if(fg_imgscalerx &lt;= fg_imgscalery){
							portfolioslideswidths[portfolioslideswidths.length] = fg_imgnewsizew;
						}else{
							portfolioslideswidths[portfolioslideswidths.length] = fg_imgareaw;
						}
					}
				}else{
					iel.css({'top':Math.floor((fg_imgareah-fg_imgnewsizeh)/2)+'px', 'left':(Math.floor((fg_imgareaw-fg_imgnewsizew)/2))+'px', 'width':fg_imgnewsizew+'px', 'height':fg_imgnewsizeh+'px'});
					if(chkpushwidthtoarr){
						portfolioslideswidths[portfolioslideswidths.length] = fg_imgareaw;
					}
				}
			}else{
				jQuery(".video-js", iel).css({'top':Math.floor((fg_imgareah-fg_imgnewsizeh)/2)+'px', 'left':(Math.floor((fg_imgareaw-fg_imgnewsizew)/2))+'px', 'width':fg_imgnewsizew+'px', 'height':fg_imgnewsizeh+'px'});
				if(chkpushwidthtoarr){
					portfolioslideswidths[portfolioslideswidths.length] = fg_imgareaw;
				}
			}
		}
	}
}
function tresizewindowf(){
	jQuery(".portfolio-fs-viewport, .portfolio-fs-slide").css({'width':((portfoliofixedmenum==3)?(jQuery(window).width()-220):jQuery(window).width()), 'height':jQuery(window).height()});
	if(portfoliofixedmenum == 3){
		jQuery(".portfolio-fs-viewport").css({"left":"220px"});
	}
	if(jQuery(".portfolio-fs-slide").length){
		jQuery(".portfolio-fs-slides").css({'width': (jQuery(".portfolio-fs-slide").length*jQuery(window).width()), 'height':jQuery(window).height()});
	}
	
	resizeimageslide(jQuery("#myimage_original"),false,false);
	portfolioslideswidths = [];
	//jQuery(".myimage:not(.myvideo)").each(function(i,e){
	jQuery(".myimage").each(function(i,e){
		if(jQuery(e).hasClass("myvideo")){
			resizeimageslide(e,true,true);
		}else{
			resizeimageslide(e,false,true);
		}
	});
	//jQuery(".myimage.myvideo").each(function(i,e){
		//resizeimageslide(e,true,true);
	//});
	//jQuery(".myimage.myvideo:not(.current_sli)").css({'margin-left':(jQuery(window).width())+"px"});
	
	if(portfoliocurrentslideactive){
		jQuery(".portfolio-fs-slides").stop().css({'left':portfolioslidescountnleft(portfoliocurrentslideactive).left});
	}
	
	setSlideDescriptionScale();
}
	function videojsvolumemuteclick(){
		jQuery(".vjs-volume-control").click(function() {
			if(jQuery(".video-js").prop('muted')){
				jQuery(".video-js").prop('muted', false).prop('volume', 1);;
			}else{
				jQuery(".video-js").prop('muted', true).prop('volume', 1);;
			}
		});
	}
