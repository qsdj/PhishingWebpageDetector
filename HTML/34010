       <script type="text/javascript">
        function valid_search()
            {
                var val = true;
            
                if(document.getElementById("search_keyword").value == "" )
                {
                    //alert("Please enter eny keyword");
                    document.getElementById("search_keyword1").style.border = "1px solid red";
                    //document.getElementById("search_keyword").style.border = "1px solid red";
                    document.getElementById("search_keyword").focus();
                    val = false;
                }
                else
                {
                    
                    document.getElementById("search_keyword1").style.border = "1px solid #b9b2ae";
                    
                }
                
                return val;
                
            }
       </script>
