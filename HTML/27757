  <script type="text/javascript">
   function setTab03Syn ( i )
	{
		selectTab03Syn(i);
	}
	
	function selectTab03Syn ( i )
	{
		switch(i){
			case 1:
			document.getElementById("TabTab03Con1").style.display="block";
			document.getElementById("TabTab03Con2").style.display="none";
			document.getElementById("TabTab03Con3").style.display="none";
			document.getElementById("font1").style.color="#ffffff";
			document.getElementById("font2").style.color="#7b869c";
			document.getElementById("font3").style.color="#7b869c";
			document.getElementById("font1").style.height="33px";
			document.getElementById("font2").style.height="27px";
			document.getElementById("font3").style.height="27px";
			document.getElementById("font1").style.backgroundImage="url(/Public/img/01.png)";
			document.getElementById("font2").style.backgroundImage="url()";
			document.getElementById("font3").style.backgroundImage="url()";
			break;
			case 2:
			document.getElementById("TabTab03Con1").style.display="none";
			document.getElementById("TabTab03Con2").style.display="block";
			document.getElementById("TabTab03Con3").style.display="none";
			document.getElementById("font1").style.color="#7b869c";
			document.getElementById("font2").style.color="#ffffff";
			document.getElementById("font3").style.color="#7b869c";
			document.getElementById("font1").style.height="27px";
			document.getElementById("font2").style.height="33px";
			document.getElementById("font3").style.height="27px";
			document.getElementById("font2").style.backgroundImage="url(/Public/img/01.png)";
			document.getElementById("font1").style.backgroundImage="url()";
			document.getElementById("font3").style.backgroundImage="url()";
			break;
			case 3:
			document.getElementById("TabTab03Con1").style.display="none";
			document.getElementById("TabTab03Con2").style.display="none";
			document.getElementById("TabTab03Con3").style.display="block";
			document.getElementById("font1").style.color="#7b869c";
			document.getElementById("font2").style.color="#7b869c";
			document.getElementById("font3").style.color="#ffffff";
			document.getElementById("font1").style.height="27px";
			document.getElementById("font2").style.height="27px";
			document.getElementById("font3").style.height="33px";
			document.getElementById("font3").style.backgroundImage="url(/Public/img/01.png)";
			document.getElementById("font2").style.backgroundImage="url()";
			document.getElementById("font1").style.backgroundImage="url()";
			break;
		}
	}
  </script>
