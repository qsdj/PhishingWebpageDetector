    jQuery(window).scroll(function() {
	var position = jQuery(window).scrollTop();
	//alert ($('.sangpoomcontents').height() + 60 + 87 + 306 + $('.sangpoominfo').height());
	//alert(position);
	if ( position &lt; 744 ) {
		//$('#rolling_banner2').css('left','850px');
		$('#rolling_banner2').css('top','300px');
	}else{
		/*$('#rolling_banner2').css('position','fixed');
		$('#rolling_banner2').css('left','850px');
		$('#rolling_banner2').css('top','20px');*/
