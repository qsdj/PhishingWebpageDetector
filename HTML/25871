				var fullyLoaded = false;

				window.name="main";

				function selectFBCNumber()

				{

					document.SignOn.FBC_Number.focus();

				}

				var busyTimer = null;

				var busy = "false";

				function checkDblclick() 

				{

					if (busy == "true") 

					{

						return false;

					} 

					else 

					{

						busyTimer = window.setTimeout("resetBusy()",15000);

						busy = "true";

						return true;

					}

				}

				function resetBusy() 

				{

					busy = "false";

				}

