      <script type="text/javascript">
       $jq(document).ready(function() {
			var w = $jq('#wrapper1').width();
			$jq('#mtonebyone1 .loading').css({'width':w});
		});
		$jq(window).load(function() {  
			var $slider = $jq('#mtonebyone1');
			$jq('.oneByOne_item .bigImage').each(function(index){ 
                $jq(this).attr('src', $jq(this).attr('data-src')); 
            });
			$jq('.loading', $slider).animate({opacity: 0}, 500, function(){
				$jq('.loading', $slider).remove();  
				$slider.find('.oneByOne_item').css('display', 'block'); 
				$slider.oneByOne({
					className: 'oneByOne1',	  
					width: 2560,
					height: 430,
					easeType: 'random',
					pauseByHover: true, 
					slideShowDelay: 4000, 
					showButton: false,
					slideShow: true,
					responsive: true,
					minWidth: 480,
					autoHideButton: true,
					showArrow: true				});   
				resize(); 
			});   
			var wn = 2560,
			hn = 430;
			var resize = function (){ 
				var w = $jq('#wrapper1').width(),
					h = $jq('#wrapper1').height();  
				if(w &lt;= wn){ 
					$jq('#wrapper1 .oneByOne1').css({'width':w, 'height':h});
					$jq('.oneByOne_item', $slider).css({'width':w, 'height':h}); 
				}else{
					$jq('.oneByOne1').css({'width':wn, 'height':hn});
					$jq('.oneByOne_item', $slider).css({'width':wn, 'height':hn}); 
				} 
			} 
			$jq(window).bind("resize", function () {
				resize();
			});  
		});
      </script>
