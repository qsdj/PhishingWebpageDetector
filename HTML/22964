     jQuery(document).ready(function(){
					if(jQuery(document).scrollTop()&gt;35){
						jQuery('#navigation').addClass('pos_fixed_nav');
					}
				   jQuery(window).scroll(function(e){
					   if(jQuery(document).scrollTop()&gt;35){
							jQuery('#navigation').addClass('pos_fixed_nav');
					   }else{
						   jQuery('#navigation').removeClass('pos_fixed_nav');
