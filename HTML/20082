$("#location").change(function()
{
var id=$(this).val();
var dataString = id;
$.ajax
({
/* type: "POST",
url: "http://isfsecurities.com/wp-content/themes/hoxa/cities.php",
data: dataString,
cache: false,
success: function(html)
{
$("#listCity").html(html);
} */

 url:"http://isfsecurities.com/wp-content/themes/hoxa/cities.php",
type:"post",
data:{city:id},
success:function(response)
{

$("#place").html(response);
}

});

