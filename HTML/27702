		getCharBasedOnLangId=function(arrayObj){
			var chars = arrayObj[langID];
			var langArr = [];
			var character;
			
			if(chars == null){
				chars = arrayObj["001"];
			} 
			var charLen = chars.length;
			for(i=0; i &lt; charLen ; i++){
				character = chars.substring(i,i+1);
				if(character == "\""){
					langArr[i] = "&amp;quot";
				}else{
					langArr[i] = character;	
				}
			}		
			return langArr;
		};
		
		
