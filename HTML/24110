  <script type="text/javascript">
   // MENU STYLES
function showmenu(elmnt)
{
if(document.getElementById &amp;&amp; document.getElementById(elmnt))
{
	return document.getElementById(elmnt).style.visibility="visible"
}
else if (document.all &amp;&amp; document.all(elmnt))
{
	return document.all(elmnt).style.visibility="visible"
}
else
{
	return false;
}
}

function hidemenu(elmnt)
{
if(document.getElementById &amp;&amp; document.getElementById(elmnt))
{
	return document.getElementById(elmnt).style.visibility="hidden"
}
else if (document.all &amp;&amp; document.all(elmnt))
{
	return document.all(elmnt).style.visibility="hidden"
}
else
{
	return false;
}
}
  </script>
  <script type="text/javascript">
   &lt;!-- // SHOW LAYER ON CLICK // --&gt;
	function elem(ent){return document.getElementById(ent);}

	function toggle_layer(id)
	{
		if (elem(id).style.display == 'none')
		{
			elem(id).style.display = 'block';
		}
		else
		{
			elem(id).style.display = 'none';
		}
	}
  </script>
  <script type="text/javascript" language="JavaScript">
