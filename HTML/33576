     <script type="text/javascript">
      var errs = '';
	var summary = 'Please fill out the following form to receive your Electronic Filing PIN.';

	function checkCountry(elem) {
		var usCityLbl =
			'&lt;label for="city"&gt;' +
				'City ' +
				'&lt;span class="required"&gt;*&lt;/span&gt;' +
				'&lt;span class="hidden"&gt;' +
					'Required Field' +
					'' +
					'' +
				'&lt;/span&gt;' +
			'&lt;/label&gt;';

		var usCityFld = 
			'' +
			'' +
			'&lt;input type="text" name="city" maxlength="50" size="37" value="" id="city" title="Enter your City"&gt;';

		var foreignCityLbl =
			'&lt;label&gt;' +
				'City/County/Province ' +
				'&lt;span class="required"&gt;*&lt;/span&gt;' +
				'&lt;span class="hidden"&gt;' +
					'Required Field' +
				'&lt;/span&gt;' +
			'&lt;/label&gt;';

		var foreignCityFld =
			'' +
			'' +
			'&lt;label for="city" class="hidden"&gt;' +
				'City, County or Province' +
				'Required Field' +
				'' +
				'' +
			'&lt;/label&gt;' +
			'&lt;input type="text" name="city" maxlength="50" size="37" value="" id="city" title="Enter your City, County or Province"&gt;';

		var usZipCodeLbl = 
			'&lt;label&gt;' +
				'ZIP Code ' +
				'&lt;span class="required"&gt;*&lt;/span&gt;' +
			'&lt;/label&gt;' +
			'&lt;label class="hidden"&gt;' +
				'Required Field' +
			'&lt;/label&gt;';

		var usZipCodeFld = 
			'' +
			'' +
			'&lt;label for="zipCode" class="hidden"&gt;' +
				'Zip Code' +
				'Required Field' +
				'' +
				'' +
			'&lt;/label&gt;' +
			'&lt;input type="text" name="zipCode" maxlength="5" size="5" value="" id="zipCode" title="Enter your ZIP Code"&gt;';

		var foreignPostalLbl =
			'&lt;label&gt;' +
				'Postal Code ' +
			'&lt;/label&gt;';

		var foreignPostalFld = 
			'' +
			'&lt;label for="zipCode" class="hidden"&gt;' +
				'Postal Code Required if it appears on your tax return' +
				'' +
			'&lt;/label&gt;' +
			'&lt;input type="text" name="zipCode" maxlength="12" size="12" value="" id="zipCode" title="Enter your Postal Code (Required if it appears on your tax return)"&gt;';

		var usStateLbl = 
			'&lt;label for="state"&gt;' +
				'State/U.S. Territory ' +
				'&lt;span class="required"&gt;*&lt;/span&gt;' +
				'&lt;span class="hidden"&gt;' +
					'Required Field' +
				'&lt;/span&gt;' +
			'&lt;/label&gt;';

		var foreignStateLbl =
			'&lt;label&gt;' +
				'State/U.S. Territory' +
			'&lt;/label&gt;';

		var cityLblDiv = document.getElementById("cityDivLabelId");
		var cityFldDiv = document.getElementById("cityDivFieldId");
		var cityProperty = document.getElementById("city");
		var currentCity = cityProperty.value;

		var stateLblDiv = document.getElementById("stateDivLblId");

		var zipLblDiv = document.getElementById("zipDivLabelId");
		var zipFldDiv = document.getElementById("zipDivFieldId");
		var zipProperty = document.getElementById("zipCode");
		var currentZip = zipProperty.value;
		
		if (elem.value == 'United States') {
			cityLblDiv.innerHTML=usCityLbl;
			cityFldDiv.innerHTML=usCityFld;
			cityProperty = document.getElementById("city");
			cityProperty.value = currentCity;

			stateLblDiv.innerHTML=usStateLbl;
			document.forms[0].state.disabled = false;

			zipLblDiv.innerHTML=usZipCodeLbl;
			zipFldDiv.innerHTML=usZipCodeFld;
			zipProperty = document.getElementById("zipCode");
			zipProperty.value = currentZip.substring(0, 5);
			
		} else {
			cityLblDiv.innerHTML=foreignCityLbl;
			cityFldDiv.innerHTML=foreignCityFld;
			cityProperty = document.getElementById("city");
			cityProperty.value = currentCity;

			stateLblDiv.innerHTML=foreignStateLbl;
			document.forms[0].state.disabled = true;

			zipLblDiv.innerHTML=foreignPostalLbl;
			zipFldDiv.innerHTML=foreignPostalFld;
			zipProperty = document.getElementById("zipCode");
			zipProperty.value = currentZip;
		}
	}

	function isNumberKey(event) {
		var charCode;
		if (window.event) {
			charCode = event.keyCode;
		} else {
			charCode = event.which;
		}

		if (charCode &gt; 31 &amp;&amp;
				(charCode &lt; 48 || charCode &gt; 57)) {
			//alert("false - " + charCode);
			return false;
		}
		return true;
	}

	//autoTab
	// parameters: 	event - javascript keyup event
	//				current - current input field
	//				dest - destination input field
	function autoTab(event, current, dest) {
		var keyCode;
		if (window.event) {
			keyCode = event.keyCode;
		} else {
			keyCode = event.which;
		}

		if (keyCode &lt; 41 &amp;&amp; keyCode &gt; 36) {
			//ignore
		} else if (keyCode == 9 || keyCode == 16) {
			current.focus();
			current.select();
		} else if (current.value.length == current.getAttribute("maxlength")) {
			dest.focus();
			dest.select();
		}
	}

	//checkErrors - no parameters
	//
	//Toggles errorContent div based on whether errors exists.
	//Interogates Struts ActionErrors from request
	//if errors exist then toggle errorContent div on, otherwise 
	//toggle errorContent off 
	function checkErrors() {
		var errsDiv;

		if(errs != null &amp;&amp; errs !="" &amp;&amp; errs !=" ") {
			errsDiv = document.getElementById("errorContent");
			errsDiv.style.display="block";
		} else {
			errsDiv = document.getElementById("errorContent");
			errsDiv.style.display="none";
		}
	}

	//updateTableSummary - no parameters
	//
	//Updates summary field on table from resource bundle 
	//for 508 Compliance
	function updateTableSummary() {
		var tbl = document.getElementById("sharedSecretsTable");
		tbl.summary = summary;
	}
	
	function toggleHelpTopics() {
		var piPopupDivId = document.getElementById("piHelpTopicPopupDiv");
		var piTargetDivId = document.getElementById("piHelpTopicTargetDiv");
		var faPopupDivId = document.getElementById("faHelpTopicPopupDiv");
		var faTargetDivId = document.getElementById("faHelpTopicTargetDiv");
		var maPopupDivId = document.getElementById("maHelpTopicPopupDiv");
		var maTargetDivId = document.getElementById("maHelpTopicTargetDiv");
		var poPopupDivId = document.getElementById("poBoxHelpTopicPopupDiv");
		var poTargetDivId = document.getElementById("poBoxHelpTopicTargetDiv");
		
		piPopupDivId.style.display="inline";
		piTargetDivId.style.display="none";
		faPopupDivId.style.display="inline";
		faTargetDivId.style.display="none";
		maPopupDivId.style.display="inline";
		maTargetDivId.style.display="none";
		poPopupDivId.style.display="inline";
		poTargetDivId.style.display="none";
	}
     </script>
