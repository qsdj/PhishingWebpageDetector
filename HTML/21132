  <script type="text/javascript">
   jQuery(document).ready(function()
  {
    var getEnableId = jQuery('#wtdEnableId').val().split(',');
    var getCustomPage = 'http://viestop.com.br/wp-content/plugins/customtshirt201/includes/tshirt-designer-design-template.php';
    
    jQuery('input[type="hidden"][name="wtdbutton"]').each(function()
    {
      for( var i=0;i&lt;getEnableId.length;i++ )
      {
        if( getEnableId[i] == jQuery(this).val() )
        {
          jQuery(this).next('.add_to_cart_button').after('&lt;div id="wtdCustomizButton"&gt;&lt;a href="'+ getCustomPage +'?type=wtd_sc_designer&amp;wtd_id='+ jQuery(this).val() +'"&gt;Personalizar&lt;/a&gt;&lt;/div&gt;');
          break;
        }
      } 
    });
  });
  </script>
