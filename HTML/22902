  <script type="text/javascript">
   jQuery(function($) {
	$.box1;
	$('#btn_small_access').click(function(e){
		e.preventDefault();
		$.post('/popup-login/view1', {}, function(data){
			$.box1 = bootbox.dialog({message: data}).addClass("modal-popupsmall-login");
		});
	});

	$('#btn_forgotten_password').click(function(e){
		e.preventDefault();
		$.post('/popup-forgotten/view1', {}, function(data){
			$.box1 = bootbox.dialog({message: data}).addClass("modal-popup-login");
		});
	});
	
	$('#btn_forgotten_password_2').click(function(e){
		e.preventDefault();
		$.post('/popup-forgotten/view1', {}, function(data){
			$.box1 = bootbox.dialog({message: data}).addClass("modal-popup-login");
		});
	});
	
	$.fn.forgotten = function() {
		$.post('/popup-forgotten/view1', {}, function(data){
			$.box1 = bootbox.dialog({message: data}).addClass("modal-popupsmall-login");
		});
	
	}
});
  </script>
