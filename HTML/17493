  var enableOtherOverlays = function(){
  //console.log("in enable");
   var isAnotherOverlayEnabled=false;
  	if(document.getElementById('OverlayPanel') != null)
  	{
  		document.getElementById('OverlayPanel').style.opacity="1";
  		 if(document.getElementById('OverlayPanel').style!=null &amp;&amp; document.getElementById('OverlayPanel').style.display == "block"){
  		     if(document.getElementById('saveLink')!=null)
                  document.getElementById('saveLink').focus();
             if(document.getElementById('addressDeleteLink')!=null)
                  document.getElementById('addressDeleteLink').focus(); 
           isAnotherOverlayEnabled=true;     
         }   
  		
  	}
  	 if(document.getElementById('OverlayPanelEmail') != null)
    {
    
        document.getElementById('OverlayPanelEmail').style.opacity="1";
        if(document.getElementById('OverlayPanelEmail').style!=null &amp;&amp; document.getElementById('OverlayPanelEmail').style.display == "block"){
         document.getElementById('emailDeleteLink').focus();
        isAnotherOverlayEnabled=true;
        }
    }
  
  	if(document.getElementById('OverlayPanelEsa') != null)
  	{
  		document.getElementById('OverlayPanelEsa').style.opacity="1";
  		isAnotherOverlayEnabled=true;
  	}
  	if(document.getElementById('OverlayRecoveryVerify') != null)
  	{
  		document.getElementById('OverlayRecoveryVerify').style.opacity="1";
  		if(document.getElementById('OverlayRecoveryVerify').style!=null &amp;&amp; document.getElementById('OverlayRecoveryVerify').style.display == "block")
  		document.getElementById('verifyRecoveryKeyInput').focus();
  		isAnotherOverlayEnabled=true;
