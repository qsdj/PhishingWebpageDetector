		 /*  for standard and replacement*/
var prixInitial = 290;
var prixStandard = 0;
var prixRemplacementvacances = 0;
var prixSpeciale = 0;
var prixnbre_semaine= 50;
var prixBonatirer= 0;
var prixChiffre= 0;
var prixBase=prixStandard+prixRemplacementvacances;
var prixTotal = 0;
var valuenbre_semaine = 2;		 
	$('#edit-submitted-type-doffre-demploi-1, #edit-submitted-type-doffre-demploi-2').click(function () {
		 if ($('[name=submitted[type_doffre_demploi]]').attr('checked')) {
			  //$('#edit-submitted-offer-price').val('290');
			prixStandard = 0;
		prixRemplacementvacances = 0;
		prixTotal =0;
		prixTotal = prixTotal+prixStandard+prixRemplacementvacances+prixSpeciale+prixBonatirer+prixChiffre;
	}
	else{
		prixStandard = 0;
		prixRemplacementvacances = 190;
		prixTotal = 0;
		prixTotal = prixTotal+prixStandard+prixRemplacementvacances+prixSpeciale+
prixBonatirer+prixChiffre ;
	}
	$(".currentprice").text(prixTotal);
  	});	 
	$('#edit-submitted-offres-speciales-1').click(function () {
		 if ($('#edit-submitted-offres-speciales-1').attr('checked')) {
			prixSpeciale = prixnbre_semaine * parseInt(valuenbre_semaine);
		prixTotal = 0;
		prixTotal = prixTotal+prixStandard+prixRemplacementvacances+prixSpeciale+prixBonatirer+prixChiffre ;		

	}
	else{
		prixSpeciale = 0;
		prixTotal = 0;
		prixTotal = prixTotal+prixStandard+prixRemplacementvacances+prixSpeciale+prixBonatirer+prixChiffre ;
	}
		 $(".currentprice").text(prixTotal);
  	});	 
	
$('#edit-submitted-offer-week-list').change(function () {
	var valuenbre_semaine = document.getElementById("edit-submitted-offer-week-list").value;
	prixSpeciale = prixnbre_semaine * parseInt(valuenbre_semaine);	
				prixTotal = 0;
				prixTotal = prixTotal+prixStandard+prixRemplacementvacances+prixSpeciale+prixBonatirer+prixChiffre ;
				$(".currentprice").text(prixTotal);
				
		});
$('#edit-submitted-offres-speciales-2').click(function () {
	 if ($('#edit-submitted-offres-speciales-2').attr('checked')) {
		 prixBonatirer = 90;
		prixTotal = 0;
		prixTotal = prixTotal+prixStandard+prixRemplacementvacances+prixSpeciale+prixBonatirer+prixChiffre ;
	}
	else{
		prixBonatirer = 0;
		prixTotal = 0;
		prixTotal = prixTotal+prixStandard+prixRemplacementvacances+prixSpeciale+prixBonatirer+prixChiffre ;
	}
	$(".currentprice").text(prixTotal);
});
	
$('#edit-submitted-offres-speciales-3').click(function () {
		 if ($('#edit-submitted-offres-speciales-3').attr('checked')) {
			 prixChiffre = 190;
		prixTotal = 0;
		prixTotal = prixTotal + prixStandard+prixRemplacementvacances+prixSpeciale+prixBonatirer+prixChiffre ;
	}
	else{
		prixChiffre = 0;
		prixTotal = 0;
		prixTotal = prixTotal + prixStandard+prixRemplacementvacances+prixSpeciale+prixBonatirer+prixChiffre ;
	}
	$(".currentprice").text(prixTotal);
});
			 	
