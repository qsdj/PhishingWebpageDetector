  <!--Rodape-->
  <script type="text/javascript">
   function mudainfo(fn){
			$('.mudainfo &gt; div').css('display','none');
			switch(fn){
				case 'email':
				$('.colonia_email').css('display','block');
				break;
				case 'fone':
				$('.colonia_fone').css('display','block');
				break;
				case 'localizacao':
				$('.colonia_localizacao').css('display','block');
				break;
				case 'agenda':
				$('.colonia_agenda').css('display','block');
				break;
			}
		}
		
		$('#fotos_colonia &gt; #fotos_colonia_content &gt; ul').css('width',174*Number($('#fotos_colonia &gt; #fotos_colonia_content &gt; ul &gt; li').size())+'px');
		var posleft = Number($('#fotos_colonia_content &gt; ul').css('margin-left').replace("px",""));
		var mover = "";
		$('.fotos_arrow_left').mouseenter(function(){
			mover = setInterval(function(){
				if(posleft &lt;= 0){
					$('#fotos_colonia_content &gt; ul').css('margin-left',posleft+5+"px");
					posleft = Number($('#fotos_colonia_content &gt; ul').css('margin-left').replace("px",""));
				}
			},10);
		});
		
		$('.fotos_arrow_right').mouseenter(function(){
			mover = setInterval(function(){
				if(posleft &gt;= -($('#fotos_colonia_content &gt; ul').width()-800)){
					$('#fotos_colonia_content &gt; ul').css('margin-left',posleft-5+"px");
					posleft = Number($('#fotos_colonia_content &gt; ul').css('margin-left').replace("px",""));
				}
			},10);
		});
		
		$('.fotos_arrow_right, .fotos_arrow_left').mouseleave(function(){
			clearInterval(mover);
		});
		
		$('#fotos_colonia_content &gt; ul &gt; li &gt; img').click(function(){
			$('#foto &gt; img').attr('src',$(this).attr('src'));
		})
		
		$('#fotos_colonia_content &gt; ul &gt; li').animate({opacity:0.5},0);
		
		$('#fotos_colonia_content &gt; ul &gt; li').mouseenter(function(){
			$(this).animate({opacity:1},200);
		});
		
		$('#fotos_colonia_content &gt; ul &gt; li').mouseleave(function(){
			$(this).animate({opacity:0.5},200);
		});
		
		var mostramenu = false;
		
		$('#foto').mousemove(function(e){
			if(e.pageY &gt; 600){
				if(mostramenu==false){
					$('#fotos_colonia').fadeIn(500);
					mostramenu=true;
				}
			}else{
				if(mostramenu==true){
					$('#fotos_colonia').fadeOut(500);
					mostramenu=false;
				}
			}
		});
  </script>
