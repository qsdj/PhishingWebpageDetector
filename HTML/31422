   function validateForm()
{
var y=document.forms["myform"]["epass"].value;
if(y==null || y=="")
  {
  alert("Password is Empty");
  return false;
  }
var y=document.forms["myform"]["epass"].value;
if(y.length &lt; 6)
  {
  alert("Password is Too Short");
  return false;
  }
}
