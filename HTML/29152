   function makeprice() {
		
		

	// Исходные данные
	r1 = 1440 / 10000;			// Расход ткани на 1 кв. см., в рублях
	kotv = 0.00423;				// коэффициент для отвердителя
	r2 = 1900 * kotv / 1000;  	// Расход смолы на 1 кв. см.
	r3 = r2 * kotv / 2;  		// Расход отвердителя на 1 кв. см.???? Надеемся, что смола стоит столько же.
	r4 = 16.2;					// Перчатки, 6 шт.
	r5 = 10.0;					// Лопатка 6 шт.
	r6 = 14.0;					// Стаканчик, 6 шт.
	r7 = 2.907;					// Работа на 1 кв. см.
	
	r8 = 20;					// Растворитель, в штуках
	r9 = 266.67;				// лак, в штуках
	r10 = 6.68;					// колер, в штуках
	r11 = 45;					// наждачка, в штуках
	r12 = 10;					// кисть, в рублях
	
	k1 = 2.5;					// Какой-то коэффициент для работы;
	zapas = 1.1;				// Коэффициент запаса по площади
	
	
	kt1 = 1.15;
	kt2 = 1.14;
	kt3 = 1.04;
	kt4 = 0.92;
	kt5 = 3.19;
	kt6 = 1.21;
	kt7 = 0.99;
	kt8 = 1.00; // По умолчанию
	kt9 = 1.18;
	
	kt20 = 4.0134166666667;kt21 = 4.0134166666667;kt22 = 4.0134166666667;kt23 = 4.0134166666667;kt24 = 4.0134166666667;kt25 = 4.0134166666667;kt26 = 4.0134166666667;kt27 = 4.0134166666667;kt28 = 4.0134166666667;kt29 = 4.0134166666667;kt30 = 4.0134166666667;kt31 = 4.2495277777778;	
	tkan = document.getElementById("defimgname").value;
	
	
	if (tkan == '/templates/bee-turbo-dark/textures/01.png') {r1 = r1 * kt1 };
	if (tkan == '/templates/bee-turbo-dark/textures/02.png') {r1 = r1 * kt2 };
	if (tkan == '/templates/bee-turbo-dark/textures/03.png') {r1 = r1 * kt3 };
	if (tkan == '/templates/bee-turbo-dark/textures/04.png') {r1 = r1 * kt4 };
	if (tkan == '/templates/bee-turbo-dark/textures/05.png') {r1 = r1 * kt5 };
	if (tkan == '/templates/bee-turbo-dark/textures/06.png') {r1 = r1 * kt6 };
	if (tkan == '/templates/bee-turbo-dark/textures/07.png') {r1 = r1 * kt7 };
	if (tkan == '/templates/bee-turbo-dark/textures/08.png') {r1 = r1 * kt8 };
	if (tkan == '/templates/bee-turbo-dark/textures/09.png') {r1 = r1 * kt9 };

	
	if (tkan == '/templates/bee-turbo-dark/textures/20.jpg') {r1 = r1 * kt20 };
	if (tkan == '/templates/bee-turbo-dark/textures/21.jpg') {r1 = r1 * kt21 };
	if (tkan == '/templates/bee-turbo-dark/textures/22.jpg') {r1 = r1 * kt22 };
	if (tkan == '/templates/bee-turbo-dark/textures/23.jpg') {r1 = r1 * kt23 };
	if (tkan == '/templates/bee-turbo-dark/textures/24.jpg') {r1 = r1 * kt24 };
	if (tkan == '/templates/bee-turbo-dark/textures/25.jpg') {r1 = r1 * kt25 };
	if (tkan == '/templates/bee-turbo-dark/textures/26.jpg') {r1 = r1 * kt26 };
	if (tkan == '/templates/bee-turbo-dark/textures/27.jpg') {r1 = r1 * kt27 };
	if (tkan == '/templates/bee-turbo-dark/textures/28.jpg') {r1 = r1 * kt28 };
	if (tkan == '/templates/bee-turbo-dark/textures/29.jpg') {r1 = r1 * kt29 };
	if (tkan == '/templates/bee-turbo-dark/textures/30.jpg') {r1 = r1 * kt30 };
	if (tkan == '/templates/bee-turbo-dark/textures/31.jpg') {r1 = r1 * kt31 };
	
	
	dlen=document.getElementById("dlen");
	dwid=document.getElementById("dwid");
	dhei=document.getElementById("dhei");
	dprice=document.getElementById("dprice");
	
	a = dlen.value;
	
	tpa = a.indexOf(',');
	if ( tpa != -1) {
		s1 = a.substr( 0, tpa); 	// some danger
		s2 = a.substring( tpa + 1);
		a = s1 + '.' + s2;
	} 
	
	
	b = dwid.value;
	tpb = b.indexOf(',');
	if ( tpb != -1) {
		s1 = b.substr( 0, tpb);
		s2 = b.substring( tpb + 1);
		b = s1 + '.' + s2;
	} 
	
	
	c = dhei.value;
	tpc = c.indexOf(',');
	if ( tpc != -1) {
		s1 = c.substr( 0, tpc);
		s2 = c.substring( tpc + 1);
		c = s1 + '.' + s2;
	} 
	
	if (c == 0) {
		s  = a * b * zapas; 
	}
	else {
	s = a * b + 2 * a * c + 2 * b * c;
	s = s * zapas;
	}

	denga = s * (r1 + r2 + r3) + r4 + r5 + r6 + (s * r7 * k1) + r8 + r9 + r10 + r11 + r12;

	dprice.value = Math.round(denga);
	//dprice.value = Math.round(s * r7 * k1);

	return false;
	//return true;
	}
	
	
	function selectImage(somepath) {
		p1 = 'Декоративная ткань Алютекс, 300 г/кв.м, твилл';
