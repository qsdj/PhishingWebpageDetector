				//jQuery(".slides .myvideo").stop().css({ height: '100%' }).animate({ marginTop: ~jQuery(window).height() }, 500);
				//jQuery("#content").stop().css({ "position" : 'fixed' }).animate({ top: ~jQuery(window).height() }, 500);
				jQuery(".video-js").stop().delay(500).remove();
