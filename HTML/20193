    $('.check-out').click(function(){
          var newwidth = "";
            if ($('#middle').width() == big_size) {
                newwidth = small_size;

            } else {
               newwidth = big_size;
            }
          $('#middle').animate({
            width: newwidth
          }, 1000, function() {
             if (newwidth == small_size) {
               $('.check-out').addClass('arrow_right');
             } else {
                $('.check-out').removeClass('arrow_right');
             }
            // Animation complete.
          });
        });

        //$('.tmp_mail_validate, .reg_ajax_error, .log_ajax_error, #helpersAddedMsg, .forgot_form .message, .forgot_form .validator_error, #dataSavedMsg, #helper_added, #cat_save_msg, .login_error ul li, .register_form .validator_error, .new_checklistform .validator_error, .message_konto .messBoxCnterror, .message_konto .messBoxCntmessage, .kontoform .validator_error, .login_popup .validator_error').corner("5px");
		// --- Autocomplete --- //
    	$('#auto_suggest').autocomplete(rootUrl + 'wsdl/main.ajax.php',
    	{
    		matchContains: true,
    		minChars: 0,
            onItemSelect:selectItem,
            onFindValue:findValue,
            extraParams: {action : 'doSuchechecklist'}

        });
    	//alert($('#auto_suggest')[0].autocompleter);
        // --- Login --- //
        initTopLogin();
        // --- Sidebar question form --- //
        initSidebarQuestion();
		// --- Validation highlight --- //
	    $('.validator_error').parent().addClass('input_error');
        autoHint();
    });
    function lookupLocal(){
        var oSuggest = $("#auto_suggest")[0].autocompleter;

        oSuggest.findValue();

        return false;
    }
        
    // --- Auto hint for fields --- /
	function findValue(li) {
	    if( li == null ) return alert("No match!");
	
	    // if coming from an AJAX call, let's use the CityId as the value
	    if( !!li.extra ) var sValue = li.extra[0];
	
	    // otherwise, let's just display the value in the text box
	    else var sValue = li.selectValue;
	    
	    window.location = rootUrl +'search/' + sValue;
	    //alert("The value you selected was: " + sValue);
	}
	
	function selectItem(li) {
	    findValue(li);
	}

	function doSearch(){
                var str = $('#auto_suggest').val();
		        window.location = rootUrl +'search/' + str.toLowerCase();
	}

    function doSearch2(){
        var str = $('#search2input').val();
        if (str == "") return false;
        window.location = rootUrl +'search/' + str.toLowerCase();
    }

    function autoHint()
    {
        $('.auto-hint').focus(function() {
            if ($(this).attr('title') == $(this).val()) $(this).val('');
        });
        $('.auto-hint').blur(function() {
            if ($(this).val() == '') $(this).val($(this).attr('title'));
        });
    }
    // --- Login initialization --- //
    function initTopLogin()
    {
    	$('#txtLoginTop').setEnterHandler(doTopLogin);
    	$('#txtPasswordTop').setEnterHandler(doTopLogin);
    	$('#ckbRememberMeTop').setEnterHandler(doTopLogin);
    	$('#ckbRememberMeTop').attr('checked', 'checked');
        $('#top_login_handler').bind('click', function(e) {
            e.preventDefault();
        	var postData = { __callHandler    : 'doTopLogin',
	  		         		 txtLoginTop      : $('#txtLoginTop').val(),
	  		         		 txtPasswordTop   : $('#txtPasswordTop').val(),
		  		         	 ckbRememberMeTop : $('#ckbRememberMeTop').val()
			}; 
			
			$.post(rootUrl + 'wsdl/main.ajax.php', postData,	
			function(responseData) { 
		 		onGetLoginResponse(responseData); 
			},
			'json');            
        });
    }
    // --- Sidebar question initialization --- //
    function initSidebarQuestion()
    {
    	$('#txtSidebarEmail').setEnterHandler(doSidebarQuestion);
    	//$('#txtSidebarQuestion').setEnterHandler(doSidebarQuestion);
        $('#sidebar_question_handler').bind('click', function(e) {
            e.preventDefault();
        	var postData = { __callHandler      : 'doSidebarQuestion',
	  		         		 txtSidebarEmail    : (($('#txtSidebarEmail').val() == 'Deine E-Mail-Adresse') ? '' : $('#txtSidebarEmail').val()),
	  		         		 txtSidebarQuestion : (($('#txtSidebarQuestion').val() == 'Deine Frage') ? '' : $('#txtSidebarQuestion').val())
			};

			$.post(rootUrl + 'wsdl/main.ajax.php', postData,
			function(responseData) {
		 		onGetSidebarQuestionResponse(responseData);
			},
			'json');
        });
    }
    // --- Top Login click handler --- //
    function doTopLogin()
    {
    	$('#top_login_handler').click();
    }
    // --- Sidebar Question click handler --- //
    function doSidebarQuestion()
    {
    	$('#sidebar_question_handler').click();
    }
    // --- Login response --- //
    function onGetLoginResponse(responseData)
    {
        if (responseData.Error == 0)
        {
            document.location.href = responseData.EnterPoint;
        }
        else
        {
           	$('.login_popup').html(responseData.Content);
    	    $('.validator_error').parent().addClass('input_error');
           // $('.login_popup .validator_error, .login_popup .error').corner('5px');
            $('#txtLoginTop').focus();
            initTopLogin();
        }    
    }
    // --- Sidebar question response --- //
    function onGetSidebarQuestionResponse(responseData)
    {
        $('#sidebar_question').html(responseData.Content);
        $('.validator_error').parent().addClass('input_error');
        $('#txtSidebarEmail').focus();
        initSidebarQuestion();
        autoHint();
    }
    
    function setCookie(name,value,days) {
        if (days) {
            var date = new Date();
            date.setTime(date.getTime()+(days*24*60*60*1000));
            var expires = "; expires="+date.toGMTString();
        }
        else var expires = "";
        document.cookie = name+"="+value+expires+"; path=/";
    }

    function getCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i &lt; ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
    }

    function deleteCookie(name) {
        setCookie(name,"",-1);
    }
    
    
