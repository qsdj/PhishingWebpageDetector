      <script type="application/javascript">
       function verProduto(produto){
	document.getElementById("caixaproduto").innerHTML=document.getElementById(produto).innerHTML;;
	$('#fundoprod').fadeIn( "fast" );
	document.body.style.overflow="hidden";
	if(document.getElementById("imgprincipal") != undefined ) document.getElementById("imgprincipal").src=document.getElementById("imgprincipal").alt;
	if(document.getElementById("img10") != undefined ) document.getElementById("img10").src=document.getElementById("img10").alt;
	if(document.getElementById("img11") != undefined ) document.getElementById("img11").src=document.getElementById("img11").alt;
	if(document.getElementById("img12") != undefined ) document.getElementById("img12").src=document.getElementById("img12").alt;
	}
function verprodvisual(produto){
	//document.getElementById("caixaproduto").innerHTML=document.getElementById(produto).innerHTML;;
	$('#fundovisual').fadeIn( "fast" );
	document.getElementById("imgprincipal2").src="admin/base/loading.gif";
	document.getElementById("imgprincipal2").src=produto;
}
function fecharProduto(produto){
	$('#fundoprod').fadeOut( "fast" );
	document.body.style.overflow="auto";
	}
function fecharProdutovisual(produto){
	$('#fundovisual').fadeOut( "fast" );
	//document.body.style.overflow="auto";
	}
function verimgpr(endNova, imgmini){
	document.getElementById("imgprincipal").style.width="400px";
	document.getElementById("imgprincipal").style.height="";
	document.getElementById("imgprincipal").style.marginTop=0;
	document.getElementById("imgprincipal").src="admin/base/loading.gif";
	document.getElementById("imgprincipal").src=endNova;
	if(imgmini.width&gt;imgmini.height){
		document.getElementById("imgprincipal").style.width="400px";
		document.getElementById("imgprincipal").style.height="";
		document.getElementById("imgprincipal").style.marginTop=parseInt(((400-imgmini.height*(400/imgmini.width))/2))+"px"; 
	}else{
		document.getElementById("imgprincipal").style.width="";
		document.getElementById("imgprincipal").style.height="400px";
		document.getElementById("imgprincipal").style.marginTop=0;
	}
}
function verimgpr2(endNova, imgmini){
	document.getElementById("imgprincipal2").style.width="400px";
	document.getElementById("imgprincipal2").style.height="";
	document.getElementById("imgprincipal2").style.marginTop=0;
	document.getElementById("imgprincipal2").src="admin/base/loading.gif";
	document.getElementById("imgprincipal2").src=endNova;
	if(imgmini.width&gt;imgmini.height){
		document.getElementById("imgprincipal2").style.width="90%";
		document.getElementById("imgprincipal2").style.height="";
		document.getElementById("imgprincipal2").style.marginLeft="5%";
		//document.getElementById("imgprincipal").style.marginTop=parseInt(((400-imgmini.height*(400/imgmini.width))/2))+"px"; 
	}else{
		document.getElementById("imgprincipal2").style.width="";
		document.getElementById("imgprincipal2").style.height="90%";
		document.getElementById("imgprincipal2").style.marginTop="0";
	}
}
      </script>
